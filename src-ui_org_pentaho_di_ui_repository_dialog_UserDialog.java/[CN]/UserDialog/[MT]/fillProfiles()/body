{
  try {
    wProfile.removeAll();
    String prof[]=rep.getProfiles();
    for (int i=0; i < prof.length; i++) {
      wProfile.add(prof[i]);
    }
  }
 catch (  KettleException e) {
    MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_ERROR);
    mb.setMessage(BaseMessages.getString(PKG,"UserDialog.Dialog.ErrorRetrievingProfiles.Message") + Const.CR + e.getMessage());
    mb.setText(BaseMessages.getString(PKG,"UserDialog.Dialog.ErrorRetrievingProfiles.Title"));
    mb.open();
  }
}

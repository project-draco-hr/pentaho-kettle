{
  if (v != null) {
    setType(v.getType());
    value=v.getValueCopy();
    if (v.getName() != null)     setName(new String(v.getName()));
 else     name=null;
    setLength(v.getLength(),v.getPrecision());
    setNull(v.isNull());
    if (v.origin != null)     setOrigin(new String(v.origin));
 else     origin=null;
  }
 else {
    clearValue();
    setNull(true);
  }
}

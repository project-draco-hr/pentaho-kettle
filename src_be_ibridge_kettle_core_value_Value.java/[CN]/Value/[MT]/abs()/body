{
  if (isNull())   return this;
  if (isBigNumber()) {
    setValue(getBigNumber().abs());
  }
 else   if (isNumber()) {
    setValue(Math.abs(getNumber()));
  }
 else   if (isInteger()) {
    setValue(Math.abs(getInteger()));
  }
 else {
    throw new KettleValueException("Function ABS only works with a number");
  }
  return this;
}

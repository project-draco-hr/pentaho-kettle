{
  if (v == null)   return false;
  if (getType() != v.getType())   return false;
  if (!checkTypeOnly) {
    if ((getName() == null && v.getName() != null) || (getName() != null && v.getName() == null) || !(getName().equals(v.getName())))     return false;
    if (getLength() != v.getLength())     return false;
    if (getPrecision() != v.getPrecision())     return false;
  }
  return true;
}

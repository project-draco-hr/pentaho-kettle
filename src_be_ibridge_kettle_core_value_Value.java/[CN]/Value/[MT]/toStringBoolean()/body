{
  String retval;
  if (value == null)   return null;
  if (isNull()) {
    retval=Const.NULL_BOOLEAN;
  }
 else {
    if (getLength() == 1) {
      retval=value.getBoolean() ? "Y" : "N";
    }
 else {
      retval=value.getBoolean() ? "true" : "false";
    }
  }
  if (getLength() >= 0) {
    StringBuffer ret=new StringBuffer(retval);
    Const.rightPad(ret,getLength());
    retval=ret.toString();
  }
  return retval;
}

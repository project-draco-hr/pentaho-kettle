{
  if (isNull()) {
    setType(VALUE_TYPE_DATE);
  }
 else {
    if (isNumeric()) {
      value=new ValueDate();
      value.setInteger(getInteger());
      setType(VALUE_TYPE_DATE);
      setLength(-1,-1);
    }
 else {
      throw new KettleValueException("Function NUM2DAT only works on a number");
    }
  }
  return this;
}

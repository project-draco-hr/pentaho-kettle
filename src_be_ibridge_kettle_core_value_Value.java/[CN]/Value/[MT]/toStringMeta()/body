{
  String retval=getTypeDesc();
switch (getType()) {
case VALUE_TYPE_STRING:
    if (getLength() > 0)     retval+="(" + getLength() + ")";
  break;
case VALUE_TYPE_NUMBER:
case VALUE_TYPE_BIGNUMBER:
if (getLength() > 0) {
  retval+="(" + getLength();
  if (getPrecision() > 0) {
    retval+=", " + getPrecision();
  }
  retval+=")";
}
break;
case VALUE_TYPE_INTEGER:
if (getLength() > 0) {
retval+="(" + getLength() + ")";
}
break;
default :
break;
}
return retval;
}

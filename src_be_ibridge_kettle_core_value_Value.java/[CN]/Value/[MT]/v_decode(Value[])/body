{
  int i;
  boolean found;
  if (args.length >= 3 && (args.length % 2) == 1) {
    i=0;
    found=false;
    while (i < args.length - 1 && !found) {
      if (this.equals(args[i])) {
        setValue(args[i + 1]);
        found=true;
      }
      i+=2;
    }
    if (!found)     setValue(args[args.length - 1]);
  }
 else {
    throw new KettleValueException("Function DECODE can't have " + args.length + " arguments!");
  }
  return this;
}

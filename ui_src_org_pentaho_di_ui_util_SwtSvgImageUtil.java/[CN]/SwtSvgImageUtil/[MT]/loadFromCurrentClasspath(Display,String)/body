{
  ClassLoader cl=Thread.currentThread().getContextClassLoader();
  URL res=cl.getResource(location);
  if (res == null) {
    return null;
  }
  InputStream s;
  try {
    s=res.openStream();
  }
 catch (  IOException ex) {
    return null;
  }
  if (s == null) {
    return null;
  }
  try {
    return loadImage(display,s,location);
  }
  finally {
    IOUtils.closeQuietly(s);
  }
}

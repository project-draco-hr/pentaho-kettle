{
  addMappingDefinitionTab(definition,index + 1,BaseMessages.getString(PKG,"MappingDialog.InputTab.Title"),BaseMessages.getString(PKG,"MappingDialog.InputTab.Tooltip"),BaseMessages.getString(PKG,"MappingDialog.InputTab.label.InputSourceStepName"),BaseMessages.getString(PKG,"MappingDialog.InputTab.label.OutputTargetStepName"),BaseMessages.getString(PKG,"MappingDialog.InputTab.label.Description"),BaseMessages.getString(PKG,"MappingDialog.InputTab.column.SourceField"),BaseMessages.getString(PKG,"MappingDialog.InputTab.column.TargetField"),true);

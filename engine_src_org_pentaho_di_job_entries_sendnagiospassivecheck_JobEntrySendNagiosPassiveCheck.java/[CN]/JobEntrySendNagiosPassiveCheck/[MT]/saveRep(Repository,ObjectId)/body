{
  try {
    rep.saveJobEntryAttribute(id_job,getObjectId(),"port",port);
    rep.saveJobEntryAttribute(id_job,getObjectId(),"servername",serverName);
    rep.saveJobEntryAttribute(id_job,getObjectId(),"password",password);

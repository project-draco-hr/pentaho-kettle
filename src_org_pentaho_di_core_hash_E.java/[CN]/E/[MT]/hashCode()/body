{
  boolean up=true;
  int idx=0;
  int hashCode=0;
  for (int i=0; i < key.length; i++) {
    hashCode^=Math.round((0xFF << idx) * key[i]);
    if (up) {
      idx++;
      if (idx == 8) {
        idx=6;
        up=false;
      }
    }
 else {
      idx--;
      if (idx < 0) {
        idx=1;
        up=true;
      }
    }
  }
  return hashCode;
}

{
  try {
    DataOutputStream out=new DataOutputStream(new BufferedOutputStream(new FileOutputStream(file)));
    int c;
    while ((c=is.read()) != -1) {
      out.writeByte(c);
    }
    is.close();
    out.close();
  }
 catch (  IOException e) {
    throw new KettleException(BaseMessages.getString(PKG,"JobFTPS.Error.WritingToFile",file.getName()),e);
  }
}

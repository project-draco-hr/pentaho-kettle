{
  try {
    filename=XMLHandler.getTagValue(stepnode,"file","name");
    addtoresultfilenames="Y".equalsIgnoreCase(XMLHandler.getTagValue(stepnode,"file","add_to_result_filenames"));
  }
 catch (  Exception e) {

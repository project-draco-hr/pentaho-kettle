{
  try {
    rep.saveStepAttribute(id_transformation,id_step,"head_name",getHeadStepName());
    rep.saveStepAttribute(id_transformation,id_step,"tail_name",getTailStepName());
  }
 catch (  Exception e) {
    throw new KettleException(Messages.getString("MergeRowsMeta.Exception.UnableToSaveStepInfo") + id_step,e);
  }
}

{
switch (type) {
case TYPE_INTEGER:
    setConversionMask("#;-#");
  break;
case TYPE_NUMBER:
setConversionMask("#.#;-#.#");
break;
case TYPE_BIGNUMBER:
setConversionMask("#.###############################################;-#.###############################################");
setGroupingSymbol(null);
setDecimalSymbol(".");
break;
default :
break;
}
}

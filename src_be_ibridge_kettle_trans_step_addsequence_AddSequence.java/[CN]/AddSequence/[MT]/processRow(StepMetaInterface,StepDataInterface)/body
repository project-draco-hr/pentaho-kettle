{
  meta=(AddSequenceMeta)smi;
  data=(AddSequenceData)sdi;
  Row r=null;
  r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  log.logRowlevel(toString(),Messages.getString("AddSequence.Log.ReadRow") + linesRead + " : "+ r);
  try {
    addSequence(r);
    putRow(r);
    log.logRowlevel(toString(),Messages.getString("AddSequence.Log.WriteRow") + linesWritten + " : "+ r);
    if ((linesRead > 0) && (linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)     logBasic(Messages.getString("AddSequence.Log.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    logError(Messages.getString("AddSequence.Log.ErrorInStep") + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

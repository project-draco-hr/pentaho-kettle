{
  String d[]=JobEntryInterface.type_desc;
  for (int i=1; i < d.length; i++) {
    if (d[i].equalsIgnoreCase(desc))     return i;
  }
  d=JobEntryInterface.type_desc_long;
  for (int i=1; i < d.length; i++) {
    if (d[i].equalsIgnoreCase(desc))     return i;
  }
  return JobEntryInterface.TYPE_JOBENTRY_NONE;
}

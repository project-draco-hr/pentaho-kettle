{
  String[] result=Const.splitString("Hello, world",null,null);
  assertNotNull(result);
  assertEquals(1,result.length);
  assertEquals("Hello, world",result[0]);
  result=Const.splitString("Hello, world",null,null,true);
  assertNotNull(result);
  assertEquals(1,result.length);
  assertEquals("Hello, world",result[0]);
}

{
  boolean retval=false;
  try {
    String sLine=meta.getEndedLine();
    if (sLine != null) {
      if (sLine.trim().length() > 0)       data.writer.write(sLine.toCharArray());
    }
  }
 catch (  Exception e) {
    logError("Error writing ended tag line: " + e.toString());
    e.printStackTrace();
    retval=true;
  }
  linesOutput++;
  return retval;
}

{
  boolean retval=false;
  try {
    data.writer.close();
    if (meta.getFileCompression() == "Zip") {
      data.zip.closeEntry();
      data.zip.finish();
      data.zip.close();
    }
    if (meta.getFileCompression() == "GZip") {
      data.gzip.finish();
    }
    retval=true;
  }
 catch (  Exception e) {
  }
  return retval;
}

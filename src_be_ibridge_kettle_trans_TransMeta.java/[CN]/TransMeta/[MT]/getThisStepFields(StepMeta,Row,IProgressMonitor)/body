{
  log.logDebug(toString(),Messages.getString("TransMeta.Log.GettingFieldsFromStep") + stepMeta.getName() + ", type="+ stepMeta.getStepID());
  String name=stepMeta.getName();
  if (monitor != null) {
    monitor.subTask(Messages.getString("TransMeta.Log.GettingFieldsFromStep2") + name + "]");
  }
  StepMetaInterface stepint=stepMeta.getStepMetaInterface();
  Row inform=null;
  StepMeta[] lu=getInfoStep(stepMeta);
  if (lu != null) {
    inform=getStepFields(lu);
  }
 else {
    inform=stepint.getTableFields();
  }
  stepint.getFields(row,name,inform);
  return row;
}

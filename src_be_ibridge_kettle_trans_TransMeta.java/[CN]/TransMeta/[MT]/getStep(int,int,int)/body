{
  int i, s;
  s=steps.size();
  for (i=s - 1; i >= 0; i--) {
    StepMeta stepMeta=(StepMeta)steps.get(i);
    if (partOfTransHop(stepMeta) || stepMeta.isDrawn()) {
      Point p=stepMeta.getLocation();
      if (p != null) {
        if (x >= p.x && x <= p.x + iconsize && y >= p.y && y <= p.y + iconsize + 20) {
          return stepMeta;
        }
      }
    }
  }
  return null;
}

{
  if (getAccessType() == DatabaseMeta.TYPE_ACCESS_ODBC) {
    return "jdbc:odbc:" + databaseName;
  }
 else {
    if (Const.isEmpty(port)) {
      return "jdbc:mysql://" + hostname + "/"+ databaseName;
    }
 else {
      return "jdbc:mysql://" + hostname + ":"+ port+ "/"+ databaseName;
    }
  }
}

{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMeta("int_field",ValueMeta.TYPE_INTEGER,8,0),new ValueMeta("str_field",ValueMeta.TYPE_STRING,30,0),new ValueMeta("RET_CODE",ValueMeta.TYPE_INTEGER,8,0),new ValueMeta("RET_STRING",ValueMeta.TYPE_STRING,30,0)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMetaInteger("int_field",8,0),new ValueMetaString("str_field",30,0),new ValueMetaInteger("RET_CODE",8,0),new ValueMetaString("RET_STRING",30,0)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

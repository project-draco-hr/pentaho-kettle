{
  int i, nrfields;
  nrfields=0;
  allocate(nrfields);
  for (i=0; i < nrfields; i++) {
    fieldName[i]=BaseMessages.getString(PKG,"AggregateRowsMeta.Fieldname.Label");
    fieldNewName[i]=BaseMessages.getString(PKG,"AggregateRowsMeta.NewName.Label");
    aggregateType[i]=TYPE_AGGREGATE_SUM;
  }
}

{
  if (input.length > 0) {
    CheckResult cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("FilesFromResultMeta.CheckResult.StepExpectingNoReadingInfoFromOtherSteps"),stepMeta);
    remarks.add(cr);
  }
 else {
    CheckResult cr=new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("FilesFromResultMeta.CheckResult.NoInputReceivedError"),stepMeta);
    remarks.add(cr);
  }
}

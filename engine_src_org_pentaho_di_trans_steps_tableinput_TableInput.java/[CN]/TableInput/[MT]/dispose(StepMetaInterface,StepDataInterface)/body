{
  if (log.isBasic())   logBasic("Finished reading query, closing connection.");
  try {
    closePreviousQuery();
  }
 catch (  KettleException e) {
    logError("Unexpected error closing query : " + e.toString());
    setErrors(1);
    stopAll();
  }
 finally {
    if (data.db != null) {
      data.db.disconnect();
    }
  }
  super.dispose(smi,sdi);
}

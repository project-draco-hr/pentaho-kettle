{
  if (getAccessType() == DatabaseMeta.TYPE_ACCESS_NATIVE) {
    String url="jdbc:teradata://" + hostname;
    if (!StringUtils.isEmpty(port)) {
      url+=":" + port;
    }
    if (!StringUtils.isEmpty(databaseName)) {
      url+="/DATABASE=" + databaseName;
    }
    return url;
  }
 else {
    return "jdbc:odbc:" + databaseName;
  }
}

{
  Object res[]=messageDialogWithToggle("Warning",null,message,Const.WARNING,new String[]{BaseMessages.getString(PKG,"System.Button.Yes"),BaseMessages.getString(PKG,"System.Button.No")},1,rememberText,!"Y".equalsIgnoreCase(props.getProperty(rememberPropertyName)));
  int idx=((Integer)res[0]).intValue();
  boolean overwrite=((idx & 0xFF) == 0);
  boolean toggleState=((Boolean)res[1]).booleanValue();
  props.setProperty(rememberPropertyName,(!toggleState) ? "Y" : "N");
  return overwrite;
}

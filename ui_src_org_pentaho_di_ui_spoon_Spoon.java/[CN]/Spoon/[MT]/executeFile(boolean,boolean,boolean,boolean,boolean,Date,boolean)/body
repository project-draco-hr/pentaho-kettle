{
  TransMeta transMeta=getActiveTransformation();
  if (transMeta != null)   executeTransformation(transMeta,local,remote,cluster,preview,debug,replayDate,safe,transExecutionConfiguration.getLogLevel());
  JobMeta jobMeta=getActiveJob();
  if (jobMeta != null)   executeJob(jobMeta,local,remote,replayDate,safe,null,0);
}

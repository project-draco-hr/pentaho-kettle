{
  try {
    StepMeta stepMeta=transMeta.findStep(wReference.getText());
    if (stepMeta != null) {
      Row prev=transMeta.getStepFields(stepMeta);
      if (prev != null) {
        BaseStepDialog.getFieldsFromPrevious(prev,wValues,1,new int[]{1},new int[]{},-1,-1,null);
      }
    }
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("MergeRowsDialog.ErrorGettingFields.DialogTitle"),Messages.getString("MergeRowsDialog.ErrorGettingFields.DialogMessage"),e);
  }
}

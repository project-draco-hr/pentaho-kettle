{
  meta=(HTTPMeta)smi;
  data=(HTTPData)sdi;
  boolean sendToErrorRow=false;
  String errorMessage=null;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (first) {
    data.outputRowMeta=getInputRowMeta().clone();
    meta.getFields(data.outputRowMeta,getStepname(),null,null,this);
  }
  try {
    Object[] outputRowData=execHttp(getInputRowMeta(),r);
    putRow(data.outputRowMeta,outputRowData);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("HTTP.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    if (getStepMeta().isDoingErrorHandling()) {
      sendToErrorRow=true;
      errorMessage=e.toString();
    }
 else {
      logError(Messages.getString("HTTP.ErrorInStepRunning") + e.getMessage());
      setErrors(1);
      stopAll();
      setOutputDone();
      return false;
    }
    if (sendToErrorRow) {
      putError(getInputRowMeta(),r,1,errorMessage,null,"HTTP001");
    }
  }
  return true;
}

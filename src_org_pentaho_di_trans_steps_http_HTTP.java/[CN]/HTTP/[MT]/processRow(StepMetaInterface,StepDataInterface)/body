{
  meta=(HTTPMeta)smi;
  data=(HTTPData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (first) {
    data.outputRowMeta=(RowMetaInterface)getInputRowMeta().clone();
    meta.getFields(data.outputRowMeta,getStepname(),null,null);
  }
  try {
    execHttp(data.outputRowMeta,r);
    putRow(data.outputRowMeta,r);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("HTTP.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    logError(Messages.getString("HTTP.ErrorInStepRunning") + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

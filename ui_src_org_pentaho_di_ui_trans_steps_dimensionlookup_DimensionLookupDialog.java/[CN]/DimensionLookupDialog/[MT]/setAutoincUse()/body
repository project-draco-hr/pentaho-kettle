{
  boolean enable=true;
  if (ci != null) {
    DatabaseInterface databaseInterface=ci.getDatabaseInterface();
    boolean supportsAutoGeneratedKeys=!(databaseInterface instanceof DatabaseInterfaceExtended) || ((DatabaseInterfaceExtended)databaseInterface).supportsAutoGeneratedKeys();
    enable=ci.supportsAutoinc() && supportsAutoGeneratedKeys;
  }
  wlAutoinc.setEnabled(enable);
  wAutoinc.setEnabled(enable);
  if (!enable && wAutoinc.getSelection()) {
    wAutoinc.setSelection(false);
    wSeqButton.setSelection(false);
    wTableMax.setSelection(true);
  }
}

{
  Collections.sort(objects,new Comparator(){
    public int compare(    Object o1,    Object o2){
      RepositoryObject r1=(RepositoryObject)o1;
      RepositoryObject r2=(RepositoryObject)o2;
      int result=0;
switch (sortPosition) {
case 0:
        result=compareStrings(r1.getName(),r2.getName());
      break;
case 1:
    result=compareStrings(r1.getObjectType(),r2.getObjectType());
  break;
case 2:
result=compareStrings(r1.getModifiedUser(),r2.getModifiedUser());
break;
case 3:
result=compareDates(r1.getModifiedDate(),r2.getModifiedDate());
break;
}
if (!ascending) result*=-1;
return result;
}
}
);
}

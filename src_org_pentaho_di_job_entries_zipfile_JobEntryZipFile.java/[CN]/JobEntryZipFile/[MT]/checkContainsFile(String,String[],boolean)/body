{
  boolean retval=false;
  for (int i=0; i < filelist.length; i++) {
    File file;
    if (isDirectory) {
      file=new File(realSourceDirectoryOrFile + Const.FILE_SEPARATOR + filelist[i]);
    }
 else {
      file=getFile(realSourceDirectoryOrFile);
    }
    if ((file.exists() && file.isFile()))     retval=true;
  }
  return retval;
}

{
  String retval="gggg";
  if (filename != null)   retval=environmentSubstitute(filename);
  SimpleDateFormat daf=new SimpleDateFormat();
  Date now=new Date();
  if (add_date) {
    daf.applyPattern("yyyMMdd");
    String d=daf.format(now);
    retval+="_" + d;
  }
  if (add_time) {
    daf.applyPattern("HHmmss");
    String t=daf.format(now);
    retval+="_" + t;
  }
  retval+=".zip";
  return retval;
}

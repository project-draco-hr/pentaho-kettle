{
  boolean answer=true;
  if (jobMeta.hasChanged()) {
    MessageBox mb=new MessageBox(shell,SWT.YES | SWT.NO | SWT.ICON_WARNING);
    mb.setMessage(Messages.getString("Chef.Dialog.OpenNewFileHasChanged.Message"));
    mb.setText(Messages.getString("Chef.Dialog.OpenNewFileHasChanged.Title"));
    answer=mb.open() == SWT.YES;
  }
  return answer;
}

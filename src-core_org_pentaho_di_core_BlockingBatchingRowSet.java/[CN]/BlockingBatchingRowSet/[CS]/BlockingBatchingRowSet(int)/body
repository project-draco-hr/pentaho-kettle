{
  super();
  putArray=new ArrayBlockingQueue<Object[][]>(100,false);
  getArray=new ArrayBlockingQueue<Object[][]>(100,false);
  size=maxSize / 100;
  Object[][] buffer;
  for (int i=0; i < 100; i++) {
    buffer=new Object[size][];
    getArray.offer(buffer);
  }
  outputBuffer=null;
  putIndex=getIndex=size;
}

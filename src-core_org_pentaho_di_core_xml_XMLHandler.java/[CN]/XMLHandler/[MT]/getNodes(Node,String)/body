{
  NodeList children;
  Node childnode;
  List<Node> nodes=new ArrayList<Node>();
  if (n == null)   return nodes;
  children=n.getChildNodes();
  for (int i=0; i < children.getLength(); i++) {
    childnode=children.item(i);
    if (childnode.getNodeName().equalsIgnoreCase(tag)) {
      nodes.add(childnode);
    }
  }
  return nodes;
}

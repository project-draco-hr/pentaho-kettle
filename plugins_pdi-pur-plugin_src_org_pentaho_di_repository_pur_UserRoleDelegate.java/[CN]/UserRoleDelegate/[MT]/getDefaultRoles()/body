{
  if (hasNecessaryPermissions) {
    try {
      return UserRoleHelper.convertToListFromProxyPentahoDefaultRoles(userRoleSecurityInfo,rsm);
    }
 catch (    Exception e) {
      throw new KettleException(BaseMessages.getString(UserRoleDelegate.class,"UserRoleDelegate.ERROR_0011_UNABLE_TO_GET_ROLES"),e);
    }
  }
 else {
    throw new KettleException(BaseMessages.getString(UserRoleDelegate.class,"UserRoleDelegate.ERROR_0014_INSUFFICIENT_PRIVILEGES"));
  }
}

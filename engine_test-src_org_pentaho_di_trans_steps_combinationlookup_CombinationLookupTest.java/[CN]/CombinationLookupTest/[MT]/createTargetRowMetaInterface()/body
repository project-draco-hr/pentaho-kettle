{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface valuesMeta[]={new ValueMeta("ID",ValueMeta.TYPE_INTEGER,8,0),new ValueMeta("DLR_CD",ValueMeta.TYPE_STRING,8,0),new ValueMeta("DLR_NM",ValueMeta.TYPE_STRING,30,0),new ValueMeta("DLR_DESC",ValueMeta.TYPE_STRING,30,0)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

{
  jobEntry.setName(wName.getText());
  jobEntry.setHostname(wHostname.getText());
  jobEntry.setNbrPackets(wNbrPackets.getText());
  jobEntry.setTimeOut(wTimeOut.getText());
  jobEntry.ipingtype=wPingType.getSelectionIndex();
  if (wPingType.getSelectionIndex() == jobEntry.isystemPing)   jobEntry.pingtype=jobEntry.systemPing;
 else   if (wPingType.getSelectionIndex() == jobEntry.ibothPings)   jobEntry.pingtype=jobEntry.bothPings;
 else   jobEntry.pingtype=jobEntry.classicPing;
  dispose();
}

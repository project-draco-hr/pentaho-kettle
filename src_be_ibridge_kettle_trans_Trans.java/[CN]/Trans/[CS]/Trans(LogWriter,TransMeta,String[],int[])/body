{
  this(lw,(String)null,(String)null,new String[]{Messages.getString("Trans.Dialog.Description.NoFileNamePreviewMode")});
  this.transMeta=transMeta;
  preview=true;
  preview_steps=prev_steps;
  preview_sizes=prev_sizes;
  log.logBasic(toString(),Messages.getString("Trans.Log.TransformationIsInPreviewMode"));
  log.logDebug(toString(),Messages.getString("Trans.Log.NumberOfStepsToPreivew",String.valueOf(transMeta.nrSteps()),String.valueOf(transMeta.nrTransHops())));
}

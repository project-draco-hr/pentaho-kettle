{
  int nrEnded=0;
  if (steps == null)   return 0;
  for (int i=0; i < steps.size(); i++) {
    StepMetaDataCombi sid=(StepMetaDataCombi)steps.get(i);
    BaseStep thr=(BaseStep)sid.step;
    StepDataInterface data=sid.data;
    if ((thr != null && !thr.isAlive()) || data.getStatus() == StepDataInterface.STATUS_FINISHED || data.getStatus() == StepDataInterface.STATUS_HALTED || data.getStatus() == StepDataInterface.STATUS_STOPPED) {
      nrEnded++;
    }
  }
  return nrEnded;
}

{
  String message;
  if (running) {
    if (isFinished()) {
      message="Finished";
      if (getResult().getNrErrors() > 0)       message+=" (with errors)";
    }
 else {
      message="Running";
    }
  }
 else   if (preparing) {
    message="Preparing executing";
  }
 else   if (initializing) {
    message="Initializing";
  }
 else {
    message="Waiting";
  }
  return message;
}

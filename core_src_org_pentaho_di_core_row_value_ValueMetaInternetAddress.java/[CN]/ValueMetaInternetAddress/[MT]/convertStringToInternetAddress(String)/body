{
  string=Const.trimToType(string,getTrimType());
  if (Const.isEmpty(string)) {
    return null;
  }
  try {
    return InetAddress.getByName(string);
  }
 catch (  Exception e) {
    throw new KettleValueException(toString() + " : couldn't convert string [" + string+ "] to an internet address",e);
  }
}

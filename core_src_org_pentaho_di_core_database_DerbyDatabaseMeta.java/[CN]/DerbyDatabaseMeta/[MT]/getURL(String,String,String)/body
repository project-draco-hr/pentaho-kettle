{
  if (getAccessType() == DatabaseMeta.TYPE_ACCESS_NATIVE) {
    if (!Const.isEmpty(hostname)) {
      String url="jdbc:derby://" + hostname;
      if (!Const.isEmpty(port)) {
        url+=":" + port;
      }
      url+="/" + databaseName;
      return url;
    }
 else {
      return "jdbc:derby:" + databaseName;
    }
  }
 else {
    return "jdbc:odbc:" + databaseName;
  }
}

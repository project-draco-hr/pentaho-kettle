{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMeta("field1",ValueMeta.TYPE_STRING),new ValueMeta("field2",ValueMeta.TYPE_STRING),new ValueMeta("outf3",ValueMeta.TYPE_STRING)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

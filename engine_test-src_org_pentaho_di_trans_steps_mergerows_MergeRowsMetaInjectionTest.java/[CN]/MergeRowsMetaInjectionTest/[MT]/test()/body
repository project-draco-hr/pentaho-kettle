{
  check("FLAG_FIELD",new StringGetter(){
    public String get(){
      return meta.getFlagField();
    }
  }
);
  check("KEY_FIELDS",new StringGetter(){
    public String get(){
      return meta.getKeyFields()[0];
    }
  }
);
  check("VALUE_FIELDS",new StringGetter(){
    public String get(){
      return meta.getValueFields()[0];
    }
  }
);
}

{
  if (rowList == null) {
    wText.setText("");
    return;
  }
  int nr=wFields.table.getSelectionIndex();
  if (nr >= 0 && rowList != null && nr < rowList.size()) {
    Row row=(Row)rowList.get(nr);
    String logging=row.getString("LOG_FIELD","");
    if (logging != null) {
      wText.setText(logging);
    }
 else {
      wText.setText("");
    }
  }
}

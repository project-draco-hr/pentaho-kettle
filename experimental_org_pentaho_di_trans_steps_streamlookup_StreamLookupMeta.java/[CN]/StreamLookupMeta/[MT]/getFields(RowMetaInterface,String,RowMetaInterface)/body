{
  if (info != null && info.size() != 0) {
    for (int i=0; i < valueName.length; i++) {
      ValueMetaInterface v=info.searchValueMeta(value[i]);
      if (v != null) {
        v.setName(valueName[i]);
        v.setOrigin(name);
        row.addValueMeta(v);
      }
 else {
        throw new KettleStepException(Messages.getString("StreamLookupMeta.Exception.ReturnValueCanNotBeFound",value[i]));
      }
    }
  }
 else {
    for (int i=0; i < valueName.length; i++) {
      ValueMetaInterface v=new ValueMeta(valueName[i],valueDefaultType[i]);
      v.setOrigin(name);
      row.addValueMeta(v);
    }
  }
}

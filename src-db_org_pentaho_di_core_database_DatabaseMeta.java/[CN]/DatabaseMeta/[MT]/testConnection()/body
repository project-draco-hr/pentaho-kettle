{
  StringBuffer report=new StringBuffer();
  try {
    DatabaseFactoryInterface factory=getDatabaseFactory();
    return factory.getConnectionTestReport(this);
  }
 catch (  ClassNotFoundException e) {
    report.append(Messages.getString("BaseDatabaseMeta.TestConnectionReportNotImplemented.Message")).append(Const.CR);
    report.append(Messages.getString("DatabaseMeta.report.ConnectionError",getName()) + e.toString() + Const.CR);
    report.append(Const.getStackTracker(e) + Const.CR);
  }
catch (  Exception e) {
    report.append(Messages.getString("DatabaseMeta.report.ConnectionError",getName()) + e.toString() + Const.CR);
    report.append(Const.getStackTracker(e) + Const.CR);
  }
  return report.toString();
}

{
  Object[] RowData=new Object[data.outputRowMeta.size()];
  System.arraycopy(row,0,RowData,0,rowMeta.size());
  int length=meta.getFieldInStream().length;
  for (int i=0; i < length; i++) {
    String value=ReplaceInString((String)row[data.inStreamNrs[i]],data.useRegEx[i],data.replaceString[i],data.replaceByString[i],data.wholeWordl[i],data.caseSensitive[i]);
    if (Const.isEmpty(data.outStreamNrs[i]))     RowData[data.inStreamNrs[i]]=value;
 else     RowData[data.inputFieldsNr + i]=value;
  }
  return RowData;
}

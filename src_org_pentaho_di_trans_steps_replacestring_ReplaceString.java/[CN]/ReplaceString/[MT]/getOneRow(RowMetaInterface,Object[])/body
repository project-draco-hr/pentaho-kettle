{
  Object[] rowData=RowDataUtil.resizeArray(row,data.outputRowMeta.size());
  int length=meta.getFieldInStream().length;
  for (int i=0; i < length; i++) {
    String value=replaceString(getInputRowMeta().getString(row,data.inStreamNrs[i]),data.patterns[i],data.replaceByString[i]);
    if (Const.isEmpty(data.outStreamNrs[i]))     rowData[data.inStreamNrs[i]]=value;
 else     rowData[data.inputFieldsNr + i]=value;
  }
  return rowData;
}

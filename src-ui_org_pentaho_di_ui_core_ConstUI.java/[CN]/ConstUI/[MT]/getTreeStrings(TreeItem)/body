{
  int nrlevels=getTreeLevel(ti) + 1;
  String retval[]=new String[nrlevels];
  int level=0;
  retval[nrlevels - 1]=ti.getText();
  TreeItem parent=ti.getParentItem();
  while (parent != null) {
    level++;
    retval[nrlevels - level - 1]=parent.getText();
    parent=parent.getParentItem();
  }
  return retval;
}

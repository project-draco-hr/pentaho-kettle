{
  CheckResult cr;
  if (input.length > 0)   cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.NoInputExpected"),stepMeta);
 else   cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.NoInput"),stepMeta);
  remarks.add(cr);
  if (Const.isEmpty(targeturl))   cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.NoURL"),stepMeta);
 else   cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.URLOk"),stepMeta);
  remarks.add(cr);
  if (Const.isEmpty(username))   cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.NoUsername"),stepMeta);
 else   cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.UsernameOk"),stepMeta);
  remarks.add(cr);
  if (Const.isEmpty(module))   cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.NoModule"),stepMeta);
 else   cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.ModuleOk"),stepMeta);
  remarks.add(cr);
  if (updateLookup.length == 0)   cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.NoFields"),stepMeta);
 else   cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"SalesforceInputMeta.CheckResult.FieldsOk"),stepMeta);
  remarks.add(cr);
}

{
  int i;
  log.logDebug(toString(),Messages.getString("FlattenerDialog.Log.GettingKeyInfo"));
  if (input.getFieldName() != null)   wField.setText(input.getFieldName());
  if (input.getTargetField() != null)   for (i=0; i < input.getTargetField().length; i++) {
    TableItem item=wFields.table.getItem(i);
    if (input.getTargetField()[i] != null)     item.setText(1,input.getTargetField()[i]);
  }
  wStepname.selectAll();
  wFields.setRowNums();
  wFields.optWidth(true);
}

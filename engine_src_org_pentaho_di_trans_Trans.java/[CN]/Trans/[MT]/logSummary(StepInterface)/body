{
  log.logBasic(si.getStepname(),BaseMessages.getString(PKG,"Trans.Log.FinishedProcessing",String.valueOf(si.getLinesInput()),String.valueOf(si.getLinesOutput()),String.valueOf(si.getLinesRead())) + BaseMessages.getString(PKG,"Trans.Log.FinishedProcessing2",String.valueOf(si.getLinesWritten()),String.valueOf(si.getLinesUpdated()),String.valueOf(si.getErrors())));
}

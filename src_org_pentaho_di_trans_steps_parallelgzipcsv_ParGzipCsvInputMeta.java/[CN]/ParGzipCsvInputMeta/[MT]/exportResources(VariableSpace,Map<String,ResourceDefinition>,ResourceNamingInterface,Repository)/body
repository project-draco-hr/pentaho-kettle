{
  try {
    if (Const.isEmpty(filenameField)) {
      FileObject fileObject=KettleVFS.getFileObject(space.environmentSubstitute(filename));
      if (fileObject.exists()) {
        filename=fileObject.getName().getPath();
        return filename;
      }
    }
    return null;
  }
 catch (  Exception e) {
    throw new KettleException(e);
  }
}

{
  boolean retval=false;
  try {
    data.writer.write(("</" + meta.getMainElement() + ">"+ Const.CR).toCharArray());
    if (meta.isZipped()) {
      data.zip.closeEntry();
      data.zip.finish();
      data.zip.close();
    }
 else {
      data.writer.close();
    }
    retval=true;
  }
 catch (  Exception e) {
  }
  return retval;
}

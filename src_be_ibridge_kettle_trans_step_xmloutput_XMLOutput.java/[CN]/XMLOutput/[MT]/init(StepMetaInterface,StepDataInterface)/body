{
  meta=(XMLOutputMeta)smi;
  data=(XMLOutputData)sdi;
  if (super.init(smi,sdi)) {
    data.splitnr=0;
    if (openNewFile()) {
      debug="start";
      return true;
    }
 else {
      logError("Couldn't open file " + meta.getFileName() + " ["+ debug+ "]");
      setErrors(1L);
      stopAll();
    }
  }
  return false;
}

{
  try {
    String str=XMLHandler.addTagValue(element,formatField(v,idx),false);
    if (str != null)     data.writer.write(str.toCharArray());
    debug="End of writeField";
  }
 catch (  Exception e) {
    logError("Error writing line in [" + debug + "] :"+ e.toString());
    return false;
  }
  return true;
}

{
  if (index != previousLogTableIndex) {
switch (previousLogTableIndex) {
case LOG_INDEX_JOB:
      getJobLogTableOptions();
    break;
case LOG_INDEX_CHANNEL:
  getChannelLogTableOptions();
break;
case LOG_INDEX_JOB_ENTRY:
getJobEntryLogTableOptions();
break;
default :
break;
}
for (Control control : wLogOptionsComposite.getChildren()) {
control.dispose();
}
switch (index) {
case LOG_INDEX_JOB:
showJobLogTableOptions();
break;
case LOG_INDEX_CHANNEL:
showChannelLogTableOptions();
break;
case LOG_INDEX_JOB_ENTRY:
showJobEntryLogTableOptions();
break;
default :
break;
}
}
}

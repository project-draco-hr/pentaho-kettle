{
  ValueMetaInterface valueMeta=rowMeta.searchValueMeta(valueRename.getSourceValueName());
  if (valueMeta == null) {
    valueMeta=rowMeta.searchValueMeta(valueRename.getTargetValueName());
    if (valueMeta == null) {
      throw new KettleStepException(BaseMessages.getString(PKG,"MappingInput.Exception.UnableToFindMappedValue",valueRename.getSourceValueName()));
    }
  }
 else {
    valueMeta.setName(valueRename.getTargetValueName());
  }
}

{
  try {
    logBasic(BaseMessages.getString(PKG,"System.Log.StartingToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Throwable t) {
    logError(BaseMessages.getString(PKG,"System.Log.UnexpectedError") + " : ");
    logError(Const.getStackTracker(t));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

{
  Method method=null;
  try {
    method=baseClass.getMethod(getter,(Class[])null);
  }
 catch (  Exception e) {
    for (int i=0; i < methods.length; i++) {
      String methodName=methods[i].getName();
      if (methodName.equalsIgnoreCase(getter)) {
        return methods[i];
      }
    }
  }
  return method;
}

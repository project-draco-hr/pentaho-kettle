{
  super(parent,SWT.NONE);
  this.props=PropsUI.getInstance();
  this.rep=rep;
  this.showTrans=showTransformations;
  this.showJobs=showJobs;
  this.includeDeleted=false;
  shellText=BaseMessages.getString(PKG,"SelectObjectDialog.Dialog.Main.Title");
  lineText=BaseMessages.getString(PKG,"SelectObjectDialog.Dialog.Object.Title");
  objectName=null;
  objectDirectory=null;
  sortColumn=0;
  ascending=false;
  this.capabilities=rep.getRepositoryMeta().getRepositoryCapabilities();
  if (capabilities.supportsUsers()) {
    this.showTrans=showTransformations && rep.getUserInfo().useTransformations();
    this.showJobs=showJobs && rep.getUserInfo().useJobs();
  }
}

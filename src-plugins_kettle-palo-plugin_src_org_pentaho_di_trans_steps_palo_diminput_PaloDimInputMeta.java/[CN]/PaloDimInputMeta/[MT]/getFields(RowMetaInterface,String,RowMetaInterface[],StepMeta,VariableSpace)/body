{
  if (databaseMeta == null)   throw new KettleStepException("There is no Palo database server connection defined");
  final PaloHelper helper=new PaloHelper(databaseMeta);
  try {
    helper.connect();
    try {
      final RowMetaInterface rowMeta=helper.getDimensionRowMeta(this.getDimension(),this.getLevels(),this.getBaseElementsOnly());
      row.addRowMeta(rowMeta);
    }
  finally {
      helper.disconnect();
    }
  }
 catch (  Exception e) {
    throw new KettleStepException(e);
  }
}

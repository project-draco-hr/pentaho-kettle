{
  meta=(DatabaseJoinMeta)smi;
  data=(DatabaseJoinData)sdi;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  try {
    lookupValues(r);
    if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)     logBasic(Messages.getString("DatabaseJoin.Log.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    logError(Messages.getString("DatabaseJoin.Log.ErrorInStepRunning") + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

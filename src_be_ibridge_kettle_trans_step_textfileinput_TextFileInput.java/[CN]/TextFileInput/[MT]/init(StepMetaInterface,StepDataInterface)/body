{
  meta=(TextFileInputMeta)smi;
  data=(TextFileInputData)sdi;
  if (super.init(smi,sdi)) {
    data.files=meta.getFiles();
    if (data.files == null || data.files.length == 0) {
      logError("No file(s) specified! Stop processing.");
      return false;
    }
    return true;
  }
  return false;
}

{
  try {
    if (data.filename != null) {
      if (meta.isZipped()) {
        data.zi.closeEntry();
        data.zi.close();
      }
      data.fr.close();
    }
  }
 catch (  Exception e) {
    logError("Couldn't close file : " + data.filename + " --> "+ e.toString());
    stopAll();
    setErrors(1);
    return false;
  }
  return true;
}

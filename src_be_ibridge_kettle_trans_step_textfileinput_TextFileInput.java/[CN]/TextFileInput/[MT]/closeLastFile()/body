{
  try {
    debug="closeLastFile: close";
    if (data.filename != null) {
      if (meta.isZipped()) {
        data.zi.closeEntry();
        data.zi.close();
      }
      data.fr.close();
      data.isr.close();
      data.filename=null;
    }
    data.badLineHandler.close();
  }
 catch (  Exception e) {
    logError("Couldn't close file : " + data.filename + " --> "+ e.toString());
    stopAll();
    setErrors(1);
    return false;
  }
  debug="closeLastFile: end.";
  return !data.isLastFile;
}

{
  Date replayDate=getTrans().getReplayDate();
  if (replayDate == null)   data.textFileReplayFactory=FilePlayListAll.INSTANCE;
 else   data.textFileReplayFactory=new FilePlayListReplay(replayDate,meta.getLineNumberFilesDestinationDirectory(),meta.getLineNumberFilesExtension(),meta.getErrorLineFilesDestinationDirectory(),meta.getErrorLineFilesExtension(),meta.getEncoding());
}

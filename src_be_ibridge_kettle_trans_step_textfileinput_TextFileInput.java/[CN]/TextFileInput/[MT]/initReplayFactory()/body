{
  Date replayDate=getTrans().getReplayDate();
  if (replayDate == null)   data.textFileReplayFactory=TextFilePlayerAll.INSTANCE;
 else   data.textFileReplayFactory=new TextFileReplayFactoryLineNumber(replayDate,meta.getLineNumberFilesDestinationDirectory(),meta.getLineNumberFilesExtension(),meta.getEncoding());
}

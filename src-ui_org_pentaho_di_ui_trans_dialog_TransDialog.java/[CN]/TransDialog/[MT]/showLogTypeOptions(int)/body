{
  if (index != previousLogTableIndex) {
switch (previousLogTableIndex) {
case 0:
      getTransLogTableOptions();
    break;
default :
  break;
}
for (Control control : wLogOptionsComposite.getChildren()) {
control.dispose();
}
previousLogTableIndex=index;
switch (index) {
case 0:
showTransLogTableOptions();
break;
default :
break;
}
}
}

{
  check("JOIN_TYPE",new StringGetter(){
    public String get(){
      return meta.getJoinType();
    }
  }
);
  check("KEY_FIELDS",new StringGetter(){
    public String get(){
      return meta.getKeyFields()[0];
    }
  }
);
  check("INPUT_STEPS",new StringGetter(){
    public String get(){
      return meta.getInputSteps()[0];
    }
  }
);
}

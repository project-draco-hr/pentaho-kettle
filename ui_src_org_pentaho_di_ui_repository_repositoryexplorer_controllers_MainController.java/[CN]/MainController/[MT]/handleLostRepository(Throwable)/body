{
  KettleRepositoryLostException repLost=KettleRepositoryLostException.lookupStackStrace(e);
  try {
    if (repLost != null) {
      if (!isAborting()) {
        new ErrorDialog(shell,BaseMessages.getString(PKG,"RepositoryExplorer.Dialog.Error.Title"),repLost.getLocalizedMessage(),e);
        if (callback != null && callback.error(null)) {
          closeDialog();
        }
      }
      return true;
    }
  }
 catch (  Exception ex) {
    return true;
  }
  return false;
}

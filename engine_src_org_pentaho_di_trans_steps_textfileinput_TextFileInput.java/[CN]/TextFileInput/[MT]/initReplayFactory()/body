{
  Date replayDate=getTrans().getReplayDate();
  if (replayDate == null) {
    data.filePlayList=FilePlayListAll.INSTANCE;
  }
 else {
    data.filePlayList=new FilePlayListReplay(replayDate,meta.getLineNumberFilesDestinationDirectory(),meta.getLineNumberFilesExtension(),meta.getErrorFilesDestinationDirectory(),meta.getErrorLineFilesExtension(),meta.getEncoding());
  }
}

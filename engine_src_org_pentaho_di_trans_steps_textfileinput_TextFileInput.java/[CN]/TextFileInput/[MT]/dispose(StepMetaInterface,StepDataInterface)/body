{
  meta=(TextFileInputMeta)smi;
  data=(TextFileInputData)sdi;
  if (data.file != null) {
    try {
      data.file.close();
      data.file=null;
    }
 catch (    Exception e) {
      log.logError("Error closing file",e);
    }
  }
  if (data.fr != null) {
    BaseStep.closeQuietly(data.fr);
    data.fr=null;
  }
  super.dispose(smi,sdi);
}

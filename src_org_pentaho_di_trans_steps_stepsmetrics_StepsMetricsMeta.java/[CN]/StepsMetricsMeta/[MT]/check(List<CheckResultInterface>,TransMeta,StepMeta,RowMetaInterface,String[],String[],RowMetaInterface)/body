{
  CheckResult cr;
  if (prev == null || prev.size() == 0) {
    cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"StepsMetricsMeta.CheckResult.NotReceivingFields"),stepMeta);
    remarks.add(cr);
    if (stepName.length > 0)     cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"StepsMetricsMeta.CheckResult.AllStepsFound"),stepMeta);
 else     cr=new CheckResult(CheckResult.TYPE_RESULT_WARNING,BaseMessages.getString(PKG,"StepsMetricsMeta.CheckResult.NoStepsEntered"),stepMeta);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"StepsMetricsMeta.CheckResult.ReceivingFields"),stepMeta);
    remarks.add(cr);
  }
  if (input.length > 0)   cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"StepsMetricsMeta.CheckResult.StepRecevingData2"),stepMeta);
 else   cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"StepsMetricsMeta.CheckResult.NoInputReceivedFromOtherSteps"),stepMeta);
  remarks.add(cr);
}

{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface valuesMeta[]={new ValueMeta("string",ValueMeta.TYPE_STRING),new ValueMeta("lpadded1",ValueMeta.TYPE_STRING),new ValueMeta("lpadded2",ValueMeta.TYPE_STRING),new ValueMeta("rpadded1",ValueMeta.TYPE_STRING),new ValueMeta("rpadded2",ValueMeta.TYPE_STRING),new ValueMeta("upperStr",ValueMeta.TYPE_STRING),new ValueMeta("lowerStr",ValueMeta.TYPE_STRING)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

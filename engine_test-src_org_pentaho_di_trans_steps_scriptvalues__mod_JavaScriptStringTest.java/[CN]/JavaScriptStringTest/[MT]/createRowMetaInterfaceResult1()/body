{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface valuesMeta[]={new ValueMeta("string",ValueMeta.TYPE_STRING),new ValueMeta("original",ValueMeta.TYPE_STRING),new ValueMeta("ltrimStr",ValueMeta.TYPE_STRING),new ValueMeta("rtrimStr",ValueMeta.TYPE_STRING),new ValueMeta("trimStr",ValueMeta.TYPE_STRING)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

{
  int nrkeys=wFields.nrNonEmpty();
  inf.allocate(nrkeys);
  if (log.isDebug())   log.logDebug(toString(),BaseMessages.getString(PKG,"GetPreviousRowFieldDialog.Log.FoundFields",String.valueOf(nrkeys)));
  for (int i=0; i < nrkeys; i++) {
    TableItem item=wFields.getNonEmpty(i);
    inf.getFieldInStream()[i]=item.getText(1);
    inf.getFieldOutStream()[i]=item.getText(2);
  }
  stepname=wStepname.getText();
}

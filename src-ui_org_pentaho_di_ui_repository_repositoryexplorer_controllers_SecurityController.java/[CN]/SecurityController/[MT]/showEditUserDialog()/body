{
  if (rsm != null && rsm.getRoles() != null) {
    securityUser.clear();
    securityUser.setUser(security.getSelectedUser(),convertToUIRoleModel(rsm.getRoles()));
    securityUser.setMode(Mode.EDIT);
    userDialog.setTitle(messages.getString("EditUserDialog.Title"));
    userDialog.show();
  }
}

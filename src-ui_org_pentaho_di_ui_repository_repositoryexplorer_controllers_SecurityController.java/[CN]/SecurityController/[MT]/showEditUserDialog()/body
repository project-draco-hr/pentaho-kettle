{
  if (rui != null && rui.getRoles() != null) {
    securityUser.clear();
    securityUser.setUser(security.getSelectedUser(),convertToUIRoleModel(rui.getRoles()));
    securityUser.setMode(Mode.EDIT);
    userDialog.setTitle(messages.getString("EditUserDialog.Title"));
    userDialog.show();
  }
}

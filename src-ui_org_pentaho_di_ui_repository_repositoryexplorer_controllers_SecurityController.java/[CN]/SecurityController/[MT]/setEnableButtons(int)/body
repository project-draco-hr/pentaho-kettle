{
  boolean enableAdd=true;
  boolean enableEdit=false;
  boolean enableRemove=false;
  if (managed) {
    if (selectedIndex >= 0) {
      enableRemove=true;
      enableEdit=true;
    }
 else {
      enableRemove=false;
      enableEdit=false;
    }
  }
 else {
    enableAdd=false;
  }
  enableButtons(enableAdd,enableEdit,enableRemove);
}

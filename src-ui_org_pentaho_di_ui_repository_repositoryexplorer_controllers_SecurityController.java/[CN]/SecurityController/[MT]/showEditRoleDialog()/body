{
  if (rui != null && rui.getUsers() != null) {
    securityRole.clear();
    securityRole.setRole(security.getSelectedRole(),convertToUIUserModel(rui.getUsers()));
    securityRole.setMode(Mode.EDIT);
    roleDialog.setTitle(messages.getString("EditRoleDialog.Title"));
    roleDialog.show();
  }
}

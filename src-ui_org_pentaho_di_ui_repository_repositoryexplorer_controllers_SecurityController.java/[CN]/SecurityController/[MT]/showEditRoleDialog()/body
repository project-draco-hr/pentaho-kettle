{
  if (rsm != null && rsm.getUsers() != null) {
    securityRole.clear();
    securityRole.setRole(security.getSelectedRole(),convertToUIUserModel(rsm.getUsers()));
    securityRole.setMode(Mode.EDIT);
    roleDialog.setTitle(messages.getString("EditRoleDialog.Title"));
    roleDialog.show();
  }
}

{
  StringBuilder retval=new StringBuilder();
  retval.append("    <steps>" + Const.CR);
  for (int i=0; i < stepName.length; i++) {
    retval.append("      <step>" + Const.CR);
    retval.append("        " + XMLHandler.addTagValue("name",stepName[i]));
    retval.append("        </step>" + Const.CR);
  }
  retval.append("      </steps>" + Const.CR);
  return retval.toString();
}

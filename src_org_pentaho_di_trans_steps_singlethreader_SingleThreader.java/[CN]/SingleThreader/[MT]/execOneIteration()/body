{
  boolean more=false;
  try {
    more=data.executor.oneIteration();
    if (data.executor.isStopped() || data.executor.getErrors() > 0) {
      setErrors(1);
      stopAll();
      logError(BaseMessages.getString(PKG,"SingleThreader.Log.ErrorOccurredInSubTransformation"));
      return false;
    }
  }
 catch (  Exception e) {
    setErrors(1L);
    stopAll();
    logError(BaseMessages.getString(PKG,"SingleThreader.Log.ErrorOccurredInSubTransformation"));
    return false;
  }
  return more;
}

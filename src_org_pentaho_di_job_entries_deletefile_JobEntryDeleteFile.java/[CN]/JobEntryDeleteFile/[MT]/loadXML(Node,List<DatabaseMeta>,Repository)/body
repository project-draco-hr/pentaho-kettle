{
  try {
    super.loadXML(entrynode,databases);
    filename=XMLHandler.getTagValue(entrynode,"filename");
    failIfFileNotExists="Y".equalsIgnoreCase(XMLHandler.getTagValue(entrynode,"fail_if_file_not_exists"));
  }
 catch (  KettleXMLException xe) {
    throw new KettleXMLException(Messages.getString("JobEntryDeleteFile.Error_0001_Unable_To_Load_Job_From_Xml_Node"),xe);
  }
}

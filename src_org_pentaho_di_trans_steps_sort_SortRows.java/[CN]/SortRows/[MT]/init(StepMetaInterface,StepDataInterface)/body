{
  meta=(SortRowsMeta)smi;
  data=(SortRowsData)sdi;
  if (super.init(smi,sdi)) {
    data.buffer=new Object[meta.getSortSize()];
    data.bufferSize=0;
    if (meta.getSortSize() > 0) {
      data.rowbuffer=new ArrayList(meta.getSortSize());
    }
 else {
      data.rowbuffer=new ArrayList();
    }
    return true;
  }
  return false;
}

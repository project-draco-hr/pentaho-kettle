{
  data.buffer=null;
  data.rowbuffer=null;
  if ((data.dis != null) && (data.dis.size() > 0)) {
    for (    DataInputStream dis : data.dis) {
      BaseStep.closeQuietly(dis);
    }
  }
  if ((data.fis != null) && (data.fis.size() > 0)) {
    for (    InputStream is : data.fis) {
      BaseStep.closeQuietly(is);
    }
  }
  super.dispose(smi,sdi);
}

{
  try {
    if (ArgList.length == 1 && !isNull(ArgList[0]) && !isUndefined(ArgList[0])) {
      FileObject fileObject=null;
      try {
        fileObject=KettleVFS.getFileObject((String)ArgList[0]);
        if (fileObject.exists()) {
          if (fileObject.getType() == FileType.FILE) {
            if (!fileObject.delete())             new RuntimeException("We can not delete file [" + (String)ArgList[0] + "]!");
          }
        }
 else {
          new RuntimeException("file [" + (String)ArgList[0] + "] can not be found!");
        }
      }
 catch (      IOException e) {
        throw new RuntimeException("The function call deleteFile is not valid.");
      }
 finally {
        if (fileObject != null)         try {
          fileObject.close();
        }
 catch (        Exception e) {
        }
      }
    }
 else {
      throw new RuntimeException("The function call deleteFile is not valid.");
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e.toString());
  }
}

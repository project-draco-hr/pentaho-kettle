{
  if (ArgList.length == 1) {
    try {
      if (isNull(ArgList[0])) {
        return null;
      }
 else       if (isUndefined(ArgList[0])) {
        return undefinedValue;
      }
      return Boolean.TRUE;
    }
 catch (    Exception e) {
      return Boolean.FALSE;
    }
  }
 else {
    throw new RuntimeException("The function call isDate requires 1 argument.");
  }
}

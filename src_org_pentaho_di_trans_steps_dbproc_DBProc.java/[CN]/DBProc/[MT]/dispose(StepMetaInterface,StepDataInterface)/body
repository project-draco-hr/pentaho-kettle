{
  meta=(DBProcMeta)smi;
  data=(DBProcData)sdi;
  try {
    if (!meta.isAutoCommit()) {
      data.db.commit();
    }
  }
 catch (  KettleDatabaseException e) {
    logError(BaseMessages.getString(PKG,"DBProc.Log.CommitError") + e.getMessage());
  }
 finally {
    if (data.db != null) {
      data.db.disconnect();
    }
  }
  super.dispose(smi,sdi);
}

{
  LucidDBStreamingLoaderMeta info=new LucidDBStreamingLoaderMeta();
  getInfo(info);
  RowMetaInterface prev;
  try {
    prev=transMeta.getPrevStepFields(stepname);
    info.setSql_statement(info.getSQLStatements(prev));
    return;
  }
 catch (  KettleStepException e) {
    e.printStackTrace();
  }
}

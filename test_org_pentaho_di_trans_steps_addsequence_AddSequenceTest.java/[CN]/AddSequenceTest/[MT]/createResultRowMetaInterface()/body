{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMetaInteger("counter"),new ValueMetaInteger("valuename"),new ValueMetaInteger("valuename_1"),new ValueMetaInteger("valuename_2")};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

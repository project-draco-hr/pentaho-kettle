{
  if (databaseMeta == null)   throw new KettleStepException("There is no OpenERP database server connection defined");
  final OpenERPHelper helper=new OpenERPHelper(databaseMeta);
  try {
    helper.StartSession();
    final RowMetaInterface rowMeta=this.getRowMeta();
    row.addRowMeta(rowMeta);
  }
 catch (  Exception e) {
    throw new KettleStepException(e);
  }
}

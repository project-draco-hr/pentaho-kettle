{
  meta=(AccessOutputMeta)smi;
  data=(AccessOutputData)sdi;
  if (super.init(smi,sdi)) {
    if (!meta.isDoNotOpenNewFileInit()) {
      try {
        return OpenFile();
      }
 catch (      Exception e) {
        logError("An error occurred intialising this step: " + e.getMessage());
        stopAll();
        setErrors(1);
      }
    }
 else {
      return true;
    }
  }
  return false;
}

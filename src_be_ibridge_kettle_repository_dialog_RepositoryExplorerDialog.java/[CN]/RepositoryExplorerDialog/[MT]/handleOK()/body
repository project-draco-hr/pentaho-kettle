{
  try {
    rep.commit();
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,props,Messages.getString("RepositoryExplorerDialog.PopupMenu.Dialog.ErrorCommitingChanges.Title"),Messages.getString("RepositoryExplorerDialog.PopupMenu.Dialog.ErrorCommitingChanges.Message"),e);
  }
  dispose();
}

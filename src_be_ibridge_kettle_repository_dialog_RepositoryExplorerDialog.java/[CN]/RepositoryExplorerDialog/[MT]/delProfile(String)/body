{
  try {
    long idProfile=rep.getProfileID(profilename);
    if (idProfile > 0) {
      rep.delProfile(idProfile);
    }
    refreshTree();
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("RepositoryExplorerDialog.Dialog.Profile.Delete.UnexpectedError.Title"),Messages.getString("RepositoryExplorerDialog.Dialog.Profile.Delete.UnexpectedError.Message"),e);
  }
}

{
  try {
    repdir.delFromRep(rep);
    refreshTree();
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("RepositoryExplorerDialog.Dialog.Directory.Delete.ErrorRemoving.Title"),Messages.getString("RepositoryExplorerDialog.Dialog.Directory.Delete.ErrorRemoving.Message1"),e);
  }
}

{
  String encrPassword="";
  List<String> varList=new ArrayList<String>();
  StringUtil.getUsedVariables(password,varList,true);
  if (varList.isEmpty()) {
    encrPassword=PASSWORD_ENCRYPTED_PREFIX + Encr.encryptPassword(password);
  }
 else {
    encrPassword=password;
  }
  return encrPassword;
}

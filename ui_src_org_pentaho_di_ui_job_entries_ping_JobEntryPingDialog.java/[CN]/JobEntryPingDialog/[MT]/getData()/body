{
  wName.setText(Const.nullToEmpty(jobEntry.getName()));
  if (jobEntry.getHostname() != null)   wHostname.setText(jobEntry.getHostname());
  if (jobEntry.getNbrPackets() != null)   wNbrPackets.setText(jobEntry.getNbrPackets());
 else   wNbrPackets.setText("2");
  if (jobEntry.getTimeOut() != null)   wTimeOut.setText(jobEntry.getTimeOut());
 else   wTimeOut.setText("3000");
  wPingType.select(jobEntry.ipingtype);
  wName.selectAll();
  wName.setFocus();
}

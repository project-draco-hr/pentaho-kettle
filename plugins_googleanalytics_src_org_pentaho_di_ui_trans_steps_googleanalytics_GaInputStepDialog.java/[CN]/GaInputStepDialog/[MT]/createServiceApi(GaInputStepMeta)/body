{
  try {
    getInfo(meta);
    API=GoogleAnalyticsApiFacade.createFor(transMeta.environmentSubstitute(wGaAppName.getText()),transMeta.environmentSubstitute(wOauthAccount.getText()),transMeta.environmentSubstitute(keyFilename.getText()));
  }
 catch (  Exception e) {
    logError(BaseMessages.getString(PKG,"GoogleAnalytics.Error.UnableToLoadPrivateKey"),e);
  }
}

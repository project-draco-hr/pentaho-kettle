{
  Input input=op.getInput();
  if (input != null) {
    Message in=input.getMessage();
    List paramOrdering=op.getParameterOrdering();
    List inParts=in.getOrderedParts(paramOrdering);
    for (Iterator itr=inParts.iterator(); itr.hasNext(); ) {
      _params.add((Part)itr.next(),true);
    }
  }
  Output output=op.getOutput();
  if (output != null) {
    Message out=output.getMessage();
    List outParts=out.getOrderedParts(null);
    for (Iterator itr=outParts.iterator(); itr.hasNext(); ) {
      _oneway=false;
      _params.add((Part)itr.next(),false);
    }
  }
}

{
  String overlay=null;
  try {
    overlay=aclEnabled ? "org/pentaho/di/ui/repository/repositoryexplorer/xul/acl-enabled-layout-overlay.xul" : "org/pentaho/di/ui/repository/repositoryexplorer/xul/acl-disabled-layout-overlay.xul";
    container.loadOverlay(overlay);
  }
 catch (  XulException e) {
    log.error(BaseMessages.getString(CLZ,"RepositoryExplorer.ErrorLoadingXulOverlay",overlay));
    e.printStackTrace();
  }
}

{
  try {
    String overlay=aclEnabled ? "org/pentaho/di/ui/repository/repositoryexplorer/xul/acl-enabled-layout-overlay.xul" : "org/pentaho/di/ui/repository/repositoryexplorer/xul/acl-disabled-layout-overlay.xul";
    container.loadOverlay(overlay);
  }
 catch (  XulException e) {
    log.error("Error loading Xul overlay: acl-layout-overlay.xul");
    e.printStackTrace();
  }
}

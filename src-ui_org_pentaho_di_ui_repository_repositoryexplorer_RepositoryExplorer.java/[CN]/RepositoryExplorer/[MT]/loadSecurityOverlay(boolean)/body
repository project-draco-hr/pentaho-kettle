{
  try {
    String overlay=securityEnabled ? "org/pentaho/di/ui/repository/repositoryexplorer/xul/security-enabled-layout-overlay.xul" : "org/pentaho/di/ui/repository/repositoryexplorer/xul/security-disabled-layout-overlay.xul";
    container.loadOverlay(overlay);
  }
 catch (  XulException e) {
    log.error("Error loading Xul overlay: security-layout-overlay.xul");
    e.printStackTrace();
  }
}

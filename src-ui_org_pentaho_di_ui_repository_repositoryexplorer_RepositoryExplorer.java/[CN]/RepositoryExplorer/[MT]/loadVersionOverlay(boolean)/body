{
  try {
    String overlay=versionEnabled ? "org/pentaho/di/ui/repository/repositoryexplorer/xul/version-layout-overlay.xul" : "org/pentaho/di/ui/repository/repositoryexplorer/xul/version-disabled-layout-overlay.xul";
    container.loadOverlay(overlay);
  }
 catch (  XulException e) {
    log.error("Error loading Xul overlay: version-layout-overlay.xul");
    e.printStackTrace();
  }
}

{
  byte[] retval=new byte[0];
  try {
    int a=is.available();
    while (a > 0) {
      byte[] buffer=new byte[a];
      is.read(buffer);
      byte[] newretval=new byte[retval.length + a];
      for (int i=0; i < retval.length; i++)       newretval[i]=retval[i];
      for (int i=0; i < a; i++)       newretval[retval.length + i]=buffer[i];
      retval=newretval;
      a=is.available();
    }
    return retval;
  }
 catch (  Exception e) {
    System.out.println("Unable to read class from InputStream : " + e.toString());
    return null;
  }
}

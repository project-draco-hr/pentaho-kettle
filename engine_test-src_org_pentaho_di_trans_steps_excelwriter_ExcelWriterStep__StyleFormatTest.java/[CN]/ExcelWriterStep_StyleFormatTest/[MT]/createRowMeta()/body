{
  RowMetaInterface rm=new RowMeta();
  try {
    ValueMetaInterface[] valuesMeta={new ValueMetaInteger("col 1"),new ValueMetaNumber("col 2"),new ValueMetaBigNumber("col 3"),new ValueMetaNumber("col 4")};
    for (int i=0; i < valuesMeta.length; i++) {
      rm.addValueMeta(valuesMeta[i]);
    }
  }
 catch (  Exception ex) {
    return null;
  }
  return rm;
}

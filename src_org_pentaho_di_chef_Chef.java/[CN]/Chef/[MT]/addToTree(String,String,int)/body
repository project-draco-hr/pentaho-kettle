{
  TreeItem ti=null;
  for (int i=1; i < tiSection.length; i++) {
    if (tiSection[i].getText().equalsIgnoreCase(section))     ti=tiSection[i];
  }
  if (ti != null && item != null) {
    TreeItem ni=new TreeItem(ti,SWT.NONE);
    ni.setText(item);
    setTreeImage(ni,section,steptype);
  }
  setShellText();
}

{
  meta=(AccessOutputMeta)smi;
  data=(AccessOutputData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (first && meta.isDoNotOpenNewFileInit()) {
    try {
      if (!OpenFile())       return false;
    }
 catch (    Exception e) {
      logError("An error occurred intialising this step: " + e.getMessage());
      stopAll();
      setErrors(1);
    }
  }
  try {
    writeToTable(r);
    putRow(data.outputRowMeta,r);
    if (checkFeedback(linesOutput))     logBasic("linenr " + linesOutput);
  }
 catch (  KettleException e) {
    logError("Because of an error, this step can't continue: " + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

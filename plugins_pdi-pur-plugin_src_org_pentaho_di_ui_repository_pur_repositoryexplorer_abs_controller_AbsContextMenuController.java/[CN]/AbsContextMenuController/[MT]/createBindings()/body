{
  bf.createBinding(this,"allowed","file-context-rename","!disabled");
  bf.createBinding(this,"allowed","file-context-delete","!disabled");
  bf.createBinding(this,"allowed","folder-context-create","!disabled");
  bf.createBinding(this,"allowed","folder-context-rename","!disabled");

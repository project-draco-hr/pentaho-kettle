{
  int idx=wLoglevel.getSelectionIndex();
  log.setLogLevel(idx);
  String filter=wFilter.getText();
  if (filter != null && filter.length() > 0) {
    log.setFilter(wFilter.getText());
  }
 else {
    log.setFilter(null);
  }
  props.setLogFilter(wFilter.getText());
  props.setLogLevel(wLoglevel.getText());
  props.saveProps();
  dispose();
}

{
  if (ArgList.length == 1) {
    try {
      Display display=Display.getCurrent();
      Shell shell;
      try {
        shell=display.getActiveShell();
      }
 catch (      Exception er) {
        display=new Display();
        shell=new Shell(display);
      }
      MessageBox mb=new MessageBox(shell,SWT.OK);
      String strMessage=Context.toString(ArgList[0]);
      mb.setMessage(strMessage);
      mb.setText("Alert");
      mb.open();
    }
 catch (    Exception e) {
    }
  }
  return "";
}

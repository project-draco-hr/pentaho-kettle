{
  String sArg1="";
  String sArg2="";
  if (ArgList.length == 2) {
    try {
      sArg1=Context.toString(ArgList[0]);
      sArg2=Context.toString(ArgList[1]);
      System.setProperty(sArg1,sArg2);
    }
 catch (    Exception e) {
      throw Context.reportRuntimeError(e.toString());
    }
  }
 else {
    throw Context.reportRuntimeError("The function call setEnvironmentVar is not valid");
  }
}

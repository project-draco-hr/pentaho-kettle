{
  meta=(DummyTransMeta)smi;
  data=(DummyTransData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  putRow(getInputRowMeta(),r);
  if (checkFeedback(linesRead)) {
    if (log.isBasic())     logBasic(Messages.getString("DummyTrans.Log.LineNumber") + linesRead);
  }
  return true;
}

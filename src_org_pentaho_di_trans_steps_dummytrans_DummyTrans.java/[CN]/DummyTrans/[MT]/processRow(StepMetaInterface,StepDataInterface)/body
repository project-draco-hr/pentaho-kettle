{
  meta=(DummyTransMeta)smi;
  data=(DummyTransData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  putRow(getInputRowMeta(),r);
  if (checkFeedback(getLinesRead())) {
    if (log.isBasic())     logBasic(BaseMessages.getString(PKG,"DummyTrans.Log.LineNumber") + getLinesRead());
  }
  return true;
}

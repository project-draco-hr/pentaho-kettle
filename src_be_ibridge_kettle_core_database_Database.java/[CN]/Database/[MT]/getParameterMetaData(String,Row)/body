{
  int q=countParameters(sql);
  Row par=new Row();
  if (inform != null && q == inform.size()) {
    for (int i=0; i < q; i++) {
      Value inf=inform.getValue(i);
      Value v=new Value(inf);
      par.addValue(v);
    }
  }
 else {
    for (int i=0; i < q; i++) {
      Value v=new Value("name" + i,Value.VALUE_TYPE_NUMBER);
      v.setValue(0.0);
      par.addValue(v);
    }
  }
  return par;
}

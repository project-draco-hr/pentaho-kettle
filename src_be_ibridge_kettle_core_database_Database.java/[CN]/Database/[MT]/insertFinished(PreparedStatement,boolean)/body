{
  try {
    if (ps != null) {
      if (!isAutoCommit()) {
        if (batch && getDatabaseMetaData().supportsBatchUpdates() && batchCounter > 0) {
          ps.executeBatch();
          commit();
        }
 else {
          commit();
        }
      }
      ps.close();
    }
  }
 catch (  SQLException ex) {
    throw new KettleDatabaseException("Unable to commit connection after having inserted rows.",ex);
  }
}

{
  String debug="start";
  Row ret;
  try {
    debug="pstmt.executeQuery()";
    ResultSet res=ps.executeQuery();
    debug="res.getMetaData";
    rsmd=res.getMetaData();
    debug="getRowInfo()";
    rowinfo=getRowInfo();
    debug="getRow(res)";
    ret=getRow(res);
    debug="res.close()";
    res.close();
    return ret;
  }
 catch (  SQLException ex) {
    throw new KettleDatabaseException("Error looking up row in database (" + debug + ")",ex);
  }
}

{
  try {
    if (getDatabaseMetaData().supportsTransactions()) {
      connection.commit();
    }
 else {
      System.out.println("No commit possible on database connection [" + toString() + "]");
    }
  }
 catch (  Exception e) {
    if (databaseMeta.supportsEmptyTransactions())     throw new KettleDatabaseException("Error comitting connection",e);
  }
}

{
  String ins="";
  ins+="INSERT INTO " + tableName + "(";
  for (int i=0; i < fields.size(); i++) {
    if (i > 0)     ins+=", ";
    String name=fields.getValue(i).getName();
    ins+=databaseMeta.quoteField(name);
  }
  ins+=") VALUES (";
  for (int i=0; i < fields.size(); i++) {
    if (i > 0)     ins+=", ";
    ins+=" ?";
  }
  ins+=")";
  return ins;
}

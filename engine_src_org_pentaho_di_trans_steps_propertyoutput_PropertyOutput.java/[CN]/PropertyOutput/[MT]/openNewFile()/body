{
  try {
    data.pro=new Properties();
    data.KeySet.clear();
    data.file=KettleVFS.getFileObject(data.filename,getTransMeta());
    if (meta.isAppend() && data.file.exists())     data.pro.load(KettleVFS.getInputStream(data.file));
    createParentFolder();
    saveProcessingFile();
  }
 catch (  Exception e) {
    throw new KettleException("Error opening file [" + data.filename + "]!",e);
  }
}

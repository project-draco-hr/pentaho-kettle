{
  return connectionDelegate.getIDs("SELECT " + quote(KettleDatabaseRepository.FIELD_JOBENTRY_COPY_ID_JOBENTRY_COPY) + " FROM "+ quoteTable(KettleDatabaseRepository.TABLE_R_JOBENTRY_COPY)+ " WHERE "+ quote(KettleDatabaseRepository.FIELD_JOBENTRY_COPY_ID_JOB)+ " = ? AND "+ quote(KettleDatabaseRepository.FIELD_JOBENTRY_COPY_ID_JOBENTRY)+ " = ? ",id_job,id_jobentry);

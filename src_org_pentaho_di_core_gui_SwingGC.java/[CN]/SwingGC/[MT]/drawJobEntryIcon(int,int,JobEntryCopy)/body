{
  if (jobEntryCopy == null)   return;
  Image image=null;
  if (jobEntryCopy.isSpecial()) {
    if (jobEntryCopy.isStart()) {
      image=imageStart;
    }
    if (jobEntryCopy.isDummy()) {
      image=imageDummy;
    }
  }
 else {
    String configId=jobEntryCopy.getEntry().getConfigId();
    if (configId != null) {
      image=entryImages.get(configId);
    }
  }
  if (image == null) {
    return;
  }
  gc.drawImage(image,x + xOffset,y + yOffset,observer);
}

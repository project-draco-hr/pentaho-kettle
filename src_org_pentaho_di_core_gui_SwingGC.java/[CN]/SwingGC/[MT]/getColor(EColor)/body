{
switch (color) {
case BACKGROUND:
    return background;
case BLACK:
  return black;
case RED:
return red;
case YELLOW:
return yellow;
case ORANGE:
return orange;
case GREEN:
return green;
case BLUE:
return blue;
case MAGENTA:
return magenta;
case GRAY:
return gray;
case LIGHTGRAY:
return lightGray;
case DARKGRAY:
return darkGray;
}
return null;
}

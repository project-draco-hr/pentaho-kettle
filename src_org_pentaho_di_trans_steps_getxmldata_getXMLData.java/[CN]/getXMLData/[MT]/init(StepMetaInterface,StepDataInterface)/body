{
  meta=(getXMLDataMeta)smi;
  data=(getXMLDataData)sdi;
  if (super.init(smi,sdi)) {
    if (!meta.getIsInFields()) {
      data.files=meta.getFiles(this).getFiles();
      if (data.files == null) {
        logError(Messages.getString("getXMLData.Log.NoFiles"));
        return false;
      }
      data.rownr=1L;
    }
    return true;
  }
  return false;
}

{
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (log.isRowLevel())   logRowlevel(Messages.getString("getXMLData.Log.ReadRow",r.toString()));
  linesInput++;
  data.rownr++;
  putRow(data.outputRowMeta,r);
  if (meta.getRowLimit() > 0 && data.rownr > meta.getRowLimit()) {
    setOutputDone();
    return false;
  }
  return true;
}

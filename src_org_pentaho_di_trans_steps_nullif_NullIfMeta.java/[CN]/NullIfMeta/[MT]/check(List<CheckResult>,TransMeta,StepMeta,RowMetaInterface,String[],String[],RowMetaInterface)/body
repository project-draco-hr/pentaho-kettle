{
  CheckResult cr;
  if (prev == null || prev.size() == 0) {
    cr=new CheckResult(CheckResult.TYPE_RESULT_WARNING,Messages.getString("NullIfMeta.CheckResult.NoReceivingFieldsError"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("NullIfMeta.CheckResult.StepReceivingFieldsOK",prev.size() + ""),stepinfo);
    remarks.add(cr);
  }
  if (input.length > 0) {
    cr=new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("NullIfMeta.CheckResult.StepRecevingInfoFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("NullIfMeta.CheckResult.NoInputReceivedError"),stepinfo);
    remarks.add(cr);
  }
}

{
  CheckResult cr;
  if (prev == null || prev.size() == 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_WARNING,BaseMessages.getString(PKG,"NullIfMeta.CheckResult.NoReceivingFieldsError"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,BaseMessages.getString(PKG,"NullIfMeta.CheckResult.StepReceivingFieldsOK",prev.size() + ""),stepinfo);
    remarks.add(cr);
  }
  if (input.length > 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,BaseMessages.getString(PKG,"NullIfMeta.CheckResult.StepRecevingInfoFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"NullIfMeta.CheckResult.NoInputReceivedError"),stepinfo);
    remarks.add(cr);
  }
}

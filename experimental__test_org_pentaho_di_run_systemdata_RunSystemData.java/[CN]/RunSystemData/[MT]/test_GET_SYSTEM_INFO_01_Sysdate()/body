{
  TimedTransRunner timedTransRunner=new TimedTransRunner("experimental_test/org/pentaho/di/run/systemdata/GetSystemInfoSysdate.ktr",LogWriter.LOG_LEVEL_ERROR,AllRunTests.getOldTargetDatabase(),AllRunTests.getNewTargetDatabase(),1000000);
  timedTransRunner.runOldAndNew();
  Result oldResult=timedTransRunner.getOldResult();
  assertTrue(oldResult.getNrErrors() == 0);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 0);
}

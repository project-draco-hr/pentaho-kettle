{
  meta=(DeleteMeta)smi;
  data=(DeleteData)sdi;
  try {
    if (!data.db.isAutoCommit()) {
      if (getErrors() == 0) {
        data.db.commit();
      }
 else {
        data.db.rollback();
      }
    }
    data.db.closeUpdate();
  }
 catch (  KettleDatabaseException e) {
    log.logError(toString(),BaseMessages.getString(PKG,"Delete.Log.UnableToCommitUpdateConnection") + data.db + "] :"+ e.toString());
    setErrors(1);
  }
 finally {
    if (data.db != null) {
      data.db.disconnect();
    }
  }
  super.dispose(smi,sdi);
}

{
  StringBuffer buffer=new StringBuffer();
  try {
    InputStream is=UserDefinedJavaClassHelp.class.getResourceAsStream(strFileName);
    int c;
    while ((c=is.read()) != -1)     buffer.append((char)c);
    is.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return buffer.toString();
}

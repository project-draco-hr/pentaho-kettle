{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMeta("int_in",ValueMeta.TYPE_INTEGER),new ValueMeta("number_in",ValueMeta.TYPE_NUMBER),new ValueMeta("string_in",ValueMeta.TYPE_STRING)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

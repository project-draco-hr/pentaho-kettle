{
  try {
    for (int i=0; i < size; i++) {
      addValue(new Value(meta.getValue(i),dis));
    }
  }
 catch (  KettleEOFException e) {
    throw new KettleEOFException(Messages.getString("Row.EndOfFileReadingRow"),e);
  }
catch (  Exception e) {
    throw new KettleFileException(Messages.getString("Row.RowError"),e);
  }
}

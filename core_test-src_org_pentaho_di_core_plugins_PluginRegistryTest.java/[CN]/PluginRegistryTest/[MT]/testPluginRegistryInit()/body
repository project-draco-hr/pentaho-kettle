{
  PluginTypeInterface[] plugins=new PluginTypeInterface[]{StepPluginType.getInstance(),PartitionerPluginType.getInstance(),JobEntryPluginType.getInstance(),RepositoryPluginType.getInstance(),DatabasePluginType.getInstance()};
  for (  PluginTypeInterface pl : plugins) {
    PluginRegistry.addPluginType(pl);
  }
  PluginRegistry.init();
}

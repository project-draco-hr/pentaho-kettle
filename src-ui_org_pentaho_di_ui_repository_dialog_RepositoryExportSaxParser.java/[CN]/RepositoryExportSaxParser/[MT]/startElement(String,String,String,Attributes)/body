{
  if (STRING_REPOSITORY.equals(qName) || STRING_TRANSFORMATIONS.equals(qName) || STRING_JOBS.equals(qName)) {
    add=false;
  }
 else {
    add=true;
  }
  if (add) {
    if (STRING_TRANSFORMATION.equals(qName) || STRING_JOB.equals(qName)) {
      xml.setLength(0);
    }
    xml.append(XMLHandler.openTag(qName));
  }
}

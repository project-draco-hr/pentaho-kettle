{
  String url=transMeta.environmentSubstitute(wURL.getText());
  SalesforceConnection connection=new SalesforceConnection(log,url,transMeta.environmentSubstitute(wUserName.getText()),transMeta.environmentSubstitute(wPassword.getText()));
  int realTimeOut=Const.toInt(transMeta.environmentSubstitute(wTimeOut.getText()),0);
  connection.setTimeOut(realTimeOut);
  connection.connect();
  return connection;
}

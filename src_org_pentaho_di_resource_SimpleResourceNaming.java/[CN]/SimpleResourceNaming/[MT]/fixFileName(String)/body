{
  StringBuffer buff=new StringBuffer(name.length());
  char ch;
  for (int i=0; i < name.length(); i++) {
    ch=name.charAt(i);
    if ((ch <= '/') || (ch >= ':' && ch <= '?') || (ch >= '[' && ch <= '`')|| (ch >= '{' && ch <= '~')) {
      buff.append('_');
    }
 else {
      buff.append(ch);
    }
  }
  return buff.toString();
}

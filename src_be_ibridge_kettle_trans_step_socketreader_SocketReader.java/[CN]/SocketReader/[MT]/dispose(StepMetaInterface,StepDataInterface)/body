{
  try {
    Thread.sleep(5000);
  }
 catch (  InterruptedException e) {
  }
  logBasic("Closing input stream.");
  try {
    data.inputStream.close();
  }
 catch (  IOException e) {
  }
  logBasic("Closing output stream.");
  try {
    data.outputStream.close();
  }
 catch (  IOException e) {
  }
  logBasic("Closing socket.");
  try {
    data.socket.close();
  }
 catch (  IOException e) {
  }
}

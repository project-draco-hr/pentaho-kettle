{
  byte[] result=new byte[field.length - nrEnclosuresFound];
  int resultIndex=0;
  for (int i=0; i < field.length; i++) {
    if (field[i] == enclosure[0]) {
      if (i + 1 < field.length && field[i + 1] == enclosure[0]) {
      }
 else {
        result[resultIndex++]=field[i];
      }
    }
 else {
      result[resultIndex++]=field[i];
    }
  }
  return result;
}

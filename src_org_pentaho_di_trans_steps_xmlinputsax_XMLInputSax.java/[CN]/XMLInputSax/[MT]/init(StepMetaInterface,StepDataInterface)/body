{
  meta=(XMLInputSaxMeta)smi;
  data=(XMLInputSaxData)sdi;
  if (super.init(smi,sdi)) {
    data.files=meta.getFilePaths(getTransMeta());
    if (data.files == null || data.files.length == 0) {
      logError("No file(s) specified! Stop processing.");
      return false;
    }
    data.rownr=1L;
    return true;
  }
  return false;
}

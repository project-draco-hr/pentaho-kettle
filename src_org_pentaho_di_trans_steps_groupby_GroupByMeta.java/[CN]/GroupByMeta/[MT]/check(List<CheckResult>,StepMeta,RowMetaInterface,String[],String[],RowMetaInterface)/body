{
  CheckResult cr;
  if (input.length > 0) {
    cr=new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("GroupByMeta.CheckResult.ReceivingInfoOK"),stepMeta);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("GroupByMeta.CheckResult.NoInputError"),stepMeta);
    remarks.add(cr);
  }
}

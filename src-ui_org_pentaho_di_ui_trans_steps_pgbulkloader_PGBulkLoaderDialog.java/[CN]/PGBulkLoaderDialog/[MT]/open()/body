    public void modifyText(    ModifyEvent e){
      input.setChanged();
    }
  }
;
  FocusListener lsFocusLost=new FocusAdapter(){
    public void focusLost(    FocusEvent arg0){
      setTableFieldCombo();
    }
  }
;
  changed=input.hasChanged();
  FormLayout formLayout=new FormLayout();

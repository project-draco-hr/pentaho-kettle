{
  testSplit("SELECT 1;SELECT 2","SELECT 1","SELECT 2");
  testSplit("SELECT '1;2'","SELECT '1;2'");
  testSplit("SELECT \"1;2\"","SELECT \"1;2\"");
  testSplit("SELECT -- 1;2","SELECT -- 1;2");
  testSplit("SELECT /*1;2*/","SELECT /*1;2*/");
  testSplit("SELECT /1;2","SELECT /1","2");
  testSplit("SELECT /1;;;;2","SELECT /1","2");
  testSplit("SELECT /1;\n  \n","SELECT /1");
}

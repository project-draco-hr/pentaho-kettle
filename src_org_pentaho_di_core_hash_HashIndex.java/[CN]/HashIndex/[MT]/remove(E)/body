{
  int hash=entry.hashCode();
  hash^=(hash >>> 20) ^ (hash >>> 12);
  hash=hash ^ (hash >>> 7) ^ (hash >>> 4);
  int i=hash & (table.length - 1);
  Entry prev=table[i], e=prev;
  while (e != null) {
    Entry next=e.next;
    if (e.entry == entry) {
      size--;
      if (prev == e)       table[i]=next;
 else       prev.next=next;
      return;
    }
    prev=e;
    e=next;
  }
}

{
  if (rowList == null) {
    wText.setText("");
    return;
  }
  int nr=wFields.table.getSelectionIndex();
  if (nr >= 0 && rowList != null && nr < rowList.size()) {
    RowMetaAndData row=(RowMetaAndData)rowList.get(nr);
    try {
      wText.setText(row.getString("LOG_FIELD",""));
    }
 catch (    KettleValueException e) {
    }
  }
}

{
  try {
    for (String line=in.readLine(); line != null; line=in.readLine()) {
      if (logWarn) {
        log.logError("DC: " + line);
      }
 else {
        log.logBasic("DC: " + line);
      }
    }
  }
 catch (  IOException e) {
    log.logError("DC: Unexpected IO exception",e);
  }
 finally {
    try {
      in.close();
    }
 catch (    IOException e) {
    }
  }
}

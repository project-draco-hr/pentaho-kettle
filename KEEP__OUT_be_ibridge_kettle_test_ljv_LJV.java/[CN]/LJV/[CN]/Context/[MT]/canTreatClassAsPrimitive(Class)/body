{
  if (cz == null || cz.isPrimitive())   return true;
  if (cz.isArray())   return false;
  do {
    if (pretendPrimitiveSet.contains(cz) || pretendPrimitiveSet.contains(cz.getPackage()))     return true;
    if (cz == Object.class)     return false;
    Class[] ifs=cz.getInterfaces();
    for (int i=0; i < ifs.length; i++)     if (canTreatClassAsPrimitive(ifs[i]))     return true;
    cz=cz.getSuperclass();
  }
 while (cz != null);
  return false;
}

{
  try {
    rep.saveJobEntryAttribute(id_job,getID(),"filename",filename);
    if (arguments != null) {
      for (int i=0; i < arguments.length; i++) {
        rep.saveJobEntryAttribute(id_job,getID(),i,"name",arguments[i]);
      }
    }
  }
 catch (  KettleDatabaseException dbe) {
    throw new KettleException(BaseMessages.getString(PKG,"JobEntryFilesExist.ERROR_0003_Cannot_Save_Job_Entry","" + id_job,dbe.getMessage()));
  }
}

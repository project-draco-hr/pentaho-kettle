{
  boolean retval=false;
  if (data.OpenedNewFile) {
    try {
      data.writer.writeEndElement();
      data.writer.writeCharacters(Const.CR);
      data.writer.writeEndDocument();
      data.writer.close();
      if (meta.isZipped()) {
        data.zip.closeEntry();
        data.zip.finish();
        data.zip.close();
      }
      retval=true;
    }
 catch (    Exception e) {
    }
  }
  return retval;
}

{
  int index=jobMap.indexOf(jobMeta);
  if (index < 0) {
    jobMap.add(jobMeta);
    return true;
  }
 else {
    ShowMessageDialog dialog=new ShowMessageDialog(spoon.getShell(),SWT.OK | SWT.ICON_INFORMATION,BaseMessages.getString(PKG,"Spoon.Dialog.JobAlreadyLoaded.Title"),"'" + jobMeta.toString() + "'"+ Const.CR+ Const.CR+ BaseMessages.getString(PKG,"Spoon.Dialog.JobAlreadyLoaded.Message"));
    dialog.setTimeOut(6);
    dialog.open();
    return false;
  }
}

{
  logBasic(Messages.getString("DatabaseLookup.Log.StartingToRun"));
  try {
    logBasic(Messages.getString("DatabaseLookup.Log.ConnectedToDatabase2"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Throwable t) {
    logError(Messages.getString("DatabaseLookup.ERROR003.UnexpectedErrorDuringProcessing2") + t.getMessage());
    logError(Const.getStackTracker(t));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

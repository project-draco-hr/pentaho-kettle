{
  CheckResult cr;
  if (prev == null || prev.size() == 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_WARNING,BaseMessages.getString(PKG,"MappingMeta.CheckResult.NotReceivingAnyFields"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,BaseMessages.getString(PKG,"MappingMeta.CheckResult.StepReceivingFields",prev.size() + ""),stepinfo);
    remarks.add(cr);
  }
  if (input.length > 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,BaseMessages.getString(PKG,"MappingMeta.CheckResult.StepReceivingFieldsFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"MappingMeta.CheckResult.NoInputReceived"),stepinfo);
    remarks.add(cr);
  }
}

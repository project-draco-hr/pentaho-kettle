{
  this();
  boolean found=false;
  Enumeration appenders=rootLogger.getAllAppenders();
  while (appenders.hasMoreElements()) {
    if (appenders.nextElement() instanceof ConsoleAppender)     found=true;
  }
  if (!found) {
    rootLogger.addAppender(consoleAppender);
  }
  level=lvl;
  filter=null;
}

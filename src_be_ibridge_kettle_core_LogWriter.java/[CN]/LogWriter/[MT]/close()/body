{
  boolean retval=true;
  try {
    Logger logger=Logger.getRootLogger();
    Enumeration loggers=logger.getAllAppenders();
    while (loggers.hasMoreElements()) {
      Appender appender=(Appender)loggers.nextElement();
      appender.close();
    }
    rootLogger.removeAllAppenders();
    logWriter=null;
  }
 catch (  Exception e) {
    retval=false;
  }
  return retval;
}

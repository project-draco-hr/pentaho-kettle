{
  List params=getParameter(p,requestPart);
  for (Iterator itr=params.iterator(); itr.hasNext(); ) {
    WsdlOpParameter op=(WsdlOpParameter)itr.next();
    if (_headerNames.contains(op.getName().getLocalPart())) {
      op.setHeader();
    }
    if (requestPart) {
      op.setMode(WsdlOpParameter.ParameterMode.IN);
      add(op);
    }
 else {
      addOutputParameter(op);
    }
  }
  return true;
}

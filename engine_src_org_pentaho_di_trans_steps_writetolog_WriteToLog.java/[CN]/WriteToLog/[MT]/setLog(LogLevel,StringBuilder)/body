{
switch (loglevel) {
case ERROR:
    logError(msg.toString());
  break;
case MINIMAL:
logMinimal(msg.toString());
break;
case BASIC:
logBasic(msg.toString());
break;
case DETAILED:
logDetailed(msg.toString());
break;
case DEBUG:
logDebug(msg.toString());
break;
case ROWLEVEL:
logRowlevel(msg.toString());
break;
case NOTHING:
break;
default :
break;
}
}

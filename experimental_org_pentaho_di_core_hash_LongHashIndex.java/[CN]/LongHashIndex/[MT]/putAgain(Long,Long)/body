{
  int hash=generateHashCode(key);
  int i=hash & (table.length - 1);
  for (Entry e=table[i]; e != null; e=e.next) {
    if (e.hash == hash && e.key.longValue() == key.longValue()) {
      Long old=e.value;
      e.value=value;
      return old;
    }
  }
  table[i]=new Entry(hash,key,value,table[i]);
  resize();
  return null;
}

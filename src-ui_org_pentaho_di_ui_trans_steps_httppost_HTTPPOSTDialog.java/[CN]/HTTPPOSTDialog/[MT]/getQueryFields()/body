{
  try {
    RowMetaInterface r=transMeta.getPrevStepFields(stepname);
    if (r != null && !r.isEmpty()) {
      BaseStepDialog.getFieldsFromPrevious(r,wQuery,1,new int[]{1,2},new int[]{3},-1,-1,null);
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(shell,Messages.getString("HTTPPOSTDialog.FailedToGetFields.DialogTitle"),Messages.getString("HTTPPOSTDialog.FailedToGetFields.DialogMessage"),ke);
  }
}

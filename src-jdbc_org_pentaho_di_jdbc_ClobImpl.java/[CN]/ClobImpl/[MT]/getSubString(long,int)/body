{
  if (length == 0) {
    return EMPTY_CLOB;
  }
  try {
    byte data[]=blobBuffer.getBytes((pos - 1) * 2 + 1,length * 2);
    return new String(data,"UTF-16LE");
  }
 catch (  IOException e) {
    throw new SQLException(BaseMessages.getString(PKG,"error.generic.ioerror",e.getMessage()),"HY000");
  }
}

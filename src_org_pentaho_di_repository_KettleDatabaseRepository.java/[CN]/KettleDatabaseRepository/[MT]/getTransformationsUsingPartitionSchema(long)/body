{
  String sql="SELECT DISTINCT " + quote(KettleDatabaseRepository.FIELD_TRANS_PARTITION_SCHEMA_ID_TRANSFORMATION) + " FROM "+ quoteTable(KettleDatabaseRepository.TABLE_R_TRANS_PARTITION_SCHEMA)+ " WHERE "+ quote(KettleDatabaseRepository.FIELD_TRANS_PARTITION_SCHEMA_ID_PARTITION_SCHEMA)+ " = "+ id_partition_schema;
  return transDelegate.getTransformationsWithIDList(connectionDelegate.getRows(sql,100),connectionDelegate.getReturnRowMeta());
}

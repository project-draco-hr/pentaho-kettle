{
  bindingFactory.setBindingType(Type.ONE_WAY);
  bindingFactory.createBinding(browseController,"repositoryObjects","lock-menu","!disabled",forButtons);
  bindingFactory.createBinding(browseController,"repositoryObjects","file-context-lock","selected",checkLockedState);
  bindingFactory.createBinding(browseController,"repositoryObjects",this,"menuItemEnabledState");
  bindingFactory.createBinding(browseController,"repositoryObjects","file-context-locknotes","!disabled",checkLockedStateBool);
  bindingFactory.createBinding(browseController,"repositoryObjects","lock-context-lock","selected",checkLockedState);
  bindingFactory.createBinding(browseController,"repositoryObjects","lock-context-lock","!disabled",checkLockPermissions);
  bindingFactory.createBinding(browseController,"repositoryObjects","lock-context-locknotes","!disabled",checkLockedStateBool);
}

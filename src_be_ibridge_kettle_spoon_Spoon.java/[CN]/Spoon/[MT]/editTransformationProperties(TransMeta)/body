{
  TransDialog tid=new TransDialog(shell,SWT.NONE,transMeta,rep);
  TransMeta ti=tid.open();
  if (tid.isSharedObjectsFileChanged()) {
    loadTransSharedObjects(transMeta);
  }
  if (tid.isSharedObjectsFileChanged() || ti != null) {
    refreshTree();
    renameTabs();
  }
  setShellText();
  return ti != null;
}

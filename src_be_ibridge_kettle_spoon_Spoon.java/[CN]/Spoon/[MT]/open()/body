{
  shell.open();
  loadRepositoryObjects();
  loadSharedObjects();
  refreshPluginHistory();
  if (transMeta.nrSteps() > 0 || transMeta.nrDatabases() > 0 || transMeta.nrTransHops() > 0) {
    refreshTree(true);
  }
  transMeta.clearChanged();
  setShellText();
  if (props.showTips()) {
    TipsDialog tip=new TipsDialog(shell,props);
    tip.open();
  }
}

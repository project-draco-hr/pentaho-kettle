{
  if (jobMeta == null)   return false;
  JobDialog jd=new JobDialog(shell,SWT.NONE,jobMeta,rep);
  JobMeta ji=jd.open();
  if (jd.isSharedObjectsFileChanged()) {
    loadJobSharedObjects(jobMeta);
  }
  if (jd.isSharedObjectsFileChanged() || ji != null) {
    refreshTree();
    renameTabs();
  }
  setShellText();
  return ji != null;
}

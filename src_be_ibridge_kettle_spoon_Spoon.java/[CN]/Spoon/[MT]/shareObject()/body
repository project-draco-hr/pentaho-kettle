{
  Object[] objects=getTreeObjects();
  if (objects != null) {
    for (int i=0; i < objects.length; i++) {
      if (objects[i] instanceof SharedObjectInterface) {
        SharedObjectInterface sharedObject=(SharedObjectInterface)objects[i];
        sharedObject.setShared(true);
      }
    }
  }
  refreshTree(true);
}

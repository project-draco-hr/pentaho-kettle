{
  log.logDetailed(toString(),Messages.getString("Log.SaveToFileOrRepository"));
  if (rep != null) {
    saveRepository();
  }
 else {
    if (transMeta.getFilename() != null) {
      save(transMeta.getFilename());
    }
 else {
      saveFileAs();
    }
  }
  try {
    transMeta.getDbCache().saveCache(log);
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,props,Messages.getString("Message.ErrorDialog.Title.ERROR"),Messages.getString("Message.ErrorDialog.Info.ErrorSavingDatabaseCache"),e);
  }
}

{
  String key=makeJobGraphTabName(jobMeta);
  if (jobMap.get(key) == null) {
    jobMap.put(key,jobMeta);
  }
 else {
    MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_INFORMATION);
    mb.setMessage("'" + key + "'"+ Const.CR+ Const.CR+ Messages.getString("Spoon.Dialog.JobAlreadyLoaded.Message"));
    mb.setText(Messages.getString("Spoon.Dialog.JobAlreadyLoaded.Title"));
    mb.open();
  }
  return key;
}

{
  boolean answer=true;
  if (transMeta.hasChanged()) {
    MessageBox mb=new MessageBox(shell,SWT.YES | SWT.NO | SWT.CANCEL| SWT.ICON_WARNING);
    mb.setMessage("This model has changed.  Do you want to save it?");
    mb.setText("Warning!");
    int reply=mb.open();
    if (reply == SWT.YES) {
      saveFile();
      answer=true;
    }
 else {
      if (reply == SWT.CANCEL) {
        answer=false;
      }
 else {
        answer=true;
      }
    }
  }
  return answer;
}

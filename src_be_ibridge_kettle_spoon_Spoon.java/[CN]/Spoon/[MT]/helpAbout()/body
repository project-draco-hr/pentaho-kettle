{
  MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_INFORMATION | SWT.CENTER);
  String mess=Messages.getString("System.ProductInfo") + Const.VERSION + Const.CR+ Const.CR+ Const.CR;
  mess+=Messages.getString("System.CompanyInfo") + Const.CR;
  mess+="         " + Messages.getString("System.ProductWebsiteUrl") + Const.CR;
  mess+="         " + BuildVersion.getInstance().getVersion() + Const.CR;
  mb.setMessage(mess);
  mb.setText(APP_NAME);
  mb.open();
}

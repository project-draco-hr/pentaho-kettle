{
  List<T> list=new ArrayList<T>();
  for (  Class<? extends PluginTypeInterface> pi : pluginMap.keySet()) {
    if (Const.classIsOrExtends(pi,type)) {
      List<PluginInterface> mapList=pluginMap.get(pi);
      if (mapList != null) {
        for (        PluginInterface p : mapList) {
          T t=(T)p;
          if (!list.contains(t)) {
            list.add((T)p);
          }
        }
      }
    }
  }
  return list;
}

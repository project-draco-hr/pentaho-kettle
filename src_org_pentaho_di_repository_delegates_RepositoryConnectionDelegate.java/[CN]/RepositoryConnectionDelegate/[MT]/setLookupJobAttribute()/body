{
  String sql="SELECT " + quote(Repository.FIELD_JOB_ATTRIBUTE_VALUE_STR) + ", "+ quote(Repository.FIELD_JOB_ATTRIBUTE_VALUE_NUM)+ " FROM "+ databaseMeta.getQuotedSchemaTableCombination(null,Repository.TABLE_R_JOB_ATTRIBUTE)+ " WHERE "+ quote(Repository.FIELD_JOB_ATTRIBUTE_ID_JOB)+ " = ?  AND "+ quote(Repository.FIELD_JOB_ATTRIBUTE_CODE)+ " = ? AND "+ Repository.FIELD_JOB_ATTRIBUTE_NR+ " = ? ";
  psJobAttributesLookup=database.prepareSQL(sql);

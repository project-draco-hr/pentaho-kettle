{
  Collection<SAPFunction> sfc=new Vector<SAPFunction>();
  for (  SAPFunction sapFunction : this.sfc) {
    try {
      Thread.sleep(250);
    }
 catch (    InterruptedException e) {
    }
    ;
    if (sapFunction.getName().contains(query)) {
      sfc.add(sapFunction);
    }
  }
  return sfc;
}

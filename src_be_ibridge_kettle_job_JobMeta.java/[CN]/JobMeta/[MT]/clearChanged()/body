{
  changed_entries=false;
  changed_hops=false;
  changed_notes=false;
  for (int i=0; i < nrJobEntries(); i++) {
    JobEntryCopy entry=getJobEntry(i);
    entry.setChanged(false);
  }
  for (int i=0; i < nrJobHops(); i++) {
    JobHopMeta hop=getJobHop(i);
    hop.setChanged(false);
  }
  changed=false;
}

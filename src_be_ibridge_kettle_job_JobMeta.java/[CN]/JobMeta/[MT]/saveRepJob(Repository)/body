{
  try {
    rep.insertJob(getID(),directory.getID(),getName(),logconnection == null ? -1 : logconnection.getID(),logTable,modified_user,modified_date,useBatchId,batchIdPassed,logfieldUsed);
  }
 catch (  KettleDatabaseException dbe) {
    throw new KettleException("Unable to save job info to repository",dbe);
  }
}

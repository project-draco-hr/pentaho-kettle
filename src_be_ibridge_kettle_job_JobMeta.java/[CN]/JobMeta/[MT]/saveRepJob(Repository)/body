{
  try {
    rep.insertJob(getID(),directory.getID(),getName(),logconnection == null ? -1 : logconnection.getID(),logTable,modifiedUser,modifiedDate,useBatchId,batchIdPassed,logfieldUsed,sharedObjectsFile,description,extended_description,job_version,job_status,created_user,created_date);
  }
 catch (  KettleDatabaseException dbe) {
    throw new KettleException(Messages.getString("JobMeta.Exception.UnableToSaveJobToRepository"),dbe);
  }
}

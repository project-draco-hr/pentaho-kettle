{
  if (fileLoggingEventListener != null) {
    try {
      fileLoggingEventListener.close();
    }
 catch (    Exception e) {
      e.printStackTrace(System.err);
      status=1;
    }
    CentralLogStore.getAppender().removeLoggingEventListener(fileLoggingEventListener);
  }
  System.exit(status);
}

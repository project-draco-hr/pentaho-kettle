{
  TimedTransRunner timedTransRunner=new TimedTransRunner("experimental_test/org/pentaho/di/run/update/UpdateRowsSimple.ktr",LogWriter.LOG_LEVEL_ERROR,AllRunTests.getOldTargetDatabase(),AllRunTests.getNewTargetDatabase(),100000);
  timedTransRunner.runOldAndNew();
  be.ibridge.kettle.core.Result oldResult=timedTransRunner.getOldResult();
  assertTrue(oldResult.getNrErrors() == 0);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 0);
}

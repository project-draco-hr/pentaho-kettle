{
  setFilename(filename);
  setLocked(false);
  FileObject file=null;
  FileObject dummyfile=null;
  try {
    file=KettleVFS.getFileObject(filename);
    if (file.exists()) {
      dummyfile=KettleVFS.getFileObject(filename);
      file.moveTo(dummyfile);
    }
  }
 catch (  Exception e) {
    setLocked(true);
  }
 finally {
    if (file != null)     try {
      file.close();
    }
 catch (    Exception e) {
    }
    ;
    if (dummyfile != null)     try {
      file.close();
    }
 catch (    Exception e) {
    }
    ;
  }
}

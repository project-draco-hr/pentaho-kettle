{
  try {
    logBasic("Starting to run...");
    while (processRow(meta,data) && !isStopped())     ;
    if (data.trans != null) {
      data.trans.waitUntilFinished();
      data.trans.endProcessing("end");
    }
  }
 catch (  Exception e) {
    logError("Unexpected error in '" + debug + "' : "+ e.toString());
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

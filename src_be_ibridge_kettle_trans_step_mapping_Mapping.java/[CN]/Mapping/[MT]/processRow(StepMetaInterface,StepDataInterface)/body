{
  meta=(MappingMeta)smi;
  data=(MappingData)sdi;
  Row r=getRow();
  if (r == null) {
    data.mappingInput.setOutputDone();
    data.mappingInput.setFinished();
    return false;
  }
  data.mappingInput.putRow(r);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic("Linenr " + linesRead);
  return true;
}

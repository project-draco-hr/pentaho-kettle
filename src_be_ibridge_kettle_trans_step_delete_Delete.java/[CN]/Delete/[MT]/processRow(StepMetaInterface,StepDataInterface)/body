{
  meta=(DeleteMeta)smi;
  data=(DeleteData)sdi;
  boolean sendToErrorRow=false;
  String errorMessage=null;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  try {
    deleteValues(r);
    putRow(r);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("Delete.Log.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    if (getStepMeta().isDoingErrorHandling()) {
      sendToErrorRow=true;
      errorMessage=e.toString();
    }
 else {
      logError(Messages.getString("Delete.Log.ErrorInStep") + e.getMessage());
      setErrors(1);
      stopAll();
      setOutputDone();
      return false;
    }
    if (sendToErrorRow) {
      putError(r,1,errorMessage,null,"TOP001");
      r.setIgnore();
    }
  }
  return true;
}

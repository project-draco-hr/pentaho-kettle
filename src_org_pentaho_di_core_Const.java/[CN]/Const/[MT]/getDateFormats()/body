{
  if (dateFormats == null) {
    int dateFormatsCount=toInt(Messages.getString("Const.DateFormat.Count"),0);
    dateFormats=new String[dateFormatsCount];
    for (int i=1; i <= dateFormatsCount; i++)     dateFormats[i - 1]=Messages.getString("Const.DateFormat" + Integer.toString(i));
  }
  return dateFormats;
}

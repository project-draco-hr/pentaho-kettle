{
  int i, nrfields=0;
  allocate(nrfields);
  for (i=0; i < nrfields; i++) {
    fieldName[i]="field" + i;
    fieldType[i]="Number";
    fieldFormat[i]="?0,000,000.00;?-0,000,000.00";
    fieldLength[i]=9;
    fieldPrecision[i]=2;
    currency[i]="?";
    decimal[i]=",";
    group[i]=".";
    value[i]="-";
  }
  rowLimit=10;
}

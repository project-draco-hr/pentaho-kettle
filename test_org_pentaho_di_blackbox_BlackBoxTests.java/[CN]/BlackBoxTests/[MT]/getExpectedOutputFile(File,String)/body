{
  File expected;
  expected=new File(dir,baseName + ".expected.txt");
  if (expected.exists()) {
    return expected;
  }
  expected=new File(dir,baseName + ".expected.csv");
  if (expected.exists()) {
    return expected;
  }
  expected=new File(dir,baseName + ".expected.xml");
  if (expected.exists()) {
    return expected;
  }
  return null;
}

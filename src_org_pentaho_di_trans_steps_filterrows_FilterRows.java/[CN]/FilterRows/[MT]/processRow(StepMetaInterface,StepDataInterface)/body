{
  meta=(FilterRowsMeta)smi;
  data=(FilterRowsData)sdi;
  boolean keep;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (first) {
    data.outputRowMeta=(RowMetaInterface)getInputRowMeta().clone();
    meta.getFields(getInputRowMeta(),getStepname(),null,null,this);
  }
  keep=keepRow(getInputRowMeta(),r);
  if (!meta.chosesTargetSteps()) {
    if (keep) {
      putRow(data.outputRowMeta,r);
    }
  }
 else {
    if (keep) {
      putRowTo(data.outputRowMeta,r,meta.getSendTrueStepname());
    }
 else {
      putRowTo(data.outputRowMeta,r,meta.getSendFalseStepname());
    }
  }
  if (checkFeedback(linesRead))   logBasic(Messages.getString("FilterRows.Log.LineNumber") + linesRead);
  return true;
}

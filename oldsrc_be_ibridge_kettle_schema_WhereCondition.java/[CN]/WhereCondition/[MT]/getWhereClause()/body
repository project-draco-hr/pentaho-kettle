{
  String retval="";
  if (field != null) {
    if (getComparator() != null && getComparator().length() > 0) {
      retval=field.getAliasField() + " " + getComparator()+ " "+ getCode();
    }
 else {
      retval=field.getAliasField() + " " + getCode();
    }
  }
 else {
    retval=getCode();
  }
  return retval;
}

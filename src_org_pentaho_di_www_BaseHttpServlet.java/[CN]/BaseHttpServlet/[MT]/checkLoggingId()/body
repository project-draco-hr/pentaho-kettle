{
  if (loggingId == null) {
    loggingId=registry.registerLoggingSource(this);
    try {
      throw new Exception();
    }
 catch (    Exception e) {
      StackTraceElement[] stackTrace=e.getStackTrace();
      for (      StackTraceElement element : stackTrace) {
        System.out.println(element.getClass().toString());
      }
    }
  }
}

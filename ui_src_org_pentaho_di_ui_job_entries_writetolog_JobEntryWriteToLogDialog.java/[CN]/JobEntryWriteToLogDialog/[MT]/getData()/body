{
  if (jobEntry.getName() != null)   wName.setText(jobEntry.getName());
  wName.selectAll();
  if (jobEntry.getLogMessage() != null)   wLogMessage.setText(jobEntry.getLogMessage());
  if (jobEntry.getLogSubject() != null)   wLogSubject.setText(jobEntry.getLogSubject());
  if (jobEntry.entryLogLevel != null) {
    wLoglevel.select(jobEntry.entryLogLevel.getLevel());
  }
}

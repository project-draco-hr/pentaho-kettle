{
  try {
    PrintWriter pw=null;
    if (os != null)     pw=new PrintWriter(os);
    InputStreamReader isr=new InputStreamReader(is);
    BufferedReader br=new BufferedReader(isr);
    String line=null;
    while ((line=br.readLine()) != null) {
      if (pw != null)       pw.println(line);
      logBasic(type + ">" + line);
    }
    if (pw != null)     pw.flush();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
}

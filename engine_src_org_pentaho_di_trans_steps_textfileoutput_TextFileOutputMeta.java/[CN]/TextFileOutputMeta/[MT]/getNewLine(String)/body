{
  String nl=System.getProperty("line.separator");
  if (fformat != null) {
    if (fformat.equalsIgnoreCase("DOS")) {
      nl="\r\n";
    }
 else     if (fformat.equalsIgnoreCase("UNIX")) {
      nl="\n";
    }
 else     if (fformat.equalsIgnoreCase("CR")) {
      nl="\r";
    }
 else     if (fformat.equalsIgnoreCase("None")) {
      nl="";
    }
  }
  return nl;
}

{
  meta=(DBProcMeta)smi;
  data=(DBProcData)sdi;
  if (super.init(smi,sdi)) {
    data.db=new Database(meta.getDatabase());
    try {
      data.db.connect();
      logBasic(Messages.getString("DBProc.Log.ConnectedToDB"));
      return true;
    }
 catch (    KettleException e) {
      logError(Messages.getString("DBProc.Log.DBException") + e.getMessage());
      data.db.disconnect();
    }
  }
  return false;
}

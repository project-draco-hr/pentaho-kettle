{
  meta=(DBProcMeta)smi;
  data=(DBProcData)sdi;
  if (super.init(smi,sdi)) {
    data.db=new Database(meta.getDatabase());
    try {
      data.db.connect();
      logBasic("Connected to database...");
      return true;
    }
 catch (    KettleException e) {
      logError("An error occurred, processing will be stopped: " + e.getMessage());
      data.db.disconnect();
    }
  }
  return false;
}

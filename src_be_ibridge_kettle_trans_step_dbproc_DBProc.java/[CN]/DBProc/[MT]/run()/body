{
  logBasic("Starting to run...");
  try {
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError("Unexpected error in '" + debug + "' : "+ e.toString());
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

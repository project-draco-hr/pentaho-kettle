{
  directory="test/useCases/replay/textFileInputOutputSkipErrorLineTrue/";
  expectFiles(directory,2);
  meta=new TransMeta(directory + "transform.ktr");
  trans=new Trans(log,meta);
  boolean ok=trans.execute(null);
  assertTrue(ok);
  trans.waitUntilFinished();
  trans.endProcessing("end");
  assertEquals(0,trans.getErrors());
  expectFiles(directory,4);
  expectContent(directory + "input.txt." + getDateFormatted()+ ".line","3" + Const.CR + "5"+ Const.CR);
  expectContent(directory + "result.out","StrField;NumField;DatField;IntField" + Const.CR + "Line2 ; 1234;2005/05/26 12:34:56.000; 9876"+ Const.CR+ "Line4 ; 0009;2005/05/26 21:01:23.000; 1234"+ Const.CR);
}

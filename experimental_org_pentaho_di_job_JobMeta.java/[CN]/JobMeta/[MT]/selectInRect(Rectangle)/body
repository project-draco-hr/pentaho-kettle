{
  int i;
  for (i=0; i < nrJobEntries(); i++) {
    JobEntryCopy je=getJobEntry(i);
    Point p=je.getLocation();
    if (((p.x >= rect.x && p.x <= rect.x + rect.width) || (p.x >= rect.x + rect.width && p.x <= rect.x)) && ((p.y >= rect.y && p.y <= rect.y + rect.height) || (p.y >= rect.y + rect.height && p.y <= rect.y)))     je.setSelected(true);
  }
}

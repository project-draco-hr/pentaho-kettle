{
  try {
    long dbids[]=rep.getDatabaseIDs();
    for (int i=0; i < dbids.length; i++) {
      DatabaseMeta databaseMeta=new DatabaseMeta(rep,dbids[i]);
      DatabaseMeta check=findDatabase(databaseMeta.getName());
      if (check == null || overWriteShared) {
        if (databaseMeta.getName() != null) {
          addOrReplaceDatabase(databaseMeta);
          if (!overWriteShared)           databaseMeta.setChanged(false);
        }
      }
    }
    setChanged(false);
  }
 catch (  KettleDatabaseException dbe) {
    throw new KettleException(Messages.getString("JobMeta.Log.UnableToReadDatabaseIDSFromRepository"),dbe);
  }
catch (  KettleException ke) {
    throw new KettleException(Messages.getString("JobMeta.Log.UnableToReadDatabasesFromRepository"),ke);
  }
}

{
  if (log.isBasic())   logBasic(Messages.getString("ProcessFiles.Log.StartingToRun"));
  try {
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError(Messages.getString("ProcessFiles.Log.UnexpectedError") + " : " + e.toString());
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

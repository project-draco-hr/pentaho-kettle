{
  Locale resultLocale=null;
  if (localeCode != null) {
    StringTokenizer parser=new StringTokenizer(localeCode,"_");
    if (parser.countTokens() == 2) {
      resultLocale=new Locale(parser.nextToken(),parser.nextToken());
    }
 else {
      resultLocale=new Locale(localeCode);
    }
  }
  return resultLocale;
}

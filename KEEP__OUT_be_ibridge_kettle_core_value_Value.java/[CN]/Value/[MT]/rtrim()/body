{
  if (isNull()) {
    setType(VALUE_TYPE_STRING);
  }
 else {
    StringBuffer s;
    if (getType() == VALUE_TYPE_STRING) {
      s=new StringBuffer(getString());
    }
 else {
      s=new StringBuffer(toString());
    }
    while (s.length() > 0 && s.charAt(s.length() - 1) == ' ')     s.deleteCharAt(s.length() - 1);
    setValue(s);
  }
  return this;
}

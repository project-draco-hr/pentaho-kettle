{
  InputStream is=null;
  InputStreamReader isr=null;
  try {
    is=JsonJar.class.getResource(script).openStream();
    isr=new InputStreamReader(is);
    getEngine().eval(new BufferedReader(isr));
  }
  finally {
    try {
      if (is != null)       is.close();
      if (isr != null)       isr.close();
    }
 catch (    Exception e) {
    }
  }
}

{
  if (Const.isEmpty(info) || info[0] == null) {
    for (int i=0; i < returnValueNewName.length; i++) {
      try {
        ValueMetaInterface v=ValueMetaFactory.createValueMeta(returnValueNewName[i],returnValueDefaultType[i]);
        v.setOrigin(name);
        row.addValueMeta(v);
      }
 catch (      Exception e) {
        throw new KettleStepException(e);
      }
    }
  }
 else {
    for (int i=0; i < returnValueNewName.length; i++) {
      ValueMetaInterface v=info[0].searchValueMeta(returnValueField[i]);
      if (v != null) {
        v.setName(returnValueNewName[i]);
        v.setOrigin(name);
        row.addValueMeta(v);
      }
    }
  }
}

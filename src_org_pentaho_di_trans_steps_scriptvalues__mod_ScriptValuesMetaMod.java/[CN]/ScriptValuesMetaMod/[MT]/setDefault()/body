{
  jsScripts=new ScriptValuesScript[1];
  jsScripts[0]=new ScriptValuesScript(ScriptValuesScript.TRANSFORM_SCRIPT,Messages.getString("ScriptValuesMod.Script1"),"//" + Messages.getString("ScriptValuesMod.ScriptHere") + Const.CR+ Const.CR);
  int nrfields=0;
  allocate(nrfields);
  for (int i=0; i < nrfields; i++) {
    name[i]="newvalue";
    rename[i]="newvalue";
    type[i]=ValueMetaInterface.TYPE_NUMBER;
    length[i]=-1;
    precision[i]=-1;
    replace[i]=false;
  }
  compatible=false;
}

{
  DatabaseMeta source=getSourceDatabase();
  DatabaseMeta target=getTargetDatabase();
  if (source == null && target == null) {
    setErrorMessage(Messages.getString("CopyTableWizardPage1.SourceAndTargetIsNull.DialogMessage"));
    return false;
  }
 else   if (source == null && target != null) {
    setErrorMessage(Messages.getString("CopyTableWizardPage1.SourceIsNull.DialogMessage"));
    return false;
  }
 else   if (source != null && target == null) {
    setErrorMessage(Messages.getString("CopyTableWizardPage1.TargetIsNull.DialogMessage"));
    return false;
  }
 else   if (source != null && target != null && source.equals(target)) {
    setErrorMessage(Messages.getString("CopyTableWizardPage1.SourceAndTargetIsSame.DialogMessage"));
    return false;
  }
 else {
    setErrorMessage(null);
    setMessage(Messages.getString("CopyTableWizardPage1.GoOnNext.DialogMessage"));
    return true;
  }
}

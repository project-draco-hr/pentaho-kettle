{
  meta=(RowsToResultMeta)smi;
  data=(RowsToResultData)sdi;
  Row r=getRow();
  if (r == null) {
    getTransMeta().setResultRows(data.rows);
    setOutputDone();
    return false;
  }
  data.rows.add(r);
  putRow(r);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic("Linenr " + linesRead);
  return true;
}

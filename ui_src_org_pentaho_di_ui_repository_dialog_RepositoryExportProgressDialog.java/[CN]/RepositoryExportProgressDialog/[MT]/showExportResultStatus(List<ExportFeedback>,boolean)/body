{
  String desc=fail ? BaseMessages.getString(PKG,"RepositoryExportProgressDialog.ExportResultDialog.Fail") : BaseMessages.getString(PKG,"RepositoryExportProgressDialog.ExportResultDialog.Succes");
  EnterTextDialog dialog=new EnterTextDialog(shell,BaseMessages.getString(PKG,"RepositoryExportProgressDialog.ExportResultDialog.Title"),desc,getExportResultDetails(list,fail));
  dialog.setReadOnly();
  dialog.setModal();
  dialog.open();
}

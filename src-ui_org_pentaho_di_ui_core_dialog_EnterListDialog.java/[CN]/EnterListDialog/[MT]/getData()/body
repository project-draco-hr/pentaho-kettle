{
  if (!opened)   return;
  wListSource.removeAll();
  wListDest.removeAll();
  for (int i=0; i < input.length; i++) {
    Integer idx=new Integer(i);
    String str=selection.get(idx);
    if (str == null) {
      wListSource.add(input[i]);
    }
 else {
      wListDest.add(input[i]);
    }
  }
}

{
  if (getID() < 0) {
    try {
      if (rep.getTransformationID(getName(),directory.getID()) > 0)       return true;
    }
 catch (    KettleException dbe) {
      log.logError(toString(),BaseMessages.getString(PKG,"TransMeta.Log.DatabaseError") + dbe.getMessage());
      return true;
    }
  }
  return false;
}

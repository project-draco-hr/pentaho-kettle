{
  String transformation=null;
  if (!Const.isEmpty(getFilename())) {
    transformation=jobMeta.environmentSubstitute(getFilename());
  }
 else   if (!Const.isEmpty(getTransname()) && getDirectory() != null) {
    transformation=jobMeta.environmentSubstitute(getTransname());
  }
  if (transformation != null) {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("JobEntryTrans.CheckResult.TransformationDefined",transformation),this));
  }
 else {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryTrans.CheckResult.TransformationNotDefined"),this));
  }
}

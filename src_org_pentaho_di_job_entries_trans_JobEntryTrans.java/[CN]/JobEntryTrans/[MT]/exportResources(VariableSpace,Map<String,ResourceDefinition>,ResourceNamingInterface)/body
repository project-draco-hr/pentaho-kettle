{
  if (!Const.isEmpty(filename)) {
    Variables vars=new Variables();
    vars.copyVariablesFrom(this);
    vars.copyVariablesFrom(space);
    TransMeta transMeta=getTransMeta(null,vars);
    String newFilename=transMeta.exportResources(transMeta,definitions,namingInterface);
    transMeta.setFilename(newFilename);
    filename=newFilename;
    return newFilename;
  }
 else {
    return null;
  }
}

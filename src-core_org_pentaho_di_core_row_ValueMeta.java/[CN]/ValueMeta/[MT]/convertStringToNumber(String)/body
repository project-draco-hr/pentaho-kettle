{
  string=Const.trimToType(string,getTrimType());
  if (Const.isEmpty(string))   return null;
  try {
    ParsePosition parsePosition=new ParsePosition(0);
    Number number=getDecimalFormat(false).parse(string,parsePosition);
    return new Double(number.doubleValue());
  }
 catch (  Exception e) {
    throw new KettleValueException(toString() + " : couldn't convert String to number ",e);
  }
}

{
  boolean isClipped;
  Monitor[] monitors=display.getMonitors();
  if (monitors.length > 0) {
    isClipped=true;
    for (    Monitor monitor : monitors) {
      if (monitor.getClientArea().contains(constrainee.x + 50,constrainee.y + 50)) {
        isClipped=false;
        break;
      }
    }
  }
 else {
    isClipped=false;
  }
  return isClipped;
}

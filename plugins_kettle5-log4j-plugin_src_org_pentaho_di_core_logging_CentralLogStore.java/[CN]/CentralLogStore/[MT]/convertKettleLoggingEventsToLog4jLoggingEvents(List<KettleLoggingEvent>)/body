{
  LogWriter logWriter=LogWriter.getInstance();
  List<LoggingEvent> list=new ArrayList<LoggingEvent>();
  for (  KettleLoggingEvent event : events) {
    LoggingEvent loggingEvent=new LoggingEvent(logWriter.getPentahoLogger().getClass().getName(),logWriter.getPentahoLogger(),event.getTimeStamp(),convertKettleLogLevelToLog4jLevel(event.getLevel()),event.getMessage(),null,null,null,null,null);
    list.add(loggingEvent);
  }
  return list;
}

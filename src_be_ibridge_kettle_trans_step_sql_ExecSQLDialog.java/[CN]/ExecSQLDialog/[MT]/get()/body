{
  try {
    Row r=transMeta.getPrevStepFields(stepname);
    if (r != null) {
      BaseStepDialog.getFieldsFromPrevious(r,wFields,1,new int[]{1},new int[]{},-1,-1,null);
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(shell,Messages.getString("ExecSQLDialog.FailedToGetFields.DialogTitle"),Messages.getString("ExecSQLDialog.FailedToGetFields.DialogMessage"),ke);
  }
}

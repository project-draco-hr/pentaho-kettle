{
  meta=(UpdateMeta)smi;
  data=(UpdateData)sdi;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  try {
    lookupValues(r);
    if (!r.isIgnored())     putRow(r);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("Update.Log.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    logError(Messages.getString("Update.Log.ErrorInStep") + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

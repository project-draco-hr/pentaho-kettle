{
  meta=(DummyTransMeta)smi;
  data=(DummyTransData)sdi;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  putRow(r);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("DummyTrans.Log.LineNumber") + linesRead);
  return true;
}

{
  if (trans.getStepPerformanceSnapShots() != null) {
    StepPerformanceSnapShotDialog dialog=new StepPerformanceSnapShotDialog(shell,transMeta.getName(),trans.getStepPerformanceSnapShots(),transMeta.getStepPerformanceCapturingDelay());
    dialog.open();
  }
 else {
    MessageBox m=new MessageBox(shell,SWT.OK | SWT.ICON_INFORMATION);
    m.setText(Messages.getString("TransLog.Dialog.PerformanceMonitoringNotEnabled.Title"));
    m.setMessage(Messages.getString("TransLog.Dialog.PerformanceMonitoringNotEnabled.Message"));
    m.open();
  }
}

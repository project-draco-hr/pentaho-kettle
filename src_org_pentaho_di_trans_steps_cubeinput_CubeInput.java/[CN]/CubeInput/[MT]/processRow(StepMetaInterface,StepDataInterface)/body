{
  meta=(CubeInputMeta)smi;
  data=(CubeInputData)sdi;
  try {
    Object[] r=data.meta.readData(data.dis);
    putRow(data.meta,r);
    linesInput++;
    if (meta.getRowLimit() > 0 && linesInput >= meta.getRowLimit()) {
      setOutputDone();
      return false;
    }
  }
 catch (  KettleEOFException eof) {
    setOutputDone();
    return false;
  }
catch (  SocketTimeoutException e) {
    throw new KettleException(e);
  }
  if (checkFeedback(linesInput)) {
    if (log.isBasic())     logBasic(Messages.getString("CubeInput.Log.LineNumber") + linesInput);
  }
  return true;
}

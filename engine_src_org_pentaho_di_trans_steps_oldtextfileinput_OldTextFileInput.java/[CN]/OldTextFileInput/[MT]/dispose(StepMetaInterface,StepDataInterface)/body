{
  meta=(OldTextFileInputMeta)smi;
  data=(OldTextFileInputData)sdi;
  if (data.file != null) {
    try {
      data.file.close();
      data.file=null;
    }
 catch (    Exception e) {
      log.logError("Error closing file",e);
    }
  }
  if (data.in != null) {
    BaseStep.closeQuietly(data.in);
    data.in=null;
  }
  super.dispose(smi,sdi);
}

{
  this(colname,coltype);
  this.combovals=null;
  this.numeric=numeric;
  this.tooltip=null;
  this.allignement=SWT.LEFT;
  this.readonly=false;
  this.hide_negative=false;
  if (numeric) {
    valueType=ValueMetaInterface.TYPE_INTEGER;
  }
 else {
    valueType=ValueMetaInterface.TYPE_STRING;
  }
}

{
  int a, b, c;
  a=b=c=0xdeadbeef + (((int)length) << 2) + initVal;
  int i=0;
  while (length > 3) {
    a+=myKey[i];
    b+=myKey[i + 1];
    c+=myKey[i + 2];
    c=mix(a,b,c);
    length-=3;
    i=i + 3;
  }
switch (length) {
case 3:
    c+=myKey[i + 2];
case 2:
  b+=myKey[i + 1];
case 1:
a+=myKey[i];
c=finalMix(a,b,c);
case 0:
break;
}
return c;
}

{
  repository.connectionDelegate.performDelete("DELETE FROM " + quoteTable(KettleDatabaseRepository.TABLE_R_ELEMENT_ATTRIBUTE) + " WHERE "+ quote(KettleDatabaseRepository.FIELD_ELEMENT_ATTRIBUTE_ID_ELEMENT)+ " = ? ",elementId);

{
  return repository.connectionDelegate.getIDWithValue(quoteTable(KettleDatabaseRepository.TABLE_R_ELEMENT),quote(KettleDatabaseRepository.FIELD_ELEMENT_ID_ELEMENT),quote(KettleDatabaseRepository.FIELD_ELEMENT_NAME),elementName,new String[]{quote(KettleDatabaseRepository.FIELD_ELEMENT_ID_ELEMENT_TYPE)},new ObjectId[]{elementTypeId});

{
  if (StringUtils.isBlank(script)) {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryEval.CheckResult.ScriptIsBlank"),this));
    return;
  }
  String substr=script.substring(0,Math.min(75,script.length()));
  remarks.add(new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("JobEntryEval.CheckResult.ScriptExcerpt",substr),this));
}

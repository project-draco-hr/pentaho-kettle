{
  meta=(TextFileOutputMeta)smi;
  data=(TextFileOutputData)sdi;
  if (data.oneFileOpened)   closeFile();
  try {
    if (data.fos != null) {
      data.fos.close();
    }
  }
 catch (  Exception e) {
  }
  for (  OutputStream outputStream : data.fileWriterMap.values()) {
    try {
      outputStream.close();
    }
 catch (    IOException e) {
    }
  }
  super.dispose(smi,sdi);
}

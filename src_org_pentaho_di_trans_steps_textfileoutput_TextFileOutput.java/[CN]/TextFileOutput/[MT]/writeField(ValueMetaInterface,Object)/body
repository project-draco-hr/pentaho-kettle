{
  try {
    byte[] str;
    if (meta.isFastDump()) {
      if (valueData instanceof byte[]) {
        str=(byte[])valueData;
      }
 else {
        String lvalue=valueData.toString();
        str=getBinaryString(v.getLength() == -1 ? lvalue.trim() : lvalue);
      }
    }
 else {
      str=formatField(v,valueData);
    }
    if (str != null)     data.writer.write(str);
  }
 catch (  Exception e) {
    throw new KettleStepException("Error writing field content to file",e);
  }
}

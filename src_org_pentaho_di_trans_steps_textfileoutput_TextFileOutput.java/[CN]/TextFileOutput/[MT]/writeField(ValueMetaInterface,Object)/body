{
  try {
    byte[] str;
    if (meta.isFastDump()) {
      str=getBinaryString(valueData.toString());
    }
 else {
      str=formatField(v,valueData);
    }
    if (str != null)     data.writer.write(str);
  }
 catch (  Exception e) {
    throw new KettleStepException("Error writing field content to file",e);
  }
}

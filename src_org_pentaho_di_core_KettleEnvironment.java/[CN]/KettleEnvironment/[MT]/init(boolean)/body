{
  if (initialized == null) {
    createKettleHome();
    EnvUtil.environmentInit();
    CentralLogStore.init();
    if (simpleJndi) {
      JndiUtil.initJNDI();
    }
    PluginTypeInterface[] pluginTypes=new PluginTypeInterface[]{StepPluginType.getInstance(),PartitionerPluginType.getInstance(),JobEntryPluginType.getInstance(),RepositoryPluginType.getInstance(),DatabasePluginType.getInstance()};
    PluginRegistry.init(pluginTypes);
    initialized=true;
  }
}

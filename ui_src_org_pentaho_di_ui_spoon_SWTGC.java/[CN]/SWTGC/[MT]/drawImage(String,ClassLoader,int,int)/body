{
  Image img=SwtSvgImageUtil.getImage(PropsUI.getDisplay(),classLoader,location,Math.round(small_icon_size * currentMagnification),Math.round(small_icon_size * currentMagnification));
  if (img != null) {
    Rectangle bounds=img.getBounds();
    gc.drawImage(img,0,0,bounds.width,bounds.height,x,y,small_icon_size,small_icon_size);
  }
}

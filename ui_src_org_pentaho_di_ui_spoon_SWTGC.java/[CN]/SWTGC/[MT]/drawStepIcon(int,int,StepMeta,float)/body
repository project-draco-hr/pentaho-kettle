{
  String steptype=stepMeta.getStepID();
  Image im=null;
  if (stepMeta.isMissing()) {
    im=GUIResource.getInstance().getImageMissing();
  }
 else {
    im=images.get(steptype).getAsBitmapForSize(gc.getDevice(),Math.round(iconsize * magnification),Math.round(iconsize * magnification));
  }
  if (im != null) {
    org.eclipse.swt.graphics.Rectangle bounds=im.getBounds();
    gc.drawImage(im,0,0,bounds.width,bounds.height,x,y,iconsize,iconsize);
  }
}

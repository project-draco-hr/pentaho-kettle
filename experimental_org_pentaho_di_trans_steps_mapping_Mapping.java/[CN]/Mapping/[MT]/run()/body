{
  try {
    logBasic(Messages.getString("Mapping.Log.StartingToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError(Messages.getString("Mapping.Log.UnexpectedError") + " : " + e.toString());
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
    if (data.trans != null)     data.trans.stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

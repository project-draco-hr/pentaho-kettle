{
  int len=(fieldnrs1.length < fieldnrs2.length) ? fieldnrs1.length : fieldnrs2.length;
  for (int i=0; i < len; i++) {
    ValueMetaInterface valueMeta=getValueMeta(fieldnrs1[i]);
    int cmp=valueMeta.compare(rowData1[fieldnrs1[i]],rowData2[fieldnrs2[i]]);
    if (cmp != 0)     return cmp;
  }
  return 0;
}

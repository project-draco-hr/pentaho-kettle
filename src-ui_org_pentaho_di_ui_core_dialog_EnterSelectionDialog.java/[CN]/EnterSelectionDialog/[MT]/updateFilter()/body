{
  pattern=null;
  filterString=null;
  if (searchText != null && !searchText.isDisposed() && !Const.isEmpty(searchText.getText())) {
    if (wbRegex.getSelection()) {
      pattern=Pattern.compile(searchText.getText());
    }
 else {
      filterString=searchText.getText().toUpperCase();
    }
  }
  refresh();
}

{
  StringBuffer newName=new StringBuffer(fieldname.length());
  for (int idx=0; idx < fieldname.length(); idx++) {
    char c=fieldname.charAt(idx);
    if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z') || (c >= '0' && c <= '9')) {
      newName.append(c);
    }
 else     if (c == ' ') {
      newName.append('_');
    }
 else {
    }
  }
  return super.getSafeFieldname(newName.toString());
}

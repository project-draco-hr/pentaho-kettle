{
  String retval="";
  if (hasAggregate() && !isExact()) {
    retval+=getFunction(dbinfo) + "(" + getAliasField()+ ")";
    retval+=" as F___" + fieldnr;
  }
 else   if (isExact()) {
    retval+=getAliasField() + " as E___" + fieldnr;
  }
 else {
    retval+=getAliasField();
  }
  return retval;
}

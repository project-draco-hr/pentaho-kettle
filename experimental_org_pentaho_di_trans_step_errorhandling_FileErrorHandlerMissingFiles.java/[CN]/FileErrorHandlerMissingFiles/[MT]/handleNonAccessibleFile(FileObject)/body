{
  handleFile(file);
  try {
    getWriter(NO_PARTS).write(THIS_FILE_WAS_NOT_ACCESSIBLE);
    getWriter(NO_PARTS).write(Const.CR);
  }
 catch (  Exception e) {
    throw new KettleException(Messages.getString("FileErrorHandlerMissingFiles.Exception.CouldNotCreateNonAccessibleFile") + file.getName().getURI(),e);
  }
}

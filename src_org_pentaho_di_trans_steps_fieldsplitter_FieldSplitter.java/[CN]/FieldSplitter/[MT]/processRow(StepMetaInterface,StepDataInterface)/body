{
  meta=(FieldSplitterMeta)smi;
  data=(FieldSplitterData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  boolean noerr=splitField(r);
  if (!noerr) {
    setOutputDone();
    return false;
  }
  putRow(data.outputMeta,r);
  if (checkFeedback(linesRead))   logBasic(Messages.getString("FieldSplitter.Log.LineNumber") + linesRead);
  return true;
}

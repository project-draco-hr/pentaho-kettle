{
  meta=(FieldSplitterMeta)smi;
  data=(FieldSplitterData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  Object[] outputRowData=splitField(r);
  putRow(data.outputMeta,outputRowData);
  if (checkFeedback(linesRead)) {
    if (log.isBasic())     logBasic(Messages.getString("FieldSplitter.Log.LineNumber") + linesRead);
  }
  return true;
}

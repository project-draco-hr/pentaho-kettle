{
  String retval="";
  retval+="    <fields>" + Const.CR;
  for (int i=0; i < fieldName.length; i++) {
    retval+="      <field>" + Const.CR;
    retval+="        " + XMLHandler.addTagValue("name",fieldName[i]);
    retval+="        " + XMLHandler.addTagValue("value",fieldValue[i]);
    retval+="        </field>" + Const.CR;
  }
  retval+="      </fields>" + Const.CR;
  return retval;
}

  wRowSeparatorFormat.add(Messages.getString("GetFilesRowsCountDialog.RowSeparatorFormat.CR.Label"));
  wRowSeparatorFormat.add(Messages.getString("GetFilesRowsCountDialog.RowSeparatorFormat.LF.Label"));

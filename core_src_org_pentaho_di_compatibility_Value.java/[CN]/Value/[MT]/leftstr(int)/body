{
  if (isNull()) {
    setType(VALUE_TYPE_STRING);
    return this;
  }
  setValue(getString());
  int tot_len=getString() != null ? getString().length() : 0;
  if (tot_len > 0) {
    int totlen=getString().length();
    int f=totlen - len;
    if (f > 0) {
      setValue(getString().substring(0,len));
    }
  }
 else {
    setNull();
  }
  if (!isString())   setType(VALUE_TYPE_STRING);
  return this;
}

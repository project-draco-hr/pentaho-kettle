{
  if (isNull()) {
    setType(VALUE_TYPE_STRING);
    return this;
  }
  setValue(getString());
  if (getString() != null) {
    if (to < 0 && from >= 0) {
      setValue(getString().substring(from));
    }
 else     if (to >= 0 && from >= 0) {
      setValue(getString().substring(from,to));
    }
  }
 else {
    setNull();
  }
  if (!isString())   setType(VALUE_TYPE_STRING);
  return this;
}

{
  if (other == null || !getName().equals(other.getName()) || getType() != other.getType())   return;
switch (getType()) {
case VALUE_TYPE_BIGNUMBER:
    if (getBigNumber() == null)     setValue(other.getBigNumber());
  break;
case VALUE_TYPE_BINARY:
if (getBytes() == null || getBytes().length == 0) if (other.getBytes() != null && other.getBytes().length > 0) setValue(other.getBytes());
break;
case VALUE_TYPE_BOOLEAN:
break;
case VALUE_TYPE_DATE:
if (getDate() == null) setValue(other.getDate());
break;
case VALUE_TYPE_INTEGER:
if (getInteger() == 0l) setValue(other.getInteger());
break;
case VALUE_TYPE_NUMBER:
if (getNumber() == 0.0) setValue(other.getNumber());
break;
case VALUE_TYPE_SERIALIZABLE:
break;
case VALUE_TYPE_STRING:
if (Const.isEmpty(getString()) && !Const.isEmpty(other.getString())) setValue(other.getString());
break;
default :
break;
}
}

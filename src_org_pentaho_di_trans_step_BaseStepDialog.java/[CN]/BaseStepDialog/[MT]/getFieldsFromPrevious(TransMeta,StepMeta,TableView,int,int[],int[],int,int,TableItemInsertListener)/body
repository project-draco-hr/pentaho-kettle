{
  try {
    RowMetaInterface row=transMeta.getPrevStepFields(stepMeta);
    if (row != null) {
      getFieldsFromPrevious(row,tableView,keyColumn,nameColumn,dataTypeColumn,lengthColumn,precisionColumn,listener);
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(tableView.getShell(),Messages.getString("BaseStepDialog.FailedToGetFields.Title"),Messages.getString("BaseStepDialog.FailedToGetFields.Message",stepMeta.getName()),ke);
  }
}

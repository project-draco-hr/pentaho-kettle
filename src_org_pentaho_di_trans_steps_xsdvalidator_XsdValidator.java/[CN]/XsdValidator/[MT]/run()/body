{
  try {
    logBasic(Messages.getString("XsdValidator.Log.StartingToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError(Messages.getString("XsdValidator.Log.UnexpectedeError") + " : " + e.toString());
    logError(Messages.getString("XsdValidator.Log.ErrorStackTrace") + Const.CR + Const.getStackTracker(e));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

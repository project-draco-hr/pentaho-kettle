{
  if (isDetailed()) {
    logDetailed("=======================================");
    logDetailed(BaseMessages.getString(PKG,"JobPGPDecryptFiles.Log.Info.FilesInError","" + NrErrors));
    logDetailed(BaseMessages.getString(PKG,"JobPGPDecryptFiles.Log.Info.FilesInSuccess","" + NrSuccess));
    logDetailed("=======================================");
  }
}

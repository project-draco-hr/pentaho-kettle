{
  Row arguments=transMeta.getUsedArguments(spoon.getArguments());
  if (arguments.size() > 0) {
    EnterStringsDialog esd=new EnterStringsDialog(shell,SWT.NONE,arguments);
    if (esd.open() == null) {
      arguments=null;
    }
 else {
      spoon.props.setLastArguments(Props.convertArguments(arguments));
    }
  }
  return arguments;
}

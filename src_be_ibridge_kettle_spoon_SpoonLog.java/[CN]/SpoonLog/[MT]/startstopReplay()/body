{
  DateValidator dateValidator=new DateValidator();
  InputDialog id=new InputDialog(shell,"Enter Replay Transformation date","What's the execution date of the transformation" + Const.CR + "you want to replay?",dateValidator.df.format(new Date()),dateValidator);
  int answer=id.open();
  if (answer == 1) {
    log.logDebug(toString(),"Cancel replay");
    return;
  }
  if (dateValidator.date == null) {
    log.logDebug(toString(),"Cancel replay");
    return;
  }
  startstop(dateValidator.date);
}

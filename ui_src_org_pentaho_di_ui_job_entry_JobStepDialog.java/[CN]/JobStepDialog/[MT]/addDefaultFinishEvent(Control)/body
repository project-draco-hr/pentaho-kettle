{
  try {
    Method m=null;
    try {
      m=el.getClass().getMethod("addSelectionListener",SelectionAdapter.class);
    }
 catch (    Exception e) {
      m=el.getClass().getMethod("addSelectionListener",SelectionListener.class);
    }
    if (m != null) {
      m.invoke(el,DEFAULT_FINISH_EVENT);
    }
  }
 catch (  Exception e1) {
  }
}

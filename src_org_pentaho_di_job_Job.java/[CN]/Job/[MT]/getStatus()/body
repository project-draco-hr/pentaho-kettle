{
  String message;
  if (!initialized) {
    message=Trans.STRING_WAITING;
  }
 else {
    if (active) {
      if (stopped) {
        message=Trans.STRING_STOPPED;
      }
 else {
        message=Trans.STRING_RUNNING;
      }
    }
 else {
      message=Trans.STRING_FINISHED;
      if (result != null && result.getNrErrors() > 0) {
        message+=" (with errors)";
      }
    }
  }
  return message;
}

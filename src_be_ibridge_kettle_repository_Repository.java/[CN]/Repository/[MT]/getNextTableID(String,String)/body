{
  long retval=-1;
  Row r=database.getOneRow("SELECT max(" + database.getDatabaseMeta().quoteField(idfield) + ") FROM "+ tablename);
  if (r != null) {
    Value id=r.getValue(0);
    if (id.isNull()) {
      log.logDebug(toString(),"no max(" + idfield + ") found in table "+ tablename);
      retval=1;
    }
 else {
      log.logDebug(toString(),"max(" + idfield + ") found in table "+ tablename+ " --> "+ id.getInteger()+ " number: "+ id.getNumber());
      retval=id.getInteger() + 10000L;
    }
  }
  return retval;
}

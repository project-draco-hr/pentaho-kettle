{
  Row par=new Row();
  par.addValue(new Value("value",value));
  par.addValue(new Value("key",key));
  Row result=database.getOneRow("SELECT " + database.getDatabaseMeta().quoteField(idfield) + " FROM "+ tablename+ " WHERE "+ database.getDatabaseMeta().quoteField(lookupfield)+ " = ? AND "+ database.getDatabaseMeta().quoteField(lookupkey)+ " = ?",par);
  if (result != null && result.getValue(0).isNumeric())   return result.getValue(0).getInteger();
  return -1;
}

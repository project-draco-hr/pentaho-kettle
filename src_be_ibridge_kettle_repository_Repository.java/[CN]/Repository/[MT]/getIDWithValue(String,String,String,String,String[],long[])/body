{
  Row par=new Row();
  par.addValue(new Value(lookupfield,value));
  String sql="SELECT " + database.getDatabaseMeta().quoteField(idfield) + " FROM "+ tablename+ " WHERE "+ database.getDatabaseMeta().quoteField(lookupfield)+ " = ? ";
  for (int i=0; i < lookupkey.length; i++) {
    par.addValue(new Value(lookupkey[i],key[i]));
    sql+="AND " + database.getDatabaseMeta().quoteField(lookupkey[i]) + " = ? ";
  }
  Row result=database.getOneRow(sql,par);
  if (result != null && result.getValue(0).isNumeric())   return result.getValue(0).getInteger();
  return -1;
}

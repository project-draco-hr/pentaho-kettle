{
  String sql="SELECT " + database.getDatabaseMeta().quoteField(fieldname) + " FROM "+ tablename+ " WHERE "+ database.getDatabaseMeta().quoteField(keyfield)+ " = ?";
  Row par=new Row();
  par.addValue(new Value(keyfield,id));
  Row result=database.getOneRow(sql,par);
  if (result != null) {
    return result.getValue(0).getString();
  }
  return null;
}

{
  if (field.toUpperCase().startsWith((tableAliasPrefix + ".").toUpperCase())) {
    return ThinUtil.stripQuotesIfNoWhitespace(field.substring(tableAliasPrefix.length() + 1),'"');
  }
 else   if (field.toUpperCase().startsWith(("\"" + tableAliasPrefix + "\".").toUpperCase())) {
    return ThinUtil.stripQuotesIfNoWhitespace(field.substring(tableAliasPrefix.length() + 3),'"');
  }
 else {
    return field;
  }
}

{
  int index=0;
  if (repositoryMeta != null && availableRepositories != null && availableRepositories.size() > 0) {
    for (    RepositoryMeta meta : availableRepositories) {
      if (meta != null && meta.getName().equals(repositoryMeta.getName())) {
        break;
      }
 else {
        index++;
      }
    }
  }
 else {
    index=-1;
  }
  return index;
}

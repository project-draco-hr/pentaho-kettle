{
  this.filename=filename;
  this.file=KettleVFS.getFileObject(filename);
  InputStream is=null;
  try {
    is=KettleVFS.getInputStream(getFile());
    for (    Object data : getYaml().loadAll(is)) {
      documents.add(data);
      this.useMap=(data instanceof Map);
    }
    this.documenti=documents.iterator();
  }
  finally {
    if (is != null)     is.close();
  }
}

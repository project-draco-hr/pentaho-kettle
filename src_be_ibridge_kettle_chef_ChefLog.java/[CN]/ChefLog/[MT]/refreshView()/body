{
  if (isRunning && job != null && job.isInitialized() && !job.isActive()) {
    LocalVariables.getInstance().removeKettleVariables(job.getName());
    job=null;
    isRunning=false;
    log.logMinimal(Chef.APP_NAME,Messages.getString("ChefLog.Log.JobHasEnded"));
  }
  if (!wStart.isDisposed()) {
    if (job != null)     wStart.setText(STOP_TEXT);
 else     wStart.setText(START_TEXT);
  }
}

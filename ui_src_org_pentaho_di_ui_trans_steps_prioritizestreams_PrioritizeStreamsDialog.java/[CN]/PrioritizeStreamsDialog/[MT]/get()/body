{
  wFields.removeAll();
  Table table=wFields.table;
  for (int i=0; i < previousSteps.length; i++) {
    TableItem ti=new TableItem(table,SWT.NONE);
    ti.setText(0,"" + (i + 1));
    ti.setText(1,previousSteps[i]);
  }
  wFields.removeEmptyRows();
  wFields.setRowNums();
  wFields.optWidth(true);
}

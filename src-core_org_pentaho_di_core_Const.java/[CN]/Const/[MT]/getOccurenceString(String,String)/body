{
  if (string == null || string.length() == 0)   return 0;
  int counter=0;
  int len=searchFor.length();
  int result=0;
  if (len > 0) {
    int start=string.indexOf(searchFor);
    while (start != -1) {
      result++;
      start=string.indexOf(searchFor,start + len);
    }
  }
  return counter;
}

{
  name=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_NAME);
  rename=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_RENAME);
  type=ValueMeta.getType(XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_TYPE));
  length=Const.toInt(XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_LENGTH),-2);
  precision=Const.toInt(XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_PRECISION),-2);
  storageType=ValueMeta.getStorageType(XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_STORAGE_TYPE));
  conversionMask=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_CONVERSION_MASK);
  encoding=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_ENCODING);
  decimalSymbol=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_DECIMAL);
  groupingSymbol=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_GROUPING);
  currencySymbol=XMLHandler.getTagValue(metaNode,SelectValuesAttr.META_CURRENCY);
}

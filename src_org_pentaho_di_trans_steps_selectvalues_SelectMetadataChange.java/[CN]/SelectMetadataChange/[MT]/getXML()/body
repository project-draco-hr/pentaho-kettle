{
  StringBuffer retval=new StringBuffer();
  retval.append("      ").append(XMLHandler.openTag(XML_TAG));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_NAME,name));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_RENAME,rename));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_TYPE,ValueMeta.getTypeDesc(type)));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_LENGTH,length));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_PRECISION,precision));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_CONVERSION_MASK,conversionMask));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_ENCODING,encoding));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_DECIMAL,decimalSymbol));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_GROUPING,groupingSymbol));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_CURRENCY,currencySymbol));
  retval.append("        ").append(XMLHandler.addTagValue(SelectValuesAttr.META_STORAGE_TYPE,ValueMeta.getStorageTypeCode(storageType)));
  retval.append("      ").append(XMLHandler.closeTag(XML_TAG));
  return retval.toString();
}

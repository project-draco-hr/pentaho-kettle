{
  boolean retval=false;
  try {
    if (data.workbook != null) {
      data.workbook.write();
      data.workbook.close();
    }
    data.formats.clear();
    System.gc();
    retval=true;
  }
 catch (  Exception e) {
    logError("Unable to close openFile file : " + e.toString());
  }
  return retval;
}

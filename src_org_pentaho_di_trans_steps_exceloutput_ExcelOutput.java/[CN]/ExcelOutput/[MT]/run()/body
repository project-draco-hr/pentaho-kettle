{
  try {
    logBasic("Starting to run...");
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Throwable t) {
    logError("Unexpected error : " + t.toString());
    logError(Const.getStackTracker(t));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

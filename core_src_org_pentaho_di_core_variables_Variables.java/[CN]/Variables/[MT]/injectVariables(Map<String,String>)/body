{
  if (initialized) {
    if (prop != null) {
      for (      String key : prop.keySet()) {
        String value=prop.get(key);
        if (!Const.isEmpty(key)) {
          properties.put(key,Const.NVL(value,""));
        }
      }
      injection=null;
    }
  }
 else {
    injection=new Hashtable<String,String>();
    for (    String key : prop.keySet()) {
      String value=prop.get(key);
      if (!Const.isEmpty(key)) {
        injection.put(key,Const.NVL(value,""));
      }
    }
  }
}

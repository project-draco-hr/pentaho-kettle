{
  if (getTransMeta().getSourceRows() == null || data.pointer >= getTransMeta().getSourceRows().size()) {
    setOutputDone();
    return false;
  }
  Row r=(Row)getTransMeta().getSourceRows().get((int)linesRead);
  linesRead++;
  putRow(r);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("RowsFromResult.Log.LineNumber") + linesRead);
  return true;
}

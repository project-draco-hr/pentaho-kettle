{
  meta=(SortedMergeMeta)smi;
  data=(SortedMergeData)sdi;
  Object[] row=getRowSorted();
  if (row == null) {
    setOutputDone();
    return false;
  }
  putRow(data.rowMeta,row);
  if (checkFeedback(linesRead))   logBasic(Messages.getString("SortedMerge.Log.LineNumber") + linesRead);
  return true;
}

{
  meta=(CubeOutputMeta)smi;
  data=(CubeOutputData)sdi;
  Object[] r;
  boolean result=true;
  r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (meta.isDoNotOpenNewFileInit()) {
    try {
      PrepareFile();
      data.oneFileOpened=true;
    }
 catch (    IOException ioe) {
      logError(Messages.getString("CubeOutput.Log.ErrorOpeningCubeOutputFile") + ioe.toString());
      setErrors(1);
      return false;
    }
  }
  result=writeRowToFile(r);
  if (!result) {
    setErrors(1);
    stopAll();
    return false;
  }
  putRow(data.outputMeta,r);
  if (checkFeedback(linesOutput))   logBasic(Messages.getString("CubeOutput.Log.LineNumber") + linesOutput);
  return result;
}

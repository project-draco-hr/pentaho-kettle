{
  try {
    if (first) {
      data.outputMeta=getInputRowMeta().clone();
      data.outputMeta.writeMeta(data.dos);
      first=false;
    }
    data.outputMeta.writeData(data.dos,r);
  }
 catch (  Exception e) {
    logError(Messages.getString("CubeOutput.Log.ErrorWritingLine") + e.toString());
    return false;
  }
  linesOutput++;
  return true;
}

{
  if (data.oneFileOpened) {
    try {
      if (data.dos != null) {
        data.dos.close();
        data.dos=null;
      }
      if (data.zip != null) {
        data.zip.close();
        data.zip=null;
      }
      if (data.fos != null) {
        data.fos.close();
        data.fos=null;
      }
    }
 catch (    IOException e) {
      logError(Messages.getString("CubeOutput.Log.ErrorClosingFile") + meta.getFilename());
      setErrors(1);
      stopAll();
    }
  }
  super.dispose(smi,sdi);
}

{
  this.log=lw;
  if (name != null)   setName(name + " (" + super.getName()+ ")");
  jobMeta=new JobMeta(log);
  jobMeta.setName(name);
  jobMeta.setFilename(file);
  jobMeta.setArguments(args);
  active=false;
  stopped=false;
  jobTracker=new JobTracker(jobMeta);
  parentThread=Thread.currentThread();
  initialized=false;
}

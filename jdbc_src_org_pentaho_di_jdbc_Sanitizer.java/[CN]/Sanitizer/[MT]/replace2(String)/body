{
  char[] data=input.toCharArray();
  CharBuffer cb=CharBuffer.allocate(data.length);
  for (int i=0; i < data.length; i++) {
    char d=data[i];
    for (int j=0; j < validChars.length; j++) {
      if (d == validChars[j]) {
        cb.append(d);
        break;
      }
    }
  }
  String r=new String(cb.array());
  r=r.trim();
  return r;
}

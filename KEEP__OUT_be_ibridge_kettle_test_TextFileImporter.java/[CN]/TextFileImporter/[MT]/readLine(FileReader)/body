{
  String retval="";
  boolean eoln=false;
  try {
    int ch=fr.read();
    if (ch == '\n' || ch == '\r')     ch=fr.read();
    while (ch >= 0 && !eoln) {
      if (ch == '\n' || ch == '\r') {
        eoln=true;
      }
 else {
        retval+=(char)ch;
      }
      ch=fr.read();
    }
  }
 catch (  IOException e) {
    retval=null;
  }
  return retval;
}

{
  if (applyOnlyRadioButton.isSelected()) {
    List<UIRepositoryObject> roList=getSelectedRepositoryObject();
    applyOnObjectOnly(roList,true);
  }
 else {
    applyAclConfirmationDialog.hide();
    messageBox.setTitle(messages.getString("Dialog.Error"));
    messageBox.setAcceptLabel(messages.getString("Dialog.Ok"));
    messageBox.setMessage(messages.getString("PermissionsController.Error.FunctionalityNotSupported"));
    messageBox.open();
  }
}

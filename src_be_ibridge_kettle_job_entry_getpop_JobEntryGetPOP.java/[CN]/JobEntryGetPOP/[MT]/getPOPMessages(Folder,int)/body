{
  try {
    int unreadMsgs=folder.getUnreadMessageCount();
    int msgCount=folder.getMessageCount();
    Message msgsAll[]=folder.getMessages();
    if (retrievemails == 1) {
      Message msgsNew[]=folder.getMessages(msgCount - unreadMsgs + 1,msgCount);
      return (msgsNew);
    }
 else {
      return (msgsAll);
    }
  }
 catch (  Exception e) {
    return null;
  }
}

{
  if (connect()) {
    try {
      Folder folder=mailConn.getStore().getDefaultFolder();
      SelectFolderDialog s=new SelectFolderDialog(shell,SWT.NONE,folder);
      String foldername=s.open();
      if (foldername != null)       input.setText(foldername);
    }
 catch (    Exception e) {
    }
  }
}

{
  if (buffers.size() == 0)   return;
  int nr=wStepname.getSelectionIndex();
  ArrayList buffer=(ArrayList)buffers.get(nr);
  String name=(String)names.get(nr);
  if (buffer.size() > 0) {
    PreviewRowsDialog prd=new PreviewRowsDialog(shell,SWT.NONE,name,buffer);
    prd.open();
  }
 else {
    MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_WARNING);
    mb.setMessage(Messages.getString("EnterPreviewRowsDialog.Dialog.NoRowsFound.Message"));
    mb.setText(Messages.getString("EnterPreviewRowsDialog.Dialog.NoRowsFound.Title"));
    mb.open();
  }
}

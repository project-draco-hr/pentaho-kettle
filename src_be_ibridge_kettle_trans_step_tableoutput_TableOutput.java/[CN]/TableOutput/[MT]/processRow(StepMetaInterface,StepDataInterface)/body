{
  meta=(TableOutputMeta)smi;
  data=(TableOutputData)sdi;
  Row r;
  r=getRow();
  if (r == null) {
    return false;
  }
  try {
    writeToTable(r);
    if (!r.isIgnored()) {
      putRow(r);
      linesOutput++;
    }
    if (checkFeedback(linesOutput))     logBasic("linenr " + linesOutput);
  }
 catch (  KettleException e) {
    logError("Because of an error, this step can't continue: " + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

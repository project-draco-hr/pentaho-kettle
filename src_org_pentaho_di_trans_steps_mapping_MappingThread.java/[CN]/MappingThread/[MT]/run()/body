{
  try {
    RowMetaAndData row=getRow();
    while (row != null && !mapping.isStopped()) {
      targetStep.putRow(row.getRowMeta(),row.getData());
      row=getRow();
    }
  }
 catch (  Exception e) {
    log.logError(toString(),"Unexpected error : " + e.toString());
    log.logError(toString(),Const.getStackTracker(e));
    mapping.setErrors(1);
    mapping.stopAll();
  }
}

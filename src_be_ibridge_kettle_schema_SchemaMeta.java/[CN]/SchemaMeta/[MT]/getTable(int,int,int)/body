{
  int i, s;
  s=nrTables();
  for (i=s - 1; i >= 0; i--) {
    TableMeta ti=(TableMeta)tables.get(i);
    if (ti.isDrawn()) {
      Point p=ti.getLocation();
      if (p != null) {
        if (x >= p.x && x <= p.x + iconsize && y >= p.y && y <= p.y + iconsize) {
          return ti;
        }
      }
    }
  }
  return null;
}

{
  meta=(SortedMergeMeta)smi;
  data=(SortedMergeData)sdi;
  Row row=getRowSorted();
  if (row == null) {
    setOutputDone();
    return false;
  }
  putRow(row);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("SortedMerge.Log.LineNumber") + linesRead);
  return true;
}

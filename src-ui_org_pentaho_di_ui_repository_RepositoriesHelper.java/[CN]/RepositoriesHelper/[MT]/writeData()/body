{
  try {
    input.writeData();
  }
 catch (  Exception e) {
    log.logDetailed("Error deleting repository : " + e.getLocalizedMessage());
    messageBox.setTitle(messages.getString("Dialog.Error"));
    messageBox.setAcceptLabel(messages.getString("Dialog.Ok"));
    messageBox.setMessage(messages.getString("RepositoryLogin.ErrorSavingRepositoryDefinition"));
    messageBox.open();
  }
}

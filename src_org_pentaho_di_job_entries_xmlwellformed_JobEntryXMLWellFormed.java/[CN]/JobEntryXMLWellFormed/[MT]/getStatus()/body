{
  boolean retval=false;
  int limitFiles=Const.toInt(environmentSubstitute(getNrErrorsLessThan()),10);
  if ((NrBadFormed == 0 && getSuccessCondition().equals(SUCCESS_IF_ALL_FILES_WELL_FORMED)) || (NrWellFormed >= limitFiles && getSuccessCondition().equals(SUCCESS_IF_AT_LEAST_X_FILES_WELL_FORMED)) || (NrBadFormed <= limitFiles && getSuccessCondition().equals(SUCCESS_IF_BAD_FORMED_FILES_LESS))) {
    retval=true;
  }
  return retval;
}

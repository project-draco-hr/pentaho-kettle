{
  meta=(FieldSplitterMeta)smi;
  data=(FieldSplitterData)sdi;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  boolean noerr=splitField(r);
  if (!noerr) {
    setOutputDone();
    return false;
  }
  putRow(r);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("FieldSplitter.Log.LineNumber") + linesRead);
  return true;
}

{
  SAXParserFactory spf=SAXParserFactory.newInstance();
  try {
    SAXParser sp=spf.newSAXParser();
    sp.parse(sourceFile,this);
  }
 catch (  SAXException se) {
    se.printStackTrace();
  }
catch (  ParserConfigurationException pce) {
    pce.printStackTrace();
  }
catch (  IOException ie) {
    ie.printStackTrace();
  }
}

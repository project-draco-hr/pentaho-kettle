{
  try {
    logBasic(Messages.getString("System.Log.StartingToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Throwable t) {
    logError(Messages.getString("System.Log.UnexpectedError") + " : ",t);
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

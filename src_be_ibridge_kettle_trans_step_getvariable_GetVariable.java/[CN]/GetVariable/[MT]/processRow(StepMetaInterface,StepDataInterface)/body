{
  Row row;
  if (data.readsRows) {
    row=getRow();
    if (row == null) {
      setOutputDone();
      return false;
    }
  }
 else {
    row=new Row();
    linesRead++;
  }
  for (int i=0; i < meta.getFieldName().length; i++) {
    Value v=new Value(meta.getFieldName()[i],StringUtil.environmentSubstitute(meta.getVariableString()[i]));
    row.addValue(v);
  }
  putRow(row);
  if (!data.readsRows) {
    setOutputDone();
    return false;
  }
  return true;
}

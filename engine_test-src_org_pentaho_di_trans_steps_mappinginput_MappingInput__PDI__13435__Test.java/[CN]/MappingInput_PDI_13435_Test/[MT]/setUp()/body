{
  StepMockHelper<MappingInputMeta,MappingInputData> stepMockHelper=new StepMockHelper<MappingInputMeta,MappingInputData>("MappingInput",MappingInputMeta.class,MappingInputData.class);
  when(stepMockHelper.logChannelInterfaceFactory.create(any(),any(LoggingObjectInterface.class))).thenReturn(stepMockHelper.logChannelInterface);
  data=new MappingInputData();
  data.linked=true;
  data.valueRenames=Collections.singletonList(new MappingValueRename("source","target"));
  input=new MappingInput(stepMockHelper.stepMeta,data,0,stepMockHelper.transMeta,stepMockHelper.trans);
  input=spy(input);
  doReturn(new Object[0]).when(input).getRow();
  doNothing().when(input).putRow(any(RowMetaInterface.class),any(Object[].class));
}

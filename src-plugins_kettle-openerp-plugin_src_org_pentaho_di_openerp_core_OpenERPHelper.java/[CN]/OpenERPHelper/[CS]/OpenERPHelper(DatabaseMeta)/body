{
  openERPConnection=new Session(databaseMeta.environmentSubstitute(databaseMeta.getHostname()),Integer.parseInt(databaseMeta.environmentSubstitute(databaseMeta.getDatabasePortNumberString())),databaseMeta.environmentSubstitute(databaseMeta.getDatabaseName()),databaseMeta.environmentSubstitute(databaseMeta.getUsername()),databaseMeta.environmentSubstitute(databaseMeta.getPassword()));

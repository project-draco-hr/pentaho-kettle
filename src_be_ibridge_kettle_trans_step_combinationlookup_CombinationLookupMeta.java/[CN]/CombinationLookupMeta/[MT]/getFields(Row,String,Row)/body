{
  Row row;
  if (r == null)   row=new Row();
 else   row=r;
  Value v=new Value(technicalKeyField,Value.VALUE_TYPE_INTEGER);
  v.setLength(10,0);
  v.setOrigin(name);
  row.addValue(v);
  if (replaceFields) {
    for (int i=0; i < keyField.length; i++) {
      int idx=r.searchValueIndex(keyField[i]);
      if (idx >= 0) {
        row.removeValue(idx);
      }
    }
  }
  return row;
}

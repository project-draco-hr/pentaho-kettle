{
  TimedTransRunner timedTransRunner=new TimedTransRunner("test/org/pentaho/di/run/scriptvalues_mod/ScriptValuesMod2.ktr",LogWriter.LOG_LEVEL_ERROR,AllRunTests.getOldTargetDatabase(),AllRunTests.getNewTargetDatabase(),500000);
  timedTransRunner.init();
  timedTransRunner.runNewEngine(true);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 0);
}

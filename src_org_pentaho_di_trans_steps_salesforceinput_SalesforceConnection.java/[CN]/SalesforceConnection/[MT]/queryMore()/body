{
  try {
    if (!this.qr.isDone()) {
      setQueryResult(this.binding.queryMore(this.qr.getQueryLocator()));
      this.queryResultSize=this.qr.getSize();
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    throw new KettleException(BaseMessages.getString(PKG,"SalesforceInput.Error.QueringMore"),e);
  }
}

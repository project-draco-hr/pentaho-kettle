{
  try {
    Display.getDefault().asyncExec(new Runnable(){
      @Override public void run(){
        tipsDialog.open();
      }
    }
);
  }
 catch (  Exception e) {
    fail("Test failed due to exception: " + e.getLocalizedMessage());
  }
  try {
    Display.getDefault().asyncExec(new Runnable(){
      @Override public void run(){
        tipsDialog.dispose();
      }
    }
);
  }
 catch (  Exception e) {
    fail("Test failed due to exception: " + e.getLocalizedMessage());
  }
}

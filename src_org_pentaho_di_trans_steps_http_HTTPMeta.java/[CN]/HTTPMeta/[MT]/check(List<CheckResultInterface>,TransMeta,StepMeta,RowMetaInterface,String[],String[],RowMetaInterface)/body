{
  CheckResult cr;
  if (input.length > 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("HTTPMeta.CheckResult.ReceivingInfoFromOtherSteps"),stepMeta);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,Messages.getString("HTTPMeta.CheckResult.NoInpuReceived"),stepMeta);
    remarks.add(cr);
  }
}

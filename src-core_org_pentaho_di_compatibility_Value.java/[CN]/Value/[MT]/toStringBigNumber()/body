{
  if (value == null)   return null;
  String retval;
  if (isNull()) {
    retval=Const.NULL_BIGNUMBER;
  }
 else {
    if (value.getBigNumber() == null) {
      retval=null;
    }
 else {
      retval=value.getString();
      if (Const.DEFAULT_DECIMAL_SEPARATOR != '.') {
        retval=retval.replace('.',Const.DEFAULT_DECIMAL_SEPARATOR);
      }
    }
  }
  return retval;
}

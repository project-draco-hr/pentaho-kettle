{
  if (isNull()) {
    setType(VALUE_TYPE_STRING);
  }
 else {
    if (getString() != null) {
      StringBuffer s;
      if (getType() == VALUE_TYPE_STRING) {
        s=new StringBuffer(getString());
      }
 else {
        s=new StringBuffer(toString());
      }
      while (s.length() > 0 && s.charAt(0) == ' ')       s.deleteCharAt(0);
      setValue(s);
    }
 else {
      setNull();
    }
  }
  return this;
}

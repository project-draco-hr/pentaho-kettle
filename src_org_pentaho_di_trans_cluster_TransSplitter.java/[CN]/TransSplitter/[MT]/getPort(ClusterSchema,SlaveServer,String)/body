{
  String key=clusterSchema.getName() + " - " + serverSocketSlaveServer+ " - "+ serverSocketStep+ " - ";
  int p;
  Integer port=(Integer)clusterStepPortMap.get(key);
  if (port == null) {
    p=getNextPort(clusterSchema);
    clusterStepPortMap.put(key,new Integer(p));
  }
 else {
    p=port.intValue();
  }
  return p;
}

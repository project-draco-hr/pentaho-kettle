{
  int idx=1;
  if (rows1.size() != rows2.size()) {
    fail("Number of rows is not the same: " + rows1.size() + " and "+ rows2.size());
  }
  Iterator it1=rows1.iterator();
  Iterator it2=rows2.iterator();
  while (it1.hasNext() && it2.hasNext()) {
    Row r1=(Row)it1.next();
    Row r2=(Row)it2.next();
    if (r1.compare(r2) != 0) {
      fail("row nr " + idx + "is not equal");
    }
    idx++;
  }
}

{
  String fields[]=new String[]{"",stepname,Integer.toString(copy),Long.toString(linesRead),Long.toString(linesWritten),Long.toString(linesInput),Long.toString(linesOutput),Long.toString(linesUpdated),Long.toString(linesRejected),Long.toString(errors),statusDescription,Double.toString(seconds),speed,priority};
  return fields;
}

{
  try {
    URL url=XulHelper.getAndValidate(XUL_FILE_TOOLBAR_PROPERTIES);
    Properties props=new Properties();
    props.load(url.openStream());
    String ids[]=toolbar.getMenuItemIds();
    for (int i=0; i < ids.length; i++) {
      String methodName=(String)props.get(ids[i]);
      if (methodName != null) {
        toolbar.addMenuListener(ids[i],this,methodName);
      }
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
    new ErrorDialog(shell,BaseMessages.getString(PKG,"RepositoriesDialog.Exception.ErrorReadingXULFile.Title"),BaseMessages.getString(PKG,"RepositoriesDialog.Exception.ErrorReadingXULFile.Message",XUL_FILE_TOOLBAR_PROPERTIES),new Exception(t));
  }
}

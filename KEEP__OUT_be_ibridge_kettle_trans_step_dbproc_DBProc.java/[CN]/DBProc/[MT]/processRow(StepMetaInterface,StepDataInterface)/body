{
  meta=(DBProcMeta)smi;
  data=(DBProcData)sdi;
  boolean sendToErrorRow=false;
  String errorMessage=null;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  try {
    runProc(r);
    putRow(r);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("DBProc.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    if (getStepMeta().isDoingErrorHandling()) {
      sendToErrorRow=true;
      errorMessage=e.toString();
    }
 else {
      logError(Messages.getString("DBProc.ErrorInStepRunning") + e.getMessage());
      setErrors(1);
      stopAll();
      setOutputDone();
      return false;
    }
    if (sendToErrorRow) {
      putError(r,1,errorMessage,null,"TOP001");
      r.setIgnore();
    }
  }
  return true;
}

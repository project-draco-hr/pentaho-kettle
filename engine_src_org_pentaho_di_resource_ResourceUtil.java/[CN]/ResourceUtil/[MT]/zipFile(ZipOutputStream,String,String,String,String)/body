{
  ZipEntry zipEntry=new ZipEntry(filename);
  String comment=BaseMessages.getString(PKG,"ResourceUtil.SerializeResourceExportInterface.ZipEntryComment.OriginatingFile",fullname,Const.NVL(originalPath,"-"));
  zipEntry.setComment(comment);
  out.putNextEntry(zipEntry);
  out.write(context.getBytes());
  out.closeEntry();
}

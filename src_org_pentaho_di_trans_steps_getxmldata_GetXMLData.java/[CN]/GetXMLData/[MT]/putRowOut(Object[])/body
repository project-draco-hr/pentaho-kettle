{
  if (log.isRowLevel())   logRowlevel(BaseMessages.getString(PKG,"GetXMLData.Log.ReadRow",getInputRowMeta().getString(r)));
  incrementLinesInput();
  data.rownr++;
  putRow(data.outputRowMeta,r);
  if (meta.getRowLimit() > 0 && data.rownr > meta.getRowLimit()) {
    setOutputDone();
    return false;
  }
  return true;
}

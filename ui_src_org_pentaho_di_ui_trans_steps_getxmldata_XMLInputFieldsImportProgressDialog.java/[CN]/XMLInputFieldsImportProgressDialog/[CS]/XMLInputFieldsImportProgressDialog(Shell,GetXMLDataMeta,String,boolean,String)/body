{
  this.shell=shell;
  this.meta=meta;
  this.fields=null;
  this.filename=null;
  this.encoding=null;
  this.nr=0;
  this.loopXPath=loopXPath;
  this.list=new HashSet<String>();
  this.fieldsList=new ArrayList<RowMetaAndData>();
  if (useUrl) {
    this.xml=null;
    this.url=xmlSource;
  }
 else {
    this.xml=xmlSource;
    this.url=null;
  }
}

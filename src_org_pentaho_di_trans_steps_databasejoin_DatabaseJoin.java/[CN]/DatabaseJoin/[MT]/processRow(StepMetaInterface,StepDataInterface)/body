{
  meta=(DatabaseJoinMeta)smi;
  data=(DatabaseJoinData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  try {
    lookupValues(getInputRowMeta(),r);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("DatabaseJoin.Log.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    logError(Messages.getString("DatabaseJoin.Log.ErrorInStepRunning") + e.getMessage());
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

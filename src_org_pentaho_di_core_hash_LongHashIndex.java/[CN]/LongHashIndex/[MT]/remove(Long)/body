{
  int hash=generateHashCode(key);
  int i=hash & (table.length - 1);
  Entry prev=table[i], e=prev;
  while (e != null) {
    Entry next=e.next;
    if (e.key.longValue() == key.longValue()) {
      size--;
      if (prev == e)       table[i]=next;
 else       prev.next=next;
      return;
    }
    prev=e;
    e=next;
  }
}

{
  Row row;
  if (r == null)   row=new Row();
 else   row=r;
  if (getTargetField() != null && getTargetField().length() > 0) {
    Value extra=new Value(getTargetField(),Value.VALUE_TYPE_STRING);
    int maxlen=-1;
    for (int i=0; i < sourceValue.length; i++) {
      if (sourceValue[i] != null && sourceValue[i].length() > maxlen)       maxlen=sourceValue[i].length();
    }
    extra.setLength(maxlen);
    extra.setOrigin(name);
    row.addValue(extra);
  }
  return row;
}

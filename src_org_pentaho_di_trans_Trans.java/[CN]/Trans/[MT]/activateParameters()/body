{
  String[] keys=listParameters();
  for (  String key : keys) {
    String value=getParameterValue(key);
    String defValue=getParameterDefault(key);
    if (Const.isEmpty(value)) {
      setVariable(key,Const.NVL(defValue,""));
    }
 else {
      setVariable(key,Const.NVL(value,""));
    }
  }
}

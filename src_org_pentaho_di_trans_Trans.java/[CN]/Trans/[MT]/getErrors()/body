{
  if (steps == null)   return 0;
  int errors=0;
  for (int i=0; i < steps.size(); i++) {
    StepMetaDataCombi sid=steps.get(i);
    if (sid.step.getErrors() != 0L)     errors++;
  }
  if (errors > 0)   log.logError(toString(),Messages.getString("Trans.Log.TransformationErrorsDetected"));
  return errors;
}

{
  this.transMeta=transMeta;
  preview=false;
  previewSteps=null;
  previewSizes=null;
  log.logDetailed(toString(),Messages.getString("Trans.Log.TransformationIsPreloaded"));
  log.logDebug(toString(),Messages.getString("Trans.Log.NumberOfStepsToRun",String.valueOf(transMeta.nrSteps()),String.valueOf(transMeta.nrTransHops())));
  initializeVariablesFrom(transMeta);
}

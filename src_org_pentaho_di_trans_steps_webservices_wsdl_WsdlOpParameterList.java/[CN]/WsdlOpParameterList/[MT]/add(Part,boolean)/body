{
  List<WsdlOpParameter> params=getParameter(p,requestPart);
  for (  WsdlOpParameter op : params) {
    if (_headerNames.contains(op.getName().getLocalPart())) {
      op.setHeader();
    }
    if (requestPart) {
      op.setMode(WsdlOpParameter.ParameterMode.IN);
      add(op);
    }
 else {
      addOutputParameter(op);
    }
  }
  return true;
}

{
  meta=(CubeOutputMeta)smi;
  data=(CubeOutputData)sdi;
  Row r;
  boolean result=true;
  r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  result=writeRowToFile(r);
  if (!result) {
    setErrors(1);
    stopAll();
    return false;
  }
  putRow(r);
  if ((linesOutput > 0) && (linesOutput % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("CubeOutput.Log.LineNumber") + linesOutput);
  return result;
}

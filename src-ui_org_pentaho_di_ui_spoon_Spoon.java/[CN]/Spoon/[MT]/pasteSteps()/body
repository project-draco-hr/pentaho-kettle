{
  TransGraph transGraph=getActiveTransGraph();
  if (transGraph == null) {
    pasteTransformation();
  }
 else {
    TransMeta transMeta=(TransMeta)transGraph.getMeta();
    String clipcontent=fromClipboard();
    if (clipcontent != null) {
      Point lastMove=transGraph.getLastMove();
      if (lastMove != null) {
        pasteXML(transMeta,clipcontent,lastMove);
      }
    }
  }
}

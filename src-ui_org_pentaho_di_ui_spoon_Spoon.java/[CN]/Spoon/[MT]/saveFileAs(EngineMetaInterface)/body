{
  boolean saved=false;
  log.logBasic(toString(),Messages.getString("Spoon.Log.SaveAs"));
  if (rep != null) {
    meta.setID(-1L);
    saved=saveToRepository(meta,true);
    String sync=BasePropertyHandler.getProperty(SYNC_TRANS);
    if (Boolean.parseBoolean(sync))     delegates.tabs.renameTabs();
  }
 else {
    saved=saveXMLFile(meta,false);
    String sync=BasePropertyHandler.getProperty(SYNC_TRANS);
    if (Boolean.parseBoolean(sync))     delegates.tabs.renameTabs();
  }
  refreshTree();
  return saved;
}

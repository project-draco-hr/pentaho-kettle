{
  boolean saved=false;
  if (log.isBasic())   log.logBasic(toString(),BaseMessages.getString(PKG,"Spoon.Log.SaveAs"));
  if (rep != null) {
    meta.setID(-1L);
    saved=saveToRepository(meta,true);
  }
 else {
    saved=saveXMLFile(meta,false);
  }
  delegates.tabs.renameTabs();
  refreshTree();
  return saved;
}

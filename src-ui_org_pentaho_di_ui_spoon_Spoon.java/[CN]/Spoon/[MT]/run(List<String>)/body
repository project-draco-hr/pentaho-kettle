{
  try {
    createSplash();
    getCommandLineArgs(args);
    createSpoon();
    try {
      lcsup.onStart(this);
    }
 catch (    LifecycleException e) {
      MessageBox box=new MessageBox(shell,(e.isSevere() ? SWT.ICON_ERROR : SWT.ICON_WARNING) | SWT.OK);
      box.setMessage(e.getMessage());
      box.open();
    }
    setArguments(args.toArray(new String[args.size()]));
    start();
  }
 catch (  Throwable t) {
    log.logError(toString(),"Fatal error : " + Const.NVL(t.toString(),Const.NVL(t.getMessage(),"Unknown error")));
    log.logError(toString(),Const.getStackTracker(t));
    new ErrorDialog(shell,Messages.getString("Fatal error : "),"Fatal error : " + Const.NVL(t.toString(),Const.NVL(t.getMessage(),"Unknown error")),t);
  }
}

{
  try {
    EngineMetaInterface meta=getActiveMeta();
    if (meta != null) {
      if (meta.canSave()) {
        return saveFileAs(meta);
      }
    }
  }
 catch (  Exception e) {
    new ErrorDialog(shell,BaseMessages.getString(PKG,"Spoon.File.Save.Fail.Title"),BaseMessages.getString(PKG,"Spoon.File.Save.Fail.Message"),e);
  }
  return false;
}

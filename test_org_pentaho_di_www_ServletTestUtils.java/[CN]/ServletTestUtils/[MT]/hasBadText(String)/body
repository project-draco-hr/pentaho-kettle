{
  return value.indexOf('!') != -1 || value.indexOf('@') != -1 || value.indexOf('$') != -1 || value.indexOf('%') != -1 || value.indexOf('^') != -1 || value.indexOf('*') != -1 || value.indexOf('(') != -1 || value.indexOf(')') != -1 || value.indexOf('<') != -1 || value.indexOf('>') != -1 || value.indexOf('/') != -1;
}

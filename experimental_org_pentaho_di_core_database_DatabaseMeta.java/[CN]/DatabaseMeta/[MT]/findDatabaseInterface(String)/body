{
  DatabaseInterface di[]=getDatabaseInterfaces();
  for (int i=0; i < di.length; i++) {
    if (di[i].getDatabaseTypeDesc().equalsIgnoreCase(databaseTypeDesc) || di[i].getDatabaseTypeDescLong().equalsIgnoreCase(databaseTypeDesc))     return di[i];
  }
  throw new KettleDatabaseException("database type [" + databaseTypeDesc + "] couldn't be found!");
}

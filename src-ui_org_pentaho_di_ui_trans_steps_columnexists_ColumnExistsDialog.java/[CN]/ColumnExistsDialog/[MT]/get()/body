{
  try {
    wColumnName.removeAll();
    wTableName.removeAll();
    RowMetaInterface r=transMeta.getPrevStepFields(stepname);
    if (r != null) {
      r.getFieldNames();
      for (int i=0; i < r.getFieldNames().length; i++) {
        wTableName.add(r.getFieldNames()[i]);
        wColumnName.add(r.getFieldNames()[i]);
      }
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(shell,Messages.getString("ColumnExistsDialog.FailedToGetFields.DialogTitle"),Messages.getString("ColumnExistsDialog.FailedToGetFields.DialogMessage"),ke);
  }
}

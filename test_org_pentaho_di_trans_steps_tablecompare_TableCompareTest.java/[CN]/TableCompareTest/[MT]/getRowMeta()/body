{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMeta(ehlkd,ValueMeta.TYPE_STRING),new ValueMeta(ehlrvi,ValueMeta.TYPE_STRING),new ValueMeta(ehlcvif,ValueMeta.TYPE_STRING),new ValueMeta(schemaName,ValueMeta.TYPE_STRING),new ValueMeta(reference,ValueMeta.TYPE_STRING),new ValueMeta(compare,ValueMeta.TYPE_STRING),new ValueMeta(key,ValueMeta.TYPE_STRING),new ValueMeta(exclude,ValueMeta.TYPE_STRING)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMetaString(ehlkd),new ValueMetaString(ehlrvi),new ValueMetaString(ehlcvif),new ValueMetaString(schemaName),new ValueMetaString(reference),new ValueMetaString(compare),new ValueMetaString(key),new ValueMetaString(exclude)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

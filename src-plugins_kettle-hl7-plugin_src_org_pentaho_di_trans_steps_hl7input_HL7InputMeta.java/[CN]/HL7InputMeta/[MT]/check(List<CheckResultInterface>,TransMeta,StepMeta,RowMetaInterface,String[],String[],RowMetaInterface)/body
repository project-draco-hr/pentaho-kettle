{
  CheckResult cr;
  if (prev == null || prev.size() == 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_WARNING,BaseMessages.getString(PKG,"HL7InputMeta.CheckResult.NotReceivingFields"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,BaseMessages.getString(PKG,"HL7InputMeta.CheckResult.StepRecevingData",prev.size() + ""),stepinfo);
    remarks.add(cr);
  }
  if (input.length > 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,BaseMessages.getString(PKG,"HL7InputMeta.CheckResult.StepRecevingData2"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"HL7InputMeta.CheckResult.NoInputReceivedFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
}

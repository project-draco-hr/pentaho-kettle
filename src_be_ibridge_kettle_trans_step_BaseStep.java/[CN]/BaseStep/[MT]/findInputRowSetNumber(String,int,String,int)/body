{
  int i;
  for (i=0; i < input.size(); i++) {
    RowSet rs=(RowSet)input.get(i);
    if (rs.getOriginStepName().equalsIgnoreCase(from) && rs.getDestinationStepName().equalsIgnoreCase(to) && rs.getOriginStepCopy() == fromcopy && rs.getDestinationStepCopy() == tocopy)     return i;
  }
  return -1;
}

{
  String retval;
  if (isAlive()) {
    retval=Messages.getString("BaseStep.status.Running");
  }
 else {
    if (isInitialising()) {
      retval=Messages.getString("BaseStep.status.Init");
    }
 else {
      if (isStopped()) {
        retval=Messages.getString("BaseStep.status.Stopped");
      }
 else {
        retval=Messages.getString("BaseStep.status.Finished");
      }
    }
  }
  return retval;
}

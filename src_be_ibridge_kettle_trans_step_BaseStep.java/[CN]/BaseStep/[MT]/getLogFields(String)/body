{
  Row r=new Row();
  int i;
  Value sname=new Value(Messages.getString("BaseStep.ColumnName.Stepname"),"");
  sname.setLength(256);
  r.addValue(sname);
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.Copy"),0.0));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesReaded"),0.0));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesWritten"),0.0));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesUpdated"),0.0));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesSkipped"),0.0));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.Errors"),0.0));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.StartDate"),Const.MIN_DATE));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.EndDate"),Const.MAX_DATE));
  for (i=0; i < r.size(); i++) {
    r.getValue(i).setOrigin(comm);
  }
  return r;
}

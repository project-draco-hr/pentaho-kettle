{
  String sRC="";
  int iStartPos=strScript.indexOf(strFunction);
  if (iStartPos > 0) {
    iStartPos=strScript.indexOf('{',iStartPos);
    int iCounter=1;
    while (iCounter != 0) {
      if (strScript.charAt(iStartPos++) == '{')       iCounter++;
 else       if (strScript.charAt(iStartPos++) == '}')       iCounter--;
      sRC=sRC + strScript.charAt(iStartPos);
    }
  }
  return sRC;
}

{
  meta=(XMLInputMeta)smi;
  data=(XMLInputData)sdi;
  if (super.init(smi,sdi)) {
    data.files=meta.getFiles(this).getFiles();
    if (data.files == null || data.files.size() == 0) {
      logError(Messages.getString("XMLInput.Log.NoFiles"));
      return false;
    }
    data.rownr=1L;
    return true;
  }
  return false;
}

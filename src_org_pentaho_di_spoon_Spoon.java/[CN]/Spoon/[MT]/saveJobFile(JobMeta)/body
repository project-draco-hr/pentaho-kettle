{
  log.logDetailed(toString(),"Save file...");
  boolean saved=false;
  if (rep != null) {
    saved=saveJobRepository(jobMeta);
  }
 else {
    if (jobMeta.getFilename() != null) {
      saved=saveJob(jobMeta,jobMeta.getFilename());
    }
 else {
      saved=saveJobFileAs(jobMeta);
    }
  }
  if (saved) {
    saved=saveJobSharedObjects(jobMeta);
  }
  return saved;
}

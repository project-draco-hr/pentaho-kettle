{
  List<List<String>> dataLinesList=new ArrayList<List<String>>();
  for (int i=0; i < 3; i++) {
    List<String> dl=new ArrayList<String>();
    dl.add("line" + ((i * 2) + 1));
    dl.add("line" + ((i * 2) + 2));
    dl.add("line" + ((i * 2) + 3));
    dataLinesList.add(dl);
  }
  return dataLinesList;
}

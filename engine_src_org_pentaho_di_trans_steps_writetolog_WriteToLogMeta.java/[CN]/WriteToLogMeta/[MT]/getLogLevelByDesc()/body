{
  if (loglevel == null)   return LogLevel.BASIC;
  LogLevel retval;
  if (loglevel.equals(logLevelCodes[0]))   retval=LogLevel.NOTHING;
 else   if (loglevel.equals(logLevelCodes[1]))   retval=LogLevel.ERROR;
 else   if (loglevel.equals(logLevelCodes[2]))   retval=LogLevel.MINIMAL;
 else   if (loglevel.equals(logLevelCodes[3]))   retval=LogLevel.BASIC;
 else   if (loglevel.equals(logLevelCodes[4]))   retval=LogLevel.DETAILED;
 else   if (loglevel.equals(logLevelCodes[5]))   retval=LogLevel.DEBUG;
 else   retval=LogLevel.ROWLEVEL;
  return retval;
}

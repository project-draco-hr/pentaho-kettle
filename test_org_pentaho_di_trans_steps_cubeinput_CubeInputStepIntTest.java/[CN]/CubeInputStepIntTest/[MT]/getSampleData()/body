{
  List<RowMetaAndData> rmd=new ArrayList<RowMetaAndData>();
  RowMeta rm=new RowMeta();
  rm.addValueMeta(new ValueMetaString("col1"));
  rmd.add(new RowMetaAndData(rm,new Object[]{"data1"}));

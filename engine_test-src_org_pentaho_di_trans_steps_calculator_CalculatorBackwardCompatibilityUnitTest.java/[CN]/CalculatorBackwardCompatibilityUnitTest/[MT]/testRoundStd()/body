{
  assertRoundStd(1.0,1.2);
  assertRoundStd(2.0,1.5);
  assertRoundStd(2.0,1.7);
  assertRoundStd(2.0,2.2);
  assertRoundStd(3.0,2.5);
  assertRoundStd(3.0,2.7);
  assertRoundStd(-1.0,-1.2);
  assertRoundStd(-2.0,-1.5);
  assertRoundStd(-2.0,-1.7);
  assertRoundStd(-2.0,-2.2);
  assertRoundStd(-3.0,-2.5);
  assertRoundStd(-3.0,-2.7);
}

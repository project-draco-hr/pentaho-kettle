{
  assertRound(1.0,1.2);
  assertRound(2.0,1.5);
  assertRound(2.0,1.7);
  assertRound(2.0,2.2);
  assertRound(3.0,2.5);
  assertRound(3.0,2.7);
  assertRound(-1.0,-1.2);
  assertRound(-1.0,-1.5);
  assertRound(-2.0,-1.7);
  assertRound(-2.0,-2.2);
  assertRound(-2.0,-2.5);
  assertRound(-3.0,-2.7);
}

{
  try {
    String string=rowMeta.getString(rows.get(currentIndex),index - 1);
    if (string == null) {
      lastNull=true;
    }
    lastNull=false;
    System.out.println("getString(" + index + ") --> "+ string);
    return string;
  }
 catch (  Exception e) {
    throw new SQLException(e);
  }
}

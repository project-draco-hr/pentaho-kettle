{
  try {
    if (this.isRevSupported != isRevSupported) {
      this.isRevSupported=isRevSupported;
      if (initComplete) {
        bindRevisionTable.fireSourceChanged();
        bindRevisionLabel.fireSourceChanged();
      }
    }
  }
 catch (  Exception e) {
    System.err.println(e.getMessage());
  }
}

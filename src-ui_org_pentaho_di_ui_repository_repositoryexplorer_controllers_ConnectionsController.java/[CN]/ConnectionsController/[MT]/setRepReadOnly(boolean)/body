{
  try {
    if (this.isRepReadOnly != isRepReadOnly) {
      this.isRepReadOnly=isRepReadOnly;
      if (initComplete) {
        bindButtonNew.fireSourceChanged();
        bindButtonEdit.fireSourceChanged();
        bindButtonRemove.fireSourceChanged();
      }
    }
  }
 catch (  Exception e) {
    System.err.println(e.getMessage());
  }
}

{
  int i;
  NodeList children;
  Node childnode;
  String retval;
  if (n == null)   return null;
  children=n.getChildNodes();
  for (i=0; i < children.getLength(); i++) {
    childnode=children.item(i);
    retval=childnode.getNodeValue();
    if (retval != null) {
      return retval;
    }
  }
  return null;
}

{
  NodeList children;
  Node childnode;
  if (n == null)   return null;
  int count=0;
  children=n.getChildNodes();
  XMLHandlerCacheEntry entry=new XMLHandlerCacheEntry(n,tag);
  int lastChildNr=cache.getLastChildNr(entry);
  if (lastChildNr < 0) {
    lastChildNr=0;
  }
 else {
    count=nr;
    lastChildNr++;
  }
  for (int i=lastChildNr; i < children.getLength(); i++) {
    childnode=children.item(i);
    if (childnode.getNodeName().equalsIgnoreCase(tag)) {
      if (count == nr) {
        cache.storeCache(entry,i);
        return childnode;
      }
      count++;
    }
  }
  return null;
}

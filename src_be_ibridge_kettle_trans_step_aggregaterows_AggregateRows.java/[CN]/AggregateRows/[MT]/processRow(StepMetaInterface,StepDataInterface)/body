{
  meta=(AggregateRowsMeta)smi;
  data=(AggregateRowsData)sdi;
  Row r=null;
  r=getRow();
  if (r == null) {
    Row agg=buildAggregate();
    putRow(agg);
    setOutputDone();
    return false;
  }
  if (linesRead == 1) {
    for (int i=0; i < meta.getFieldName().length; i++) {
      data.fieldnrs[i]=r.searchValueIndex(meta.getFieldName()[i]);
      if (data.fieldnrs[i] < 0) {
        logError(Messages.getString("AggregateRows.Log.CouldNotFindField",meta.getFieldName()[i]));
        setErrors(1);
        stopAll();
        return false;
      }
      data.counts[i]=0L;
    }
  }
  AddAggregate(r);
  if (checkFeedback(linesRead))   logBasic(Messages.getString("AggregateRows.Log.LineNumber") + linesRead);
  return true;
}

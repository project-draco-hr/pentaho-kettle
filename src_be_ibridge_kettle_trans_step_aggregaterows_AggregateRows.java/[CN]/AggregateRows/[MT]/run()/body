{
  try {
    logBasic(Messages.getString("AggregateRows.Log.StartToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError(Messages.getString("AggregateRows.Log.UnexpectedErrorInfo") + debug + "' : "+ e.toString());
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    markStop();
    logSummary();
  }
}

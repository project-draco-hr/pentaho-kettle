{
  EnvUtil.environmentInit();
  Database h2db=new Database(h2meta);
  h2db.connect();
  try {
    h2db.execStatement("DROP TABLE CSV_TABLE;");
  }
 catch (  KettleDatabaseException e) {
  }
  h2db.execStatement("CREATE TABLE CSV_TABLE" + "(" + "  id INTEGER"+ ", name VARCHAR(9)"+ ", firstname VARCHAR(13)"+ ", zip INTEGER"+ ", city VARCHAR(8)"+ ", birthdate TIMESTAMP"+ ", street VARCHAR(11)"+ ", housenr SMALLINT"+ ", stateCode VARCHAR(9)"+ ", state VARCHAR(30)"+ ")"+ ";");
  try {
    h2db.execStatement("DROP TABLE SIMPLE_TABLE;");
  }
 catch (  KettleDatabaseException e) {
  }
  h2db.execStatement("CREATE TABLE SIMPLE_TABLE" + "(" + "  stringField VARCHAR(30)"+ ", dateField TIMESTAMP"+ ", boolField CHAR(1)"+ ", numField DOUBLE"+ ", intField DOUBLE"+ ", id INTEGER"+ ")"+ ";");
  h2db.disconnect();
}

{
  EnvUtil.environmentInit();
  Database target=new Database(getNewTargetDatabase());
  target.connect();
  try {
    target.execStatements("DROP TABLE CSV_TABLE;");
  }
 catch (  KettleDatabaseException e) {
  }
  target.execStatements("CREATE TABLE CSV_TABLE" + "(" + "  id INTEGER"+ ", name VARCHAR(9)"+ ", firstname VARCHAR(13)"+ ", zip INTEGER"+ ", city VARCHAR(8)"+ ", birthdate TIMESTAMP"+ ", street VARCHAR(11)"+ ", housenr SMALLINT"+ ", stateCode VARCHAR(9)"+ ", state VARCHAR(30)"+ ")"+ ";");
  try {
    target.execStatements("DROP TABLE SIMPLE_TABLE;");
  }
 catch (  KettleDatabaseException e) {
  }
  target.execStatements("CREATE TABLE SIMPLE_TABLE" + "(" + "  stringField VARCHAR(30)"+ ", dateField TIMESTAMP"+ ", boolField CHAR(1)"+ ", numField DOUBLE"+ ", intField DOUBLE"+ ", id INTEGER"+ ")"+ ";");
  target.disconnect();
}

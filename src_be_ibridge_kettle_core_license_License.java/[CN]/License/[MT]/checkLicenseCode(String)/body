{
  if (code == null || code.length() == 0)   return false;
  String oldCode=licenseCode;
  licenseCode=getSignature();
  String shortSignature=Encr.getSignatureShort(licenseCode);
  if (code.equalsIgnoreCase(shortSignature)) {
    return true;
  }
 else {
    licenseCode=oldCode;
    return false;
  }
}

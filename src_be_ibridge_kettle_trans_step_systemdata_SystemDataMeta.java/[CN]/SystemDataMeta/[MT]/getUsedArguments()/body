{
  Row row=new Row();
  for (int argNr=0; argNr < 10; argNr++) {
    boolean found=false;
    for (int i=0; i < fieldName.length; i++) {
      if (fieldType[i] == TYPE_SYSTEM_INFO_ARGUMENT_01 + argNr)       found=true;
    }
    if (found) {
      Value value=new Value("" + (argNr + 1),Value.VALUE_TYPE_STRING);
      row.addValue(value);
    }
  }
  return row;
}

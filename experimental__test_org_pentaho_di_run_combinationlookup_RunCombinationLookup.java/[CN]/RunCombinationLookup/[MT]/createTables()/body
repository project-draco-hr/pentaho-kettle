{
  EnvUtil.environmentInit();
  Database h2db=new Database(h2meta);
  h2db.connect();
  try {
    h2db.execStatement("DROP TABLE COMB_NO_HASH;");
  }
 catch (  KettleDatabaseException e) {
  }
  h2db.execStatements("CREATE TABLE COMB_NO_HASH" + "(" + "      comb_tk IDENTITY"+ "    , stateCode VARCHAR(9)"+ "    , state VARCHAR(30)"+ "    )"+ "    ;" + Const.CR + "    CREATE UNIQUE INDEX idx_COMB_NO_HASH_pk"+ "     ON COMB_NO_HASH"+ "    ( "+ "      comb_tk"+ "    )"+ "    ;"+ Const.CR+ "    CREATE INDEX idx_COMB_NO_HASH_lookup"+ "     ON COMB_NO_HASH"+ "    ( "+ "      stateCode, state"+ "    )"+ "    ;");
  try {
    h2db.execStatement("DROP TABLE COMB_HASH;");
  }
 catch (  KettleDatabaseException e) {
  }
  h2db.execStatements("CREATE TABLE COMB_HASH" + "(" + "      comb_tk IDENTITY"+ "    , stateCode VARCHAR(9)"+ "    , state VARCHAR(30)"+ "    , hashcode BIGINT"+ "    )"+ "    ;" + Const.CR + "    CREATE UNIQUE INDEX idx_COMB_HASH_pk"+ "     ON COMB_NO_HASH"+ "    ( "+ "      comb_tk"+ "    )"+ "    ;"+ Const.CR+ "    CREATE INDEX idx_COMB_HASH_lookup"+ "     ON COMB_HASH"+ "    ( "+ "      hashcode"+ "    )"+ "    ;");
  h2db.disconnect();
}

{
  this.parentLoggingObject=null;
  this.databaseMeta=databaseMeta;
  shareVariablesWith(databaseMeta);
  log=new LogChannel(this);
  logLevel=log.getLogLevel();
  containerObjectId=log.getContainerObjectId();
  pstmt=null;
  rowMeta=null;
  dbmd=null;
  rowlimit=0;
  written=0;
  if (log.isDetailed())   log.logDetailed(BaseMessages.getString(PKG,"DatabaseMeta.Info.NewConnectionDefined"));
}

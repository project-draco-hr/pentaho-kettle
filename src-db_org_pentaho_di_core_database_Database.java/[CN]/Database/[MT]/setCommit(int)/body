{
  commitsize=commsize;
  String onOff=(commitsize <= 0 ? "on" : "off");
  try {
    connection.setAutoCommit(commitsize <= 0);
    if (log.isDetailed())     log.logDetailed(toString(),"Auto commit " + onOff);
  }
 catch (  Exception e) {
    log.logError(toString(),"Can't turn auto commit " + onOff);
  }
}

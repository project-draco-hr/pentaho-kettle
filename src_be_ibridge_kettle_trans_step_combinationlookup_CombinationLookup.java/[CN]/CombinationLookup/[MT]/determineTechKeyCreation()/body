{
  String keyCreation=meta.getTechKeyCreation();
  if (meta.getDatabase().supportsAutoinc() && CombinationLookupMeta.CREATION_METHOD_AUTOINC.equals(keyCreation)) {
    setTechKeyCreation(CREATION_METHOD_AUTOINC);
  }
 else   if (meta.getDatabase().supportsSequences() && CombinationLookupMeta.CREATION_METHOD_SEQUENCE.equals(keyCreation)) {
    setTechKeyCreation(CREATION_METHOD_SEQUENCE);
  }
 else {
    setTechKeyCreation(CREATION_METHOD_TABLEMAX);
  }
}

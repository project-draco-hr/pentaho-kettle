{
  if (super.init(sii,sdi)) {
    data.cache=new Hashtable();
    data.db=new Database(meta.getDatabase());
    try {
      data.db.connect();
      logBasic(Messages.getString("CombinationLookup.Log.ConnectedToDB"));
      data.db.setCommit(meta.getCommitSize());
      return true;
    }
 catch (    KettleDatabaseException dbe) {
      logError(Messages.getString("CombinationLookup.Log.UnableToConnectDB") + dbe.getMessage());
    }
  }
  return false;
}

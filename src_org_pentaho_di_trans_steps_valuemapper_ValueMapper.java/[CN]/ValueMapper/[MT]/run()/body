{
  try {
    logBasic(Messages.getString("ValueMapper.Log.StartingToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Throwable t) {
    logError(Messages.getString("ValueMapper.RuntimeError.UnexpectedError.VALUEMAPPER0003",t.toString()));
    logError(Const.getStackTracker(t));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

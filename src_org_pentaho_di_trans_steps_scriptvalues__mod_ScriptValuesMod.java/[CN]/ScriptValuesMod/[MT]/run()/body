{
  try {
    logBasic(Messages.getString("System.Log.StartingToRun"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Throwable t) {
    try {
      if (data.cx != null)       Context.exit();
    }
 catch (    Exception er) {
    }
    ;
    logError(Messages.getString("System.Log.UnexpectedError") + " : " + t.toString());
    logError(Const.getStackTracker(t));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

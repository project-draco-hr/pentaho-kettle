{
switch (columnIndex) {
case 0:
    return BaseMessages.getString(PKG,"OpenERPObjectInputDialog.TableViewFilterOperator");
case 1:
  return BaseMessages.getString(PKG,"OpenERPObjectInputDialog.TableViewFilterField");

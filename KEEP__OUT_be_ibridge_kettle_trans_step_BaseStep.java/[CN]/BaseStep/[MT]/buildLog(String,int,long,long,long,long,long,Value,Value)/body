{
  Row r=new Row();
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.Stepname"),sname));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.Copy"),(double)copynr));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesReaded"),(double)lines_read));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesWritten"),(double)lines_written));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesUpdated"),(double)lines_updated));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.LinesSkipped"),(double)lines_skipped));
  r.addValue(new Value(Messages.getString("BaseStep.ColumnName.Errors"),(double)errors));
  r.addValue(start_date);
  r.addValue(end_date);
  return r;
}

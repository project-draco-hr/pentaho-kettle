{
  includeFilename=false;
  filenameField="";
  includeRowNumber=false;
  rowNumberField="";
  int nrfiles=0;
  int nrfields=0;
  allocate(nrfiles,nrfields);
  for (int i=0; i < nrfiles; i++) {
    fileName[i]="filename" + (i + 1);
    fileMask[i]="";
  }
  for (int i=0; i < nrfields; i++) {
    inputFields[i]=new XMLInputField("field" + (i + 1),null);
  }
  rowLimit=0L;
}

{
  meta=(SplitFieldToRowsMeta)smi;
  data=(SplitFieldToRowsData)sdi;
  Object[] r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  boolean ok=splitField(getInputRowMeta(),r);
  if (!ok) {
    setOutputDone();
    return false;
  }
  if (checkFeedback(getLinesRead())) {
    if (log.isBasic()) {
      if (log.isBasic())       logBasic(Messages.getString("SplitFieldToRows.Log.LineNumber") + getLinesRead());
    }
  }
  return true;
}

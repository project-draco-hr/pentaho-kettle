  assertRoundCustom1(1.0,1.5,BigDecimal.ROUND_DOWN);
  assertRoundCustom1(2.0,1.5,BigDecimal.ROUND_CEILING);
  assertRoundCustom1(1.0,1.5,BigDecimal.ROUND_FLOOR);

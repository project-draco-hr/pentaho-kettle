{
  assertRound2(1.0,1.2,0);
  assertRound2(2.0,1.5,0);
  assertRound2(2.0,1.7,0);
  assertRound2(2.0,2.2,0);
  assertRound2(3.0,2.5,0);
  assertRound2(3.0,2.7,0);
  assertRound2(-1.0,-1.2,0);
  assertRound2(-1.0,-1.5,0);
  assertRound2(-2.0,-1.7,0);
  assertRound2(-2.0,-2.2,0);
  assertRound2(-2.0,-2.5,0);
  assertRound2(-3.0,-2.7,0);
}

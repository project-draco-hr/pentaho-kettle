{
  colorBackground.dispose();
  colorGraph.dispose();
  colorTab.dispose();
  colorRed.dispose();
  colorGreen.dispose();
  colorBlue.dispose();
  colorGray.dispose();
  colorYellow.dispose();
  colorMagenta.dispose();
  colorOrange.dispose();
  colorWhite.dispose();
  colorDemoGray.dispose();
  colorLightGray.dispose();
  colorDarkGray.dispose();
  colorBlack.dispose();
  colorDirectory.dispose();
  if (!reload) {
    fontGraph.dispose();
    fontNote.dispose();
    fontFixed.dispose();
    fontLarge.dispose();
    fontBold.dispose();
    imageHop.dispose();
    imageConnection.dispose();
    imageBol.dispose();
    imageKettle.dispose();
    imageCredits.dispose();
    imageStart.dispose();
    imageDummy.dispose();
    imageSpoon.dispose();
    imageChef.dispose();
    imageSplash.dispose();
    imagePentaho.dispose();
    Collection images=imagesSteps.values();
    for (Iterator iter=images.iterator(); iter.hasNext(); ) {
      Image image=(Image)iter.next();
      if (image != null && !image.isDisposed())       image.dispose();
    }
    Collection smallImages=imagesStepsSmall.values();
    for (Iterator iter=smallImages.iterator(); iter.hasNext(); ) {
      Image smallImage=(Image)iter.next();
      if (smallImage != null && !smallImage.isDisposed())       smallImage.dispose();
    }
  }
}

{
  if (data.wasStarted) {
    data.mappingTrans.waitUntilFinished();
    try {
      data.mappingTrans.endProcessing(Database.LOG_STATUS_END);
    }
 catch (    KettleException e) {
      log.logError(toString(),BaseMessages.getString(PKG,"Mapping.Log.UnableToLogEndOfTransformation") + e.toString());
    }
    if (data.mappingTrans.getErrors() > 0) {
      logError(BaseMessages.getString(PKG,"Mapping.Log.ErrorOccurredInSubTransformation"));
      setErrors(1);
    }
  }
  super.dispose(smi,sdi);
}

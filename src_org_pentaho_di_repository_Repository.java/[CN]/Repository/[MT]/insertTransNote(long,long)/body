{
  RowMetaAndData table=new RowMetaAndData();
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_NOTE_ID_TRANSFORMATION,ValueMetaInterface.TYPE_INTEGER),new Long(id_transformation));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_NOTE_ID_NOTE,ValueMetaInterface.TYPE_INTEGER),new Long(id_note));
  connectionDelegate.insertTableRow(Repository.TABLE_R_TRANS_NOTE,table);
}

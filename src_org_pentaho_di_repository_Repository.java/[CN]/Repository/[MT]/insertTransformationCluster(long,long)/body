{
  long id=connectionDelegate.getNextTransformationClusterID();
  RowMetaAndData table=new RowMetaAndData();
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_CLUSTER_ID_TRANS_CLUSTER,ValueMetaInterface.TYPE_INTEGER),new Long(id));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_CLUSTER_ID_TRANSFORMATION,ValueMetaInterface.TYPE_INTEGER),new Long(id_transformation));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_CLUSTER_ID_CLUSTER,ValueMetaInterface.TYPE_INTEGER),new Long(id_cluster));
  connectionDelegate.insertTableRow(Repository.TABLE_R_TRANS_CLUSTER,table);
  return id;
}

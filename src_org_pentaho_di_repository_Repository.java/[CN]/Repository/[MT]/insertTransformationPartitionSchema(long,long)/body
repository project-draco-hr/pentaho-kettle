{
  long id=connectionDelegate.getNextTransformationPartitionSchemaID();
  RowMetaAndData table=new RowMetaAndData();
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_PARTITION_SCHEMA_ID_TRANS_PARTITION_SCHEMA,ValueMetaInterface.TYPE_INTEGER),new Long(id));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_PARTITION_SCHEMA_ID_TRANSFORMATION,ValueMetaInterface.TYPE_INTEGER),new Long(id_transformation));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_PARTITION_SCHEMA_ID_PARTITION_SCHEMA,ValueMetaInterface.TYPE_INTEGER),new Long(id_partition_schema));
  connectionDelegate.insertTableRow(Repository.TABLE_R_TRANS_PARTITION_SCHEMA,table);
  return id;
}

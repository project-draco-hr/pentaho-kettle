{
  String sql="SELECT DISTINCT " + quote(Repository.FIELD_TRANS_CLUSTER_ID_TRANSFORMATION) + " FROM "+ quoteTable(Repository.TABLE_R_TRANS_CLUSTER)+ " WHERE "+ quote(Repository.FIELD_TRANS_CLUSTER_ID_CLUSTER)+ " = "+ id_cluster;
  return transDelegate.getTransformationsWithIDList(connectionDelegate.getRows(sql,100),connectionDelegate.getReturnRowMeta());
}

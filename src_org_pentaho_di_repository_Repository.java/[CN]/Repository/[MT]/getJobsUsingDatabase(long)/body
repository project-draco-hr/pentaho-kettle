{
  String sql="SELECT DISTINCT " + quote(Repository.FIELD_JOBENTRY_DATABASE_ID_JOB) + " FROM "+ quoteTable(Repository.TABLE_R_JOBENTRY_DATABASE)+ " WHERE "+ quote(Repository.FIELD_JOBENTRY_DATABASE_ID_DATABASE)+ " = "+ id_database;
  return jobDelegate.getJobsWithIDList(connectionDelegate.getRows(sql,100),connectionDelegate.getReturnRowMeta());
}

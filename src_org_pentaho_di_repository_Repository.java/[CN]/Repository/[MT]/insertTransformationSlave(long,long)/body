{
  long id=connectionDelegate.getNextTransformationSlaveID();
  RowMetaAndData table=new RowMetaAndData();
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_SLAVE_ID_TRANS_SLAVE,ValueMetaInterface.TYPE_INTEGER),new Long(id));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_SLAVE_ID_TRANSFORMATION,ValueMetaInterface.TYPE_INTEGER),new Long(id_transformation));
  table.addValue(new ValueMeta(Repository.FIELD_TRANS_SLAVE_ID_SLAVE,ValueMetaInterface.TYPE_INTEGER),new Long(id_slave));
  connectionDelegate.insertTableRow(Repository.TABLE_R_TRANS_SLAVE,table);
  return id;
}

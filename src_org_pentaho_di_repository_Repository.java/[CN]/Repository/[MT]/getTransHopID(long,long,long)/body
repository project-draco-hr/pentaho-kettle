{
  String lookupkey[]=new String[]{quote(FIELD_TRANS_HOP_ID_TRANSFORMATION),quote(FIELD_TRANS_HOP_ID_STEP_FROM),quote(FIELD_TRANS_HOP_ID_STEP_TO)};

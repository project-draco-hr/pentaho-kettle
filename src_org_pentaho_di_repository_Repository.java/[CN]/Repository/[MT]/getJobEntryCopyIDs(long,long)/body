{
  return connectionDelegate.getIDs("SELECT " + quote(Repository.FIELD_JOBENTRY_COPY_ID_JOBENTRY_COPY) + " FROM "+ quoteTable(Repository.TABLE_R_JOBENTRY_COPY)+ " WHERE "+ quote(Repository.FIELD_JOBENTRY_COPY_ID_JOB)+ " = "+ id_job+ " AND "+ quote(Repository.FIELD_JOBENTRY_COPY_ID_JOBENTRY)+ " = "+ id_jobentry);

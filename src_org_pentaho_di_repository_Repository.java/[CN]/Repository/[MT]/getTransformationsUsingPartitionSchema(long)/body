{
  String sql="SELECT DISTINCT " + quote(FIELD_TRANS_PARTITION_SCHEMA_ID_TRANSFORMATION) + " FROM "+ databaseMeta.getQuotedSchemaTableCombination(null,TABLE_R_TRANS_PARTITION_SCHEMA)+ " WHERE "+ quote(FIELD_TRANS_PARTITION_SCHEMA_ID_PARTITION_SCHEMA)+ " = "+ id_partition_schema;
  return getTransformationsWithIDList(database.getRows(sql,100),database.getReturnRowMeta());
}

{
  long id=connectionDelegate.getNextLogID();
  RowMetaAndData table=new RowMetaAndData();
  table.addValue(new ValueMeta(Repository.FIELD_REPOSITORY_LOG_ID_REPOSITORY_LOG,ValueMetaInterface.TYPE_INTEGER),new Long(id));
  table.addValue(new ValueMeta(Repository.FIELD_REPOSITORY_LOG_REP_VERSION,ValueMetaInterface.TYPE_STRING),getVersion());
  table.addValue(new ValueMeta(Repository.FIELD_REPOSITORY_LOG_LOG_DATE,ValueMetaInterface.TYPE_DATE),new Date());
  table.addValue(new ValueMeta(Repository.FIELD_REPOSITORY_LOG_LOG_USER,ValueMetaInterface.TYPE_STRING),userinfo != null ? userinfo.getLogin() : "admin");
  table.addValue(new ValueMeta(Repository.FIELD_REPOSITORY_LOG_OPERATION_DESC,ValueMetaInterface.TYPE_STRING),description);
  connectionDelegate.insertTableRow(Repository.TABLE_R_REPOSITORY_LOG,table);
  return id;
}

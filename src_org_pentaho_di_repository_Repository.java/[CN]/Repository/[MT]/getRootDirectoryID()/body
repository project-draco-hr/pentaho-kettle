{
  RowMetaAndData result=connectionDelegate.getOneRow("SELECT " + quote(Repository.FIELD_DIRECTORY_ID_DIRECTORY) + " FROM "+ quoteTable(Repository.TABLE_R_DIRECTORY)+ " WHERE "+ quote(Repository.FIELD_DIRECTORY_ID_DIRECTORY_PARENT)+ " = 0");
  if (result != null && result.isNumeric(0))   return result.getInteger(0,-1);
  return -1;
}

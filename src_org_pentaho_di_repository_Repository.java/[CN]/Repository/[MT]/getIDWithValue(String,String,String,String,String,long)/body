{
  RowMetaAndData par=new RowMetaAndData();
  par.addValue(new ValueMeta("value",ValueMetaInterface.TYPE_STRING),value);
  par.addValue(new ValueMeta("key",ValueMetaInterface.TYPE_INTEGER),new Long(key));
  RowMetaAndData result=database.getOneRow("SELECT " + databaseMeta.quoteField(idfield) + " FROM "+ databaseMeta.quoteField(tablename)+ " WHERE "+ databaseMeta.quoteField(lookupfield)+ " = ? AND "+ databaseMeta.quoteField(lookupkey)+ " = ?",par.getRowMeta(),par.getData());
  if (result != null && result.getRowMeta() != null && result.getData() != null && result.isNumeric(0))   return result.getInteger(0,0);
  return -1;
}

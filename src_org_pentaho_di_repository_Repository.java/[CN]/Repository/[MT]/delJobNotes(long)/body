{
  long ids[]=getJobNoteIDs(id_job);
  for (int i=0; i < ids.length; i++) {
    String sql="DELETE FROM " + quoteTable(Repository.TABLE_R_NOTE) + " WHERE "+ quote(Repository.FIELD_NOTE_ID_NOTE)+ " = "+ ids[i];
    execStatement(sql);
  }
  String sql="DELETE FROM " + quoteTable(Repository.TABLE_R_JOB_NOTE) + " WHERE "+ quote(Repository.FIELD_JOB_NOTE_ID_JOB)+ " = "+ id_job;
  execStatement(sql);
}

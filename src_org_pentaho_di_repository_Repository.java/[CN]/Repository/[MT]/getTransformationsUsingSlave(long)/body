  String sql="SELECT DISTINCT " + quote(FIELD_TRANS_SLAVE_ID_TRANSFORMATION) + " FROM "+ databaseMeta.getQuotedSchemaTableCombination(null,TABLE_R_TRANS_SLAVE)+ " WHERE "+ quote(FIELD_TRANS_SLAVE_ID_SLAVE)+ " = "+ id_slave;

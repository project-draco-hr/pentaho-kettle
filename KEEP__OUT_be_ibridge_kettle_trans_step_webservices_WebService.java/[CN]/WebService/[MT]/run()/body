{
  nbRowProcess=0;
  logBasic("Starting to run...");
  try {
    requestTime=0;
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError("Unexpected error in : " + e.toString());
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logBasic("Finished, processing " + linesRead + " rows");
    markStop();
  }
}

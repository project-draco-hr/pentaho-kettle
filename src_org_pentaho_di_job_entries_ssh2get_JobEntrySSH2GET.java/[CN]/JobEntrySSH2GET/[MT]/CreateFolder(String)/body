{
  FileObject folder=null;
  try {
    folder=KettleVFS.getFileObject(filefolder);
    if (!folder.exists()) {
      if (createtargetfolder) {
        folder.createFolder();
        if (log.isDetailed())         logDetailed(BaseMessages.getString(PKG,"JobSSH2GET.Log.FolderCreated",folder.toString()));
      }
 else       return false;
    }
    return true;
  }
 catch (  Exception e) {
    logError(BaseMessages.getString(PKG,"JobSSH2GET.Log.CanNotCreateFolder",folder.toString()));
  }
 finally {
    if (folder != null) {
      try {
        folder.close();
      }
 catch (      Exception ex) {
      }
      ;
    }
  }
  return false;
}

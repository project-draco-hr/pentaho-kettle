{
  boolean filterOK=true;
  if (meta.content.noEmptyLines && line.length() == 0) {
    filterOK=false;
  }
 else {
    filterOK=data.filterProcessor.doFilters(line);
    if (!filterOK) {
      if (data.filterProcessor.isStopProcessing()) {
        data.doneReading=true;
      }
    }
  }
  return filterOK;
}

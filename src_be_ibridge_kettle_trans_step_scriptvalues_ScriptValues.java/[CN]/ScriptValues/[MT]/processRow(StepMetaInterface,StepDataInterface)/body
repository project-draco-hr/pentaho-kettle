{
  meta=(ScriptValuesMeta)smi;
  data=(ScriptValuesData)sdi;
  Row r=getRow();
  if (r == null) {
    debug=Messages.getString("ScriptValues.DebugMessage.End");
    if (data.cx != null)     Context.exit();
    setOutputDone();
    return false;
  }
  if (!addValues(r)) {
    debug=Messages.getString("ScriptValues.DebugMessage.NoMoreNewValues");
    if (data.cx != null)     Context.exit();
    setOutputDone();
    return false;
  }
  putRow(r);
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("ScriptValues.Log.LineNumber") + linesRead);
  return true;
}

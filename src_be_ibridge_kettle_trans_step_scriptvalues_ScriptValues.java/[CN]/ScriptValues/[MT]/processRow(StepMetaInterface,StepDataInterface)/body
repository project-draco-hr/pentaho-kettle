{
  meta=(ScriptValuesMeta)smi;
  data=(ScriptValuesData)sdi;
  Row r=getRow();
  if (r == null) {
    if (data.cx != null)     Context.exit();
    setOutputDone();
    return false;
  }
  if (!addValues(r)) {
    if (!getStepMeta().isDoingErrorHandling()) {
      if (data.cx != null)       Context.exit();
      setOutputDone();
      return false;
    }
  }
 else {
    putRow(r);
    if (checkFeedback(linesRead))     logBasic(Messages.getString("ScriptValues.Log.LineNumber") + linesRead);
  }
  return true;
}

  fdlFontColor.right=new FormAttachment(middle,-margin);
  wlFontColor.setLayoutData(fdlFontColor);
  wbFontColorChange=new Button(wNoteFontComp,SWT.PUSH);
  wbFontColorChange.setImage(guiresource.getImageColor());

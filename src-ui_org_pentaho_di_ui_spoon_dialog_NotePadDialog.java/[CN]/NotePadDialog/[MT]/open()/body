    public void widgetSelected(    SelectionEvent e){
      ColorDialog cd=new ColorDialog(shell);
      cd.setText(BaseMessages.getString(PKG,"NotePadDialog.Font.Color.Dialog.Label"));
      cd.setRGB(wBorderColor.getBackground().getRGB());

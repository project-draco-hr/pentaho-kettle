  fdDesc.right=new FormAttachment(100,0);
  fdDesc.bottom=new FormAttachment(100,-margin);
  wDesc.setLayoutData(fdDesc);
  fdNoteContentComp=new FormData();
  fdNoteContentComp.left=new FormAttachment(0,0);

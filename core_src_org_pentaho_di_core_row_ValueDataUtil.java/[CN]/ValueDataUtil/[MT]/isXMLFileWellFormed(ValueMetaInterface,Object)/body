{
  if (dataA == null)   return false;
  String filename=dataA.toString();
  FileObject file=null;
  try {
    file=KettleVFS.getFileObject(filename);
    return XMLCheck.isXMLFileWellFormed(file);
  }
 catch (  Exception e) {
  }
 finally {
    if (file != null)     try {
      file.close();
    }
 catch (    Exception e) {
    }
    ;
  }
  return false;
}

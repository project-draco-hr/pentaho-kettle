{
  if (metaA.isDate()) {
    Calendar cal=Calendar.getInstance();
    Date date=metaA.getDate(dataA);
    if (date != null) {
      cal.setTime(date);
      return Const.removeTimeFromDate(date);
    }
 else     return null;
  }
  throw new KettleValueException("The 'removeTimeFromDate' function only works with a date");
}

    }
  }
);
  wTabFolder.setSelection(0);
  setSize();
  getData();
  activateKey();
  activateDynamicCommand();
  input.setChanged(changed);
  shell.open();
  while (!shell.isDisposed()) {
    if (!display.readAndDispatch()) {
      display.sleep();
    }
  }
  return stepname;
}

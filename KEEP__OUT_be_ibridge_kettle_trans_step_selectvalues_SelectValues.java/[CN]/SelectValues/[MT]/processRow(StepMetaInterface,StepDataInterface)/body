{
  meta=(SelectValuesMeta)smi;
  data=(SelectValuesData)sdi;
  Row r=null;
  boolean err=true;
  r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  if (log.isRowLevel())   logRowlevel(Messages.getString("SelectValues.Log.GotRowFromPreviousStep") + r);
  err=true;
  if (data.select)   err=selectValues(r);
  if (data.deselect)   err=removeValues(r);
  if (data.metadata)   err=metadataValues(r);
  if (!err) {
    setOutputDone();
    return false;
  }
  putRow(r);
  if (log.isRowLevel())   logRowlevel(Messages.getString("SelectValues.Log.WroteRowToNextStep") + r);
  if (checkFeedback(linesRead))   logBasic(Messages.getString("SelectValues.Log.LineNumber") + linesRead);
  return true;
}

{
  if (input.length > 0) {
    CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,Messages.getString("InjectorMeta.CheckResult.StepExpectingNoReadingInfoFromOtherSteps"),stepMeta);
    remarks.add(cr);
  }
 else {
    CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("InjectorMeta.CheckResult.NoInputReceivedError"),stepMeta);
    remarks.add(cr);
  }
}

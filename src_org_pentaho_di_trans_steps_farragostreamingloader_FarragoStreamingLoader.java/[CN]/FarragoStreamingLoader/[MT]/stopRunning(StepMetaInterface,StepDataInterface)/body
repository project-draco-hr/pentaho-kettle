{
  meta=(FarragoStreamingLoaderMeta)smi;
  data=(FarragoStreamingLoaderData)sdi;
  if (data.objOut != null) {
    try {
      data.objOut.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
    if (data.client != null) {
      try {
        data.client.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
  try {
    data.sqlRunner.join();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
}

{
  if (isStopped() || data.updateBatchRows.size() == 0)   return;
  try {
    openerERPAdapter.importData(data.updateBatchRows);
    int idFieldIndex=-1;
    if (meta.getOutputIDField())     idFieldIndex=data.outputRowMeta.indexOfValue(meta.getOutputIDFieldName());
    for (int i=0; i < data.updateBatchRows.size(); i++) {
      incrementLinesOutput();
      Object[] outputrow=data.outputBatchRows.get(i);
      if (data.helper.getImportReturnIDS() && idFieldIndex >= 0)       outputrow[idFieldIndex]=Long.parseLong(data.updateBatchRows.get(i).get("id").toString());
      putRow(data.outputRowMeta,outputrow);
    }
  }
  finally {
    data.updateBatchRows.clear();
    data.outputBatchRows.clear();
  }
}

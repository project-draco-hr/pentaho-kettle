{
  if (data == null)   return true;
  if (isString()) {
    if (isStorageNormal() && ((String)data).length() == 0)     return true;
    if (isStorageBinaryString()) {
      try {
        if (((byte[])data).length == 0)         return true;
      }
 catch (      ClassCastException e) {
        throw e;
      }
    }
  }
  return false;
}

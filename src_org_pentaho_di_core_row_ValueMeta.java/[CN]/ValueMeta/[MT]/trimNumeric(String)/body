{
switch (getTrimType()) {
case TRIM_TYPE_NONE:
    string=ValueDataUtil.leftTrim(string);
  break;
case TRIM_TYPE_RIGHT:
string=ValueDataUtil.rightTrim(string);
break;
case TRIM_TYPE_LEFT:
string=ValueDataUtil.leftTrim(string);
break;
case TRIM_TYPE_BOTH:
string=ValueDataUtil.trim(string);
break;
default :
break;
}
return string;
}

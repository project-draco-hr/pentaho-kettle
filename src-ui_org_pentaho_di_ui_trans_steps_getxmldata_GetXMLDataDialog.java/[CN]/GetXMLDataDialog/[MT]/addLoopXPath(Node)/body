{
  Element ce=(Element)node;
  for (int j=0; j < ce.nodeCount(); j++) {
    Node cnode=ce.node(j);
    if (!Const.isEmpty(cnode.getName())) {
      Element cce=(Element)cnode;
      if (cce.nodeCount() > 1) {
        if (!listpath.contains(cnode.getPath()))         listpath.add(cnode.getPath());
        addLoopXPath(cnode);
      }
    }
  }
}

{
  try {
    Row r=transMeta.getPrevStepFields(stepname);
    if (r != null) {
      BaseStepDialog.getFieldsFromPrevious(r,wKey,1,new int[]{1,2},new int[]{},-1,-1,new TableItemInsertListener(){
        public boolean tableItemInserted(        TableItem tableItem,        Value v){
          tableItem.setText(3,"N");
          return true;
        }
      }
);
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(shell,Messages.getString("CombinationLookupDialog.UnableToGetFieldsError.DialogTitle"),Messages.getString("CombinationLookupDialog.UnableToGetFieldsError.DialogMessage"),ke);
  }
}

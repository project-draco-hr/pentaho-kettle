  ColumnInfo[] cArguments={new ColumnInfo(Messages.getString("TransExecutionConfigurationDialog.ArgumentsColumn.Argument"),ColumnInfo.COLUMN_TYPE_TEXT,false,true),new ColumnInfo(Messages.getString("TransExecutionConfigurationDialog.ArgumentsColumn.Value"),ColumnInfo.COLUMN_TYPE_TEXT,false,false)};
  int nrArguments=configuration.getArguments() != null ? configuration.getArguments().size() : 0;
  wArguments=new TableView(transMeta,shell,SWT.BORDER | SWT.FULL_SELECTION | SWT.MULTI,cArguments,nrArguments,true,null,props);

  wlVariables.setLayoutData(fdlVariables);
  ColumnInfo[] cVariables={new ColumnInfo(Messages.getString("TransExecutionConfigurationDialog.VariablesColumn.Argument"),ColumnInfo.COLUMN_TYPE_TEXT,false,false),new ColumnInfo(Messages.getString("TransExecutionConfigurationDialog.VariablesColumn.Value"),ColumnInfo.COLUMN_TYPE_TEXT,false,false)};

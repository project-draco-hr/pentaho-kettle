{
  String sRC="";
  if (ArgList.length == 1) {
    try {
      String sArg1=Context.toString(ArgList[0]);
      sRC=System.getProperty(sArg1,"");
    }
 catch (    Exception e) {
      sRC="";
    }
  }
 else {
    throw Context.reportRuntimeError("The function call getEnvironmentVar requires 1 argument.");
  }
  return sRC;
}

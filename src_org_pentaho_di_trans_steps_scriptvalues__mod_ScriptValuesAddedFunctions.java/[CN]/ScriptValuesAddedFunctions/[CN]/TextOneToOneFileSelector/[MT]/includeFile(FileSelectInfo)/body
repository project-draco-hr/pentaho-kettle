{
  boolean resultat=false;
  try {
    if (destfile.exists()) {
      if (overwrite_files)       resultat=true;
    }
 else     resultat=true;
  }
 catch (  Exception e) {
    throw Context.reportRuntimeError("Error trying to copy file [" + info.getFile().toString() + "]. Exception : "+ e.toString());
  }
  return resultat;
}

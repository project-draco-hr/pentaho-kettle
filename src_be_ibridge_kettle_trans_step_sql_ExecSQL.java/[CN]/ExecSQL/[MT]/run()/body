{
  try {
    logBasic("Starting to run...");
    while (!isStopped() && processRow(meta,data))     ;
  }
 catch (  Exception e) {
    logError("Unexpected error in '" + debug + "' : "+ e.toString());
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

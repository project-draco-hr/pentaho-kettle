{
  try {
    logBasic(Messages.getString("ExecSQL.Log.StartingToRun"));
    while (!isStopped() && processRow(meta,data))     ;
  }
 catch (  Exception e) {
    logError(Messages.getString("ExecSQL.Log.UnexpectedError") + " : " + e.toString());
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

{
  TransMeta transMeta=new TransMeta();
  transMeta.setName("transname");
  DatabaseMeta databaseMeta=new DatabaseMeta("<?xml version=\"1.0\" encoding=\"UTF-8\"?>" + "<connection>" + "<name>db</name>"+ "<server>127.0.0.1</server>"+ "<type>H2</type>"+ "<access>Native</access>"+ "<database>mem:db</database>"+ "<port></port>"+ "<username>sa</username>"+ "<password></password>"+ "</connection>");

{
  RowMetaInterface rm=new RowMeta();
  ValueMetaInterface[] valuesMeta={new ValueMetaInteger("ID",8,0),new ValueMetaString("DLR_CD",8,0),new ValueMetaString("DLR_NM",30,0),new ValueMetaString("DLR_DESC",30,0)};
  for (int i=0; i < valuesMeta.length; i++) {
    rm.addValueMeta(valuesMeta[i]);
  }
  return rm;
}

{
  boolean res=false;
  meta=(InfobrightLoaderMetadata)smi;
  data=(InfobrightLoaderStepData)sdi;
  if (super.init(smi,sdi)) {
    try {
      data.databaseSetup(meta,this);
      res=true;
    }
 catch (    Exception ex) {
      logError("An error occurred intialising this step",ex);
      stopAll();
      setErrors(1);
    }
  }
  return res;
}

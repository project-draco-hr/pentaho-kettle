{
  ArrayList st=new ArrayList();
  int idx;
  TableMeta si;
  RelationshipMeta hi;
  for (int x=0; x < nrRelationships(); x++) {
    hi=getRelationship(x);
    si=hi.getTableFrom();
    idx=st.indexOf(si);
    if (idx < 0)     st.add(si);
    si=hi.getTableTo();
    idx=st.indexOf(si);
    if (idx < 0)     st.add(si);
  }
  for (int x=0; x < nrTables(); x++) {
    si=getTable(x);
    if (si.isDrawn() && !isTableUsedInRelationships(si.getName())) {
      st.add(si);
    }
  }
  return st;
}

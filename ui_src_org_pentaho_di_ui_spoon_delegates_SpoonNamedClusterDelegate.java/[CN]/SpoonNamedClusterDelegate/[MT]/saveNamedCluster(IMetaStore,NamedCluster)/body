{
  try {
    NamedClusterManager.getInstance().create(namedCluster,metaStore);
  }
 catch (  MetaStoreException e) {
    new ErrorDialog(spoon.getShell(),BaseMessages.getString(PKG,"Spoon.Dialog.ErrorSavingNamedCluster.Title"),BaseMessages.getString(PKG,"Spoon.Dialog.ErrorSavingNamedCluster.Message",namedCluster.getName()),e);
  }
}

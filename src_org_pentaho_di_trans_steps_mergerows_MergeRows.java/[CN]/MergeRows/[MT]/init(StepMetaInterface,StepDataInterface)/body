{
  meta=(MergeRowsMeta)smi;
  data=(MergeRowsData)sdi;
  if (super.init(smi,sdi)) {
    StreamInterface[] infoStreams=meta.getStepIOMeta().getInfoStreams();
    if (infoStreams[0].getStepMeta() != null ^ infoStreams[1].getStepMeta() != null) {
      logError(BaseMessages.getString(PKG,"MergeRows.Log.BothTrueAndFalseNeeded"));
    }
 else {
      return true;
    }
  }
  return false;
}

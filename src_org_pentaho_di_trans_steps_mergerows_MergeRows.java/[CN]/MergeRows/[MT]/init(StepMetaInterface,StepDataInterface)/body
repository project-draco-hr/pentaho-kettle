{
  meta=(MergeRowsMeta)smi;
  data=(MergeRowsData)sdi;
  if (super.init(smi,sdi)) {
    if (meta.getReferenceStepName() != null ^ meta.getCompareStepName() != null) {
      logError(Messages.getString("MergeRows.Log.BothTrueAndFalseNeeded"));
    }
 else {
      return true;
    }
  }
  return false;
}

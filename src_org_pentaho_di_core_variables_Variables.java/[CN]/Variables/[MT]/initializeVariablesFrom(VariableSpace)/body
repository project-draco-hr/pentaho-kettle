{
  this.parent=parent;
  for (  Object key : System.getProperties().keySet()) {
    properties.put((String)key,System.getProperties().getProperty((String)key));
  }
  if (parent != null) {
    copyVariablesFrom(parent);
  }
  if (injection != null) {
    properties.putAll(injection);
    injection=null;
  }
  initialized=true;
}

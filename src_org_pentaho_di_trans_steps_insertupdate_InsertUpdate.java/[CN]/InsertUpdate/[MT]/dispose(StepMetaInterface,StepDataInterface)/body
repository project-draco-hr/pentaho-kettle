{
  meta=(InsertUpdateMeta)smi;
  data=(InsertUpdateData)sdi;
  try {
    if (!data.db.isAutoCommit()) {
      if (getErrors() == 0) {
        data.db.commit();
      }
 else {
        data.db.rollback();
      }
    }
    data.db.closeUpdate();
    data.db.closeInsert();
  }
 catch (  KettleDatabaseException e) {
    log.logError(toString(),Messages.getString("InsertUpdate.Log.UnableToCommitConnection") + e.toString());
    setErrors(1);
  }
 finally {
    if (data.db != null) {
      data.db.disconnect();
    }
  }
  super.dispose(smi,sdi);
}

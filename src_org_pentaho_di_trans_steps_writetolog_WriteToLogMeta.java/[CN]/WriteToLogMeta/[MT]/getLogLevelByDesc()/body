{
  if (loglevel == null)   return 3;
  int retval;
  if (loglevel.equals(logLevelCodes[0]))   retval=0;
 else   if (loglevel.equals(logLevelCodes[1]))   retval=1;
 else   if (loglevel.equals(logLevelCodes[2]))   retval=2;
 else   if (loglevel.equals(logLevelCodes[3]))   retval=3;
 else   if (loglevel.equals(logLevelCodes[4]))   retval=4;
 else   if (loglevel.equals(logLevelCodes[5]))   retval=5;
 else   retval=6;
  return retval;
}

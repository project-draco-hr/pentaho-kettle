{
  if (o == null || !(o instanceof Trans)) {
    return null;
  }
  TransformationEvent transEvent=new TransformationEvent(TransformationEvent.EventType.FINISHED).build((Trans)o);
  getLog().logDebug("TransformationFinishMonitor - " + ToStringBuilder.reflectionToString(o) + " , completed in "+ (transEvent.getStartTimeMillis() > 0 ? (((transEvent.getEndTimeMillis() - transEvent.getStartTimeMillis()) / 1000) % 60) : 0)+ " seconds");
  return transEvent;
}

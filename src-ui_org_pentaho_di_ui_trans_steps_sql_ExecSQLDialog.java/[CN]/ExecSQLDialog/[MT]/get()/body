{
  try {
    RowMetaInterface r=transMeta.getPrevStepFields(stepname);
    if (r != null) {
      BaseStepDialog.getFieldsFromPrevious(r,wFields,1,new int[]{1},new int[]{},-1,-1,null);
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(shell,BaseMessages.getString(PKG,"ExecSQLDialog.FailedToGetFields.DialogTitle"),BaseMessages.getString(PKG,"ExecSQLDialog.FailedToGetFields.DialogMessage"),ke);
  }
}

{
  DatabaseImpact ii=new DatabaseImpact(DatabaseImpact.TYPE_IMPACT_READ_WRITE,transMeta.getName(),stepMeta.getName(),databaseMeta.getDatabaseName(),Messages.getString("ExecSQLMeta.DatabaseMeta.Unknown.Label"),Messages.getString("ExecSQLMeta.DatabaseMeta.Unknown2.Label"),Messages.getString("ExecSQLMeta.DatabaseMeta.Unknown3.Label"),stepMeta.getName(),sql,Messages.getString("ExecSQLMeta.DatabaseMeta.Title"));
  impact.add(ii);
}

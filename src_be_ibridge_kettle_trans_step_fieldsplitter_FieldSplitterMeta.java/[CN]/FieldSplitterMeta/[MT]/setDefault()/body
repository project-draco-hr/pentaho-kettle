{
  splitField="";
  delimiter=",";
  int nrfields=0;
  allocate(nrfields);
  for (int i=0; i < nrfields; i++) {
    field[i]="field" + i;
    fieldID[i]="id" + i;
    removeID[i]=true;
    fieldType[i]=Value.VALUE_TYPE_NUMBER;
    fieldFormat[i]="";
    fieldGroup[i]="";
    fieldDecimal[i]="";
    fieldCurrency[i]="";
    fieldLength[i]=-1;
    fieldPrecision[i]=-1;
    fieldDefault[i]="";
  }
}

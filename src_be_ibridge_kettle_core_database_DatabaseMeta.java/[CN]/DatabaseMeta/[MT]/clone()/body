{
  DatabaseMeta di=new DatabaseMeta(getName(),getDatabaseTypeDesc(),getAccessTypeDesc(),getHostname(),getDatabaseName(),getDatabasePortNumberString(),getUsername(),getPassword());
  di.setServername(getServername());
  di.setDataTablespace(getDataTablespace());
  di.setIndexTablespace(getIndexTablespace());
  di.databaseInterface=(DatabaseInterface)di.getDatabaseInterface().clone();
  return di;
}

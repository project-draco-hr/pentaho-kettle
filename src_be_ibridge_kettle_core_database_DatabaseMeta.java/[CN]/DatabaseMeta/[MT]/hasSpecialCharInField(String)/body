{
  if (fieldname.indexOf('/') >= 0)   return true;
  if (fieldname.indexOf('-') >= 0)   return true;
  if (fieldname.indexOf('+') >= 0)   return true;
  if (fieldname.indexOf('.') >= 0)   return true;
  if (fieldname.indexOf('*') >= 0)   return true;
  if (fieldname.indexOf('(') >= 0)   return true;
  if (fieldname.indexOf(')') >= 0)   return true;
  if (fieldname.indexOf('{') >= 0)   return true;
  if (fieldname.indexOf('}') >= 0)   return true;
  if (fieldname.indexOf('[') >= 0)   return true;
  if (fieldname.indexOf(']') >= 0)   return true;
  if (fieldname.indexOf('%') >= 0)   return true;
  if (fieldname.indexOf('@') >= 0)   return true;
  return false;
}

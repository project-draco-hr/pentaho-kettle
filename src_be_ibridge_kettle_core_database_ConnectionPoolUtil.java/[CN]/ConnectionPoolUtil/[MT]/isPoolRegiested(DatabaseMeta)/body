{
  String[] poolNames=null;
  try {
    poolNames=pd.getPoolNames();
  }
 catch (  SQLException e) {
    e.printStackTrace();
  }
  if (poolNames != null) {
    String name=dbMeta.getName();
    for (int i=0; i < poolNames.length; i++) {
      if (poolNames[i].equals(name))       return true;
    }
  }
  return false;
}

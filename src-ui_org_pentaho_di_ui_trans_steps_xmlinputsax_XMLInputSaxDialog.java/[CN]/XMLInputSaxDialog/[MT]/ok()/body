{
  if (Const.isEmpty(wStepname.getText()))   return;
  try {
    getInfo(input);
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("XMLInputSaxDialog.Dialog.ErrorParsingInputData.Title"),Messages.getString("XMLInputSaxDialog.Dialog.ErrorParsingInputData.Message"),e);
  }
  dispose();
}

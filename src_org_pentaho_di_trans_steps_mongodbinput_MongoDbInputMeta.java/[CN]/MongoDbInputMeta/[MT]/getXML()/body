{
  StringBuffer retval=new StringBuffer(300);
  retval.append("    ").append(XMLHandler.addTagValue("hostname",hostname));
  retval.append("    ").append(XMLHandler.addTagValue("port",port));
  retval.append("    ").append(XMLHandler.addTagValue("db_name",dbName));
  retval.append("    ").append(XMLHandler.addTagValue("collection",collection));
  retval.append("    ").append(XMLHandler.addTagValue("json_field_name",jsonFieldName));
  return retval.toString();
}

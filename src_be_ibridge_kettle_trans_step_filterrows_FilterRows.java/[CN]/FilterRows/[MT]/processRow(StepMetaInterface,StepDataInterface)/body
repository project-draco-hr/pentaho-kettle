{
  meta=(FilterRowsMeta)smi;
  data=(FilterRowsData)sdi;
  Row r=null;
  boolean keep;
  r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  keep=keepRow(r);
  if (!meta.chosesTargetSteps()) {
    if (keep) {
      putRow(r);
    }
  }
 else {
    if (keep) {
      putRowTo(r,meta.getSendTrueStepname());
    }
 else {
      putRowTo(r,meta.getSendFalseStepname());
    }
  }
  if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)   logBasic(Messages.getString("FilterRows.Log.LineNumber") + linesRead);
  return true;
}

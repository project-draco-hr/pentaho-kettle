{
  TimedTransRunner timedTransRunner=new TimedTransRunner("experimental_test/org/pentaho/di/run/abort/AbortAboveLimit.ktr",LogWriter.LOG_LEVEL_ERROR,AllRunTests.getOldTargetDatabase(),AllRunTests.getNewTargetDatabase(),1);
  timedTransRunner.runOldAndNew();
  be.ibridge.kettle.core.Result oldResult=timedTransRunner.getOldResult();
  assertTrue(oldResult.getNrErrors() == 1);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 1);
}

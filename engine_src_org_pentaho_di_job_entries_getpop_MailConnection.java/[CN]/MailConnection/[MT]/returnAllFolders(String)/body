{
  Folder dfolder=null;
  String[] retval=null;
  try {
    if (Const.isEmpty(folder)) {
      dfolder=getStore().getDefaultFolder();
    }
 else {
      dfolder=getStore().getFolder(folder);
    }
    retval=returnAllFolders(dfolder);
  }
 catch (  Exception e) {
  }
 finally {
    try {
      if (dfolder != null)       dfolder.close(false);
    }
 catch (    Exception e) {
    }
    ;
  }
  return retval;
}

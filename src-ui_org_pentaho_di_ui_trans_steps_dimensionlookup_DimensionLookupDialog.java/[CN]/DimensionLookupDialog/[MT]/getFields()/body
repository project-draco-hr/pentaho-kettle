{
  if (!gotPreviousFields) {
    try {
      String field=wDatefield.getText();
      RowMetaInterface r=transMeta.getPrevStepFields(stepname);
      if (r != null) {
        wDatefield.setItems(r.getFieldNames());
      }
      if (field != null)       wDatefield.setText(field);
    }
 catch (    KettleException ke) {
      new ErrorDialog(shell,Messages.getString("DimensionLookupDialog.ErrorGettingFields.Title"),Messages.getString("DimensionLookupDialog.ErrorGettingFields.Message"),ke);
    }
    gotPreviousFields=true;
  }
}

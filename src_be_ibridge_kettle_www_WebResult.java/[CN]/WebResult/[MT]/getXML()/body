{
  StringBuffer xml=new StringBuffer();
  xml.append("<" + XML_TAG + ">").append(Const.CR);
  xml.append("  ").append(XMLHandler.addTagValue("result",result));
  xml.append("  ").append(XMLHandler.addTagValue("message",message));
  xml.append("</" + XML_TAG + ">").append(Const.CR);
  return xml.toString();
}

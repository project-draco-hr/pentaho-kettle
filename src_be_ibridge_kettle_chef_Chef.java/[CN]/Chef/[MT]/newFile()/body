{
  MessageBox mb=new MessageBox(shell,SWT.YES | SWT.NO | SWT.ICON_WARNING);
  mb.setMessage("Are you sure you want to clear all information?");
  mb.setText("Warning!");
  int answer=mb.open();
  if (answer == SWT.YES) {
    jobMeta.clear();
    loadRepositoryObjects();
    setFilename(null);
    refreshTree();
    refreshGraph();
    setUndoMenu();
  }
}

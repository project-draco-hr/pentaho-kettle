{
  boolean retval=true;
  log.logDetailed(toString(),"Quit application.");
  saveSettings();
  if (schema.hasChanged()) {
    MessageBox mb=new MessageBox(shell,SWT.YES | SWT.NO | SWT.CANCEL| SWT.ICON_WARNING);
    mb.setMessage("File has changed!  Do you want to save first?");
    mb.setText("Warning!");
    int answer=mb.open();
switch (answer) {
case SWT.YES:
      saveFile();
    dispose();
  break;
case SWT.NO:
dispose();
break;
case SWT.CANCEL:
retval=false;
break;
}
}
 else {
dispose();
}
return retval;
}

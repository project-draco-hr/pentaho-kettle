{
  try {
    Object c=Class.forName(JCO_IMPL_EXISTENCE_TEST_CLASS);
    if (c == null)     return false;
    return true;
  }
 catch (  UnsatisfiedLinkError e) {
    return false;
  }
catch (  NoClassDefFoundError e) {
    return false;
  }
catch (  ClassNotFoundException e) {
    return false;
  }
}

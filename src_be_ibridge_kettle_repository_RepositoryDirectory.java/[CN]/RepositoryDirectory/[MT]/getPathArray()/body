{
  int depth=1;
  RepositoryDirectory follow=getParent();
  if (follow != null) {
    depth++;
    follow=follow.getParent();
  }
  String retval[]=new String[depth];
  int level=depth - 1;
  retval[level]=getDirectoryName();
  follow=getParent();
  if (follow != null) {
    level--;
    retval[level]=follow.getDirectoryName();
    follow=follow.getParent();
  }
  return retval;
}

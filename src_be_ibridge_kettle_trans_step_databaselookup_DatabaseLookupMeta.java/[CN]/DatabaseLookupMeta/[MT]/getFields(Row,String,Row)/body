{
  Row row;
  if (r == null)   row=new Row();
 else   row=r;
  if (info == null) {
    for (int i=0; i < returnValueNewName.length; i++) {
      Value v=new Value(returnValueNewName[i],returnValueDefaultType[i]);
      v.setOrigin(name);
      row.addValue(v);
    }
  }
 else {
    for (int i=0; i < returnValueNewName.length; i++) {
      Value v=info.searchValue(returnValueField[i]);
      if (v != null) {
        v.setName(returnValueNewName[i]);
        v.setOrigin(name);
        row.addValue(v);
      }
    }
  }
  return row;
}

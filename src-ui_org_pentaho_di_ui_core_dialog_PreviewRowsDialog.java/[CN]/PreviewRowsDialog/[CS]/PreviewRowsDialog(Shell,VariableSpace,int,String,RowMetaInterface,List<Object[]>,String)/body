{
  this.stepname=stepName;
  this.buffer=rowBuffer;
  this.loggingText=loggingText;
  this.rowMeta=rowMeta;
  this.variables=space;
  this.parentShell=parent;
  this.style=(style != SWT.None) ? style : this.style;

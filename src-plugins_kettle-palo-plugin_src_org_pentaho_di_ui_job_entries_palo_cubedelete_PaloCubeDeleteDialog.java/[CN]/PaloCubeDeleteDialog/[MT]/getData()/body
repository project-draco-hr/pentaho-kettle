{
  if (jobEntry.getName() != null)   textStepName.setText(jobEntry.getName());
  textStepName.selectAll();
  int index=addConnectionLine.indexOf(jobEntry.getDatabaseMeta() != null ? jobEntry.getDatabaseMeta().getName() : "");
  if (index >= 0)   addConnectionLine.select(index);
  if (jobEntry.getCubeName() != null)   comboCubeName.setText(jobEntry.getCubeName());
}

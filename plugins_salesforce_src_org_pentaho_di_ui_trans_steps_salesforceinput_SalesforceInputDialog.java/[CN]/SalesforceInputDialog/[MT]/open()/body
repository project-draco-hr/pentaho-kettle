    }
  }
;
  lsGet=new Listener(){
    @Override public void handleEvent(    Event e){
      Cursor busy=new Cursor(shell.getDisplay(),SWT.CURSOR_WAIT);
      shell.setCursor(busy);
      get();
      shell.setCursor(null);
      busy.dispose();
    }
  }
;
  lsPreview=new Listener(){
    @Override public void handleEvent(    Event e){
      preview();
    }
  }
;
  lsCancel=new Listener(){
    @Override public void handleEvent(    Event e){
      cancel();
    }
  }
;

  setEnableInclModule();
  setEnableInclRownum();
  setEnableInclDeletionDate();
  setEnableQuery();
  input.setChanged(changed);
  shell.open();
  while (!shell.isDisposed()) {
    if (!display.readAndDispatch()) {
      display.sleep();
    }
  }
  return stepname;

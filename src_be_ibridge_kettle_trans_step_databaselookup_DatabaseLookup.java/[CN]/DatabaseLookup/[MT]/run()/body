{
  logBasic(Messages.getString("DatabaseLookup.Log.StartingToRun"));
  try {
    logBasic(Messages.getString("DatabaseLookup.Log.ConnectedToDatabase2"));
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError(Messages.getString("DatabaseLookup.ERROR003.UnexpectedErrorDuringProcessing2") + e.getMessage());
    logError(e.toString());
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
  }
 finally {
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

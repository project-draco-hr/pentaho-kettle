{
  Map<TargetStepAttribute,SourceStepField> targetMap=Collections.singletonMap(new TargetStepAttribute(INJECTOR_STEP_NAME,TEST_ATTR_VALUE,false),new SourceStepField(TEST_SOURCE_STEP_NAME,TEST_FIELD));
  MetaInjectMeta spyMeta=spy(meta);
  doReturn(targetMap).when(spyMeta).getTargetSourceMapping();
  assertFalse(metaInject.init(spyMeta,data));
}

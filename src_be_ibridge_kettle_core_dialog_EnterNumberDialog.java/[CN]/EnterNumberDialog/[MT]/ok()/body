{
  try {
    samples=Integer.parseInt(wNumber.getText());
    dispose();
  }
 catch (  Exception e) {
    MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_ERROR);
    mb.setMessage("Please enter an integer number!" + Const.CR);
    mb.setText("ERROR");
    mb.open();
    wNumber.selectAll();
  }
}

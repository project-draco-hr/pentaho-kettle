{
  if (log.isDetailed()) {
    logDetailed("=======================================");
    logDetailed(BaseMessages.getString(PKG,"JobUnZip.Log.Info.FilesInError","" + NrErrors));
    logDetailed(BaseMessages.getString(PKG,"JobUnZip.Log.Info.FilesInSuccess","" + NrSuccess));
    logDetailed("=======================================");
  }
}

{
  FileReader fr=null;
  try {
    fr=new FileReader(filename);
    Object o=JSONValue.parse(fr);
    if (o == null) {
      throw new KettleException(BaseMessages.getString(PKG,"JsonReader.Error.ParsingFile",filename));
    }
    eval(o);
  }
 catch (  Exception e) {
    throw new KettleException(e);
  }
 finally {
    try {
      if (fr != null)       fr.close();
    }
 catch (    Exception e) {
    }
  }
}

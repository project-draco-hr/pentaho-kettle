{
  PluginRegistry registry=getInstance();
  PluginTypeInterface[] pluginTypes=new PluginTypeInterface[]{StepPluginType.getInstance(),PartitionerPluginType.getInstance(),JobEntryPluginType.getInstance(),RepositoryPluginType.getInstance()};
  for (  PluginTypeInterface pluginType : pluginTypes) {
    registry.registerPluginType(pluginType.getClass());
    pluginType.searchPlugins();
  }
}

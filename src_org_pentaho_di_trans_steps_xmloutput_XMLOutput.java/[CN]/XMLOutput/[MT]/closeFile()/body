{
  boolean retval=false;
  if (data.OpenedNewFile) {
    try {
      data.writer.write(("</" + meta.getMainElement() + ">"+ Const.CR).toCharArray());
      data.writer.close();
      if (meta.isZipped()) {
        data.zip.closeEntry();
        data.zip.finish();
        data.zip.close();
      }
      retval=true;
    }
 catch (    Exception e) {
    }
  }
  return retval;
}

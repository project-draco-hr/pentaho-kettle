{
  long start=System.currentTimeMillis();
  while ((System.currentTimeMillis() - start) < (delayInSeconds * 1000)) {
    try {
      Thread.sleep(25);
    }
 catch (    InterruptedException e) {
      break;
    }
  }
  for (  DelayListener delayListener : delayListeners) {
    delayListener.expired();
  }
}

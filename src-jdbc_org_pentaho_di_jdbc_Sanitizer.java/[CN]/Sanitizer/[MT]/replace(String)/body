{
  char[] data=input.toCharArray();
  CharBuffer cb=CharBuffer.allocate(data.length);
  for (int i=0; i < data.length; i++) {
    char d=data[i];
    int result=java.util.Arrays.binarySearch(validChars,d);
    if (result > 0) {
      cb.append(d);
    }
  }
  String r=new String(cb.array());
  r=r.trim();
  return r;
}

  Label wlEnclosure=new Label(shell,SWT.RIGHT);
  wlEnclosure.setText(Messages.getString("S3CsvInputDialog.Enclosure.Label"));
  props.setLook(wlEnclosure);
  FormData fdlEnclosure=new FormData();

{
  if (getAccessType() == DatabaseMeta.TYPE_ACCESS_ODBC) {
    return "jdbc:odbc:" + databaseName;
  }
 else {
    if (Const.isEmpty(port)) {
      return "jdbc:ingres://" + hostname + ":II7/"+ databaseName;
    }
 else {
      return "jdbc:ingres://" + hostname + ":"+ port+ "/"+ databaseName;
    }
  }
}

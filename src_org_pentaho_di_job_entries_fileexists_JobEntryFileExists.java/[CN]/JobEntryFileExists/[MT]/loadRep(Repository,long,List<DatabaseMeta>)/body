{
  try {
    super.loadRep(rep,id_jobentry,databases);
    filename=rep.getJobEntryAttributeString(id_jobentry,"filename");
  }
 catch (  KettleException dbe) {
    throw new KettleException(Messages.getString("JobEntryFileExists.ERROR_0002_Cannot_Load_Job_From_Repository",Long.toString(id_jobentry)),dbe);
  }
}

{
switch (image) {
case LOCK:
    return imageLocked;
case STEP_ERROR:
  return imageStepError;
case EDIT:
return imageEdit;
case CONTEXT_MENU:
return imageContextMenu;
case TRUE:
return imageTrue;
case FALSE:
return imageFalse;
case ERROR:
return imageErrorHop;
case INFO:
return imageInfoHop;
case TARGET:
return imageHopTarget;
case INPUT:
return imageHopInput;
case OUTPUT:
return imageHopOutput;
case ARROW:
return imageArrow;
case COPY_ROWS:
return imageCopyHop;
case PARALLEL:
return imageParallelHop;
case UNCONDITIONAL:
return imageUnconditionalHop;
case BUSY:
return imageBusy;
}
return null;
}

{
  if (Utils.isEmpty(filename)) {
    String dir=directory.toString();
    if (dir.endsWith("/")) {
      return dir + name + " ("+ objectType.getTypeDescription()+ ")";
    }
 else {
      return dir + "/" + name+ " ("+ objectType.getTypeDescription()+ ")";
    }
  }
 else {
    return filename;
  }
}

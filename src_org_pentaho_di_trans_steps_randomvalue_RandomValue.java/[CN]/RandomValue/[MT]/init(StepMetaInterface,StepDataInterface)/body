{
  meta=(RandomValueMeta)smi;
  data=(RandomValueData)sdi;
  if (super.init(smi,sdi)) {
    data.readsRows=false;
    StepMeta previous[]=getTransMeta().getPrevSteps(getStepMeta());
    if (previous != null && previous.length > 0) {
      data.readsRows=true;
    }
    return true;
  }
  return false;
}

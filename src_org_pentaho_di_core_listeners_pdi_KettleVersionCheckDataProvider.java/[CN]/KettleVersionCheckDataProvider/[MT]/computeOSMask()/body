{
  try {
    String os=System.getProperty("os.name");
    if (os != null) {
      os=os.toLowerCase();
      if (os.indexOf("windows") >= 0) {
        return DEPTH_WINDOWS_MASK;
      }
 else       if (os.indexOf("mac") >= 0) {
        return DEPTH_MAC_MASK;
      }
 else       if (os.indexOf("linux") >= 0) {
        return DEPTH_LINUX_MASK;
      }
 else {
        return DEPTH_ALL_MASK;
      }
    }
  }
 catch (  Exception e) {
  }
  return DEPTH_ALL_MASK;
}

{
  String url=wURL.getText() != null ? wURL.getText().length() > 0 ? wURL.getText() : null : null;
  String driverClass=wDriverClass.getText() != null ? wDriverClass.getText().length() > 0 ? wDriverClass.getText() : null : null;
  if (url == null || driverClass == null) {
    setErrorMessage(Messages.getString("CreateDatabaseWizardPageGeneric.ErrorMessage.URLAndDriverClassRequired"));
    return false;
  }
 else {
    getDatabaseInfo();
    setErrorMessage(null);
    setMessage(Messages.getString("CreateDatabaseWizardPageGeneric.Message.Next"));
    return true;
  }
}

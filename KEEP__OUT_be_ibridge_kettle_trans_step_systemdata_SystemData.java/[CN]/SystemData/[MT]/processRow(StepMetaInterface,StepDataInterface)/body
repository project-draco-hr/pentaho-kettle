{
  Row row;
  if (data.readsRows) {
    row=getRow();
    if (row == null) {
      setOutputDone();
      return false;
    }
  }
 else {
    row=new Row();
    linesRead++;
  }
  row.addRow(getSystemData());
  if (log.isRowLevel())   logRowlevel("System info returned: " + row);
  putRow(row);
  if (!data.readsRows) {
    setOutputDone();
    return false;
  }
  return true;
}

{
  Row row=getRowFromXML();
  if (row == null) {
    setOutputDone();
    return false;
  }
  if (log.isRowLevel())   logRowlevel("Read row: " + row.toString());
  putRow(row);
  if (meta.getRowLimit() > 0 && data.rownr >= meta.getRowLimit()) {
    setOutputDone();
    return false;
  }
  return true;
}

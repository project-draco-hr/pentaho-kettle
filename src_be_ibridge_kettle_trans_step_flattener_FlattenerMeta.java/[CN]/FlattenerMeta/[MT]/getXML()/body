{
  String retval="";
  retval+="      " + XMLHandler.addTagValue("field_name",fieldName);
  retval+="      <fields>" + Const.CR;
  for (int i=0; i < targetField.length; i++) {
    retval+="        <field>" + Const.CR;
    retval+="          " + XMLHandler.addTagValue("name",targetField[i]);
    retval+="          </field>" + Const.CR;
  }
  retval+="        </fields>" + Const.CR;
  return retval;
}

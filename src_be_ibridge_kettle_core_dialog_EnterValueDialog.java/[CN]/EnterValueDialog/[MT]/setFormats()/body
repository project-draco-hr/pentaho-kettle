{
  wFormat.removeAll();
  int valtype=Value.getType(wValueType.getText());
switch (valtype) {
case Value.VALUE_TYPE_NUMBER:
    for (int i=0; i < Const.numberFormats.length; i++)     wFormat.add(Const.numberFormats[i]);
  break;
case Value.VALUE_TYPE_DATE:
for (int i=0; i < Const.dateFormats.length; i++) wFormat.add(Const.dateFormats[i]);
break;
case Value.VALUE_TYPE_STRING:
case Value.VALUE_TYPE_BOOLEAN:
case Value.VALUE_TYPE_INTEGER:
default :
break;
}
}

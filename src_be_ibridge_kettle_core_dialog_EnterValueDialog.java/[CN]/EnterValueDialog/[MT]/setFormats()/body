{
  wFormat.removeAll();
  int valtype=Value.getType(wValueType.getText());
switch (valtype) {
case Value.VALUE_TYPE_NUMBER:
    for (int i=0; i < Const.getNumberFormats().length; i++)     wFormat.add(Const.getNumberFormats()[i]);
  break;
case Value.VALUE_TYPE_DATE:
for (int i=0; i < Const.getDateFormats().length; i++) wFormat.add(Const.getDateFormats()[i]);
break;
case Value.VALUE_TYPE_STRING:
case Value.VALUE_TYPE_BOOLEAN:
case Value.VALUE_TYPE_INTEGER:
default :
break;
}
}

{
  assertEquals("",StringUtil.initCap(null));
  assertEquals("",StringUtil.initCap(""));
  assertEquals("",StringUtil.initCap("   "));
  assertEquals("A word",StringUtil.initCap("a word"));
  assertEquals("A word",StringUtil.initCap("A word"));
  assertEquals("Award",StringUtil.initCap("award"));
  assertEquals("Award",StringUtil.initCap("Award"));
  assertEquals("AWard",StringUtil.initCap("aWard"));
  assertEquals("AWard",StringUtil.initCap("AWard"));
}

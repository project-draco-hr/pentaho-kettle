{
  String retval="";
  String fieldname=v.getName();
  int length=v.getLength();
  int precision=v.getPrecision();
  if (add_fieldname) {
    retval+=fieldname + " ";
  }
  int type=v.getType();
switch (type) {
case ValueMetaInterface.TYPE_DATE:
    retval+="DATETIME NULL";
  break;
case ValueMetaInterface.TYPE_BOOLEAN:
if (supportsBooleanDataType()) {
  retval+="BOOLEAN";
}
 else {
  retval+="CHAR(1)";
}
break;
case ValueMetaInterface.TYPE_NUMBER:
case ValueMetaInterface.TYPE_INTEGER:
case ValueMetaInterface.TYPE_BIGNUMBER:
if (fieldname.equalsIgnoreCase(tk) || fieldname.equalsIgnoreCase(pk)) {
if (use_autoinc) {
retval+="INTEGER IDENTITY NOT NULL";
}
 else {
retval+="INTEGER NOT NULL PRIMARY KEY";
}
}
 else {
if (precision != 0 || (precision == 0 && length > 9)) {
if (precision > 0 && length > 0) {
  retval+="DECIMAL(" + length + ", "+ precision+ ") NULL";
}
 else {
  retval+="DOUBLE PRECISION NULL";
}
}
 else {
if (length < 3) {
  retval+="TINYINT NULL";
}
 else if (length < 5) {
  retval+="SMALLINT NULL";
}
 else {
  retval+="INTEGER NULL";
}
}
}
break;
case ValueMetaInterface.TYPE_STRING:
if (length >= 2048) {
retval+="TEXT NULL";
}
 else {
retval+="VARCHAR";
if (length > 0) {
retval+="(" + length + ")";
}
retval+=" NULL";
}
break;
default :
retval+=" UNKNOWN";
break;
}
if (add_cr) {
retval+=Const.CR;
}
return retval;
}

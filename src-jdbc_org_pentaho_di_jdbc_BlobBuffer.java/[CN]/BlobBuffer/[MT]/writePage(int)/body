{
  page=page & PAGE_MASK;
  if (page > raFile.length()) {
    throw new IOException("writePage: Invalid page number " + page);
  }
  if (buffer.length != PAGE_SIZE) {
    throw new IllegalStateException("writePage: buffer size invalid");
  }
  raFile.seek(page);
  raFile.write(buffer);
  bufferDirty=false;
}

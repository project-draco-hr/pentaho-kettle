{
  String retval="";
  retval+=XMLHandler.addTagValue("send_true_to",getSendTrueStepname());
  retval+=XMLHandler.addTagValue("send_false_to",getSendFalseStepname());
  retval+="    <compare>" + Const.CR;
  if (condition != null) {
    retval+=condition.getXML();
  }
  retval+="    </compare>" + Const.CR;
  return retval;
}

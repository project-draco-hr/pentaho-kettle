{
  if (dbname == null) {
    cache=new Hashtable();
    setActive();
  }
 else {
    Enumeration keys=cache.keys();
    while (keys.hasMoreElements()) {
      DBCacheEntry entry=(DBCacheEntry)keys.nextElement();
      if (entry.dbname.equalsIgnoreCase(dbname)) {
        cache.remove(entry);
      }
    }
  }
}

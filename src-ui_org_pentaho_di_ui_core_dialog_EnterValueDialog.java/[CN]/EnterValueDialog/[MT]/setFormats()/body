{
  wFormat.removeAll();
  int valtype=ValueMeta.getType(wValueType.getText());
switch (valtype) {
case ValueMetaInterface.TYPE_NUMBER:
    for (int i=0; i < Const.getNumberFormats().length; i++)     wFormat.add(Const.getNumberFormats()[i]);
  break;
case ValueMetaInterface.TYPE_DATE:
for (int i=0; i < Const.getDateFormats().length; i++) wFormat.add(Const.getDateFormats()[i]);
break;
case ValueMetaInterface.TYPE_STRING:
case ValueMetaInterface.TYPE_BOOLEAN:
case ValueMetaInterface.TYPE_INTEGER:
default :
break;
}
}

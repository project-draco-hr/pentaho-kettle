{
  if (o == null || !(o instanceof Job)) {
    return null;
  }
  JobEvent jobEvent=new JobEvent(JobEvent.EventType.FINISHED).build((Job)o);
  getLog().logDebug("JobFinishMonitor - " + ((Job)o).getName() + " , completed in "+ (jobEvent.getStartTimeMillis() > 0 ? (((jobEvent.getEndTimeMillis() - jobEvent.getStartTimeMillis()) / 1000) % 60) : 0)+ " seconds");
  return jobEvent;
}

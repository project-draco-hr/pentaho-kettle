{
  try {
    long id_database=db == null ? -1 : db.getID();
    setID(rep.insertDependency(id_transformation,id_database,tablename,fieldname));
  }
 catch (  KettleException dbe) {
    throw new KettleException(Messages.getString("TransDependency.Exception.UnableToSaveTransformationDepency") + id_transformation,dbe);
  }
}

{
  for (int i=0; i < fs.length; i++) {
    Field field=fs[i];
    if (!ctx.canIgnoreField(field)) {
      try {
        Object ref=field.get(obj);
        if (field.getType().isPrimitive() || ctx.canTreatAsPrimitive(ref))         continue;
        String name=field.getName();
        Object fabs=ctx.fieldAttributeMap.get(field);
        if (fabs == null)         fabs=ctx.fieldAttributeMap.get(name);
        out.println(dotName(obj) + " -> " + dotName(ref)+ "[label=\""+ name+ "\",fontsize=12"+ (fabs == null ? "" : "," + fabs)+ "];");
        generateDotInternal(ctx,ref,out,visited);
      }
 catch (      IllegalAccessException e) {
      }
    }
  }
}

{
  if (trustManagers == null)   if (CustomdSocketFactory.certStorePwd != null) {
    trustManagers=(new Truster[]{new Truster(CustomdSocketFactory.certStorePath,CustomdSocketFactory.certStorePwd)});
  }
 else {
    trustManagers=(new Truster[]{new Truster(CustomdSocketFactory.certStorePath)});
  }
  return trustManagers;
}

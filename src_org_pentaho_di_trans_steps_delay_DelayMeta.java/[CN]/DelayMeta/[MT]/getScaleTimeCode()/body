{
  int retval=1;
  if (scaletime == null)   return retval;
  if (scaletime.equals(ScaleTimeCode[0]))   retval=0;
 else   if (scaletime.equals(ScaleTimeCode[1]))   retval=1;
 else   if (scaletime.equals(ScaleTimeCode[2]))   retval=2;
 else   if (scaletime.equals(ScaleTimeCode[3]))   retval=3;
  return retval;
}

{
  if (this.groupFields == null) {
    for (int i=0; i < preSortedField.length; i++) {
      if (preSortedField[i] == true) {
        if (groupFields == null) {
          groupFields=new ArrayList<String>();
        }
        groupFields.add(this.fieldName[i]);
      }
    }
  }
  return groupFields;
}

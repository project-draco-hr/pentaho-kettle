{
  Value values[]={new Value("field1","KETTLE"),new Value("field2",123L),new Value("field3",10.5D),new Value("field4",new Date()),new Value("field5",true),new Value("field6",new BigDecimal(123.45))};
  Row r1=new Row();
  for (int i=0; i < values.length; i++) {
    r1.addValue(values[i]);
  }
  for (int i=0; i < values.length; i++) {
    Value v=r1.getValue(i);
    assertTrue(v.equals(values[i]));
  }
  try {
    Value v=r1.getValue(values.length + 1);
    fail("expected out of bounds exception");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}

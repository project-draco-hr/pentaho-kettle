{
  Row r0=new Row();
  Value values[]={new Value("field1","KETTLE"),new Value("field2",123L),new Value("field3",10.5D),new Value("field4",true),new Value("field5",new BigDecimal(123.0)),new Value("field6",(String)null),null};
  Row r1=new Row();
  for (int i=0; i < values.length; i++) {
    r1.addValue(values[i]);
  }
  assertEquals("[]",r0.toStringMeta());
  assertEquals("[field1(String), field2(Integer), field3(Number), field4(Boolean), field5(BigNumber), field6(String), NULL]",r1.toStringMeta());
}

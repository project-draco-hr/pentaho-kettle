{
  Value values1[]={new Value("field1","KETTLE"),new Value("field2",123L),new Value("field3",10.5D),new Value("field4",true),new Value("field5",new BigDecimal(123.0)),new Value("field6",(String)null),new Value("field7",new Date(10000000L))};
  Row r1=new Row();
  for (int i=0; i < values1.length; i++) {
    r1.addValue(values1[i]);
  }
  DataOutputStream store=new DataOutputStream(new ByteArrayOutputStream());
  r1.writeData(store);
}

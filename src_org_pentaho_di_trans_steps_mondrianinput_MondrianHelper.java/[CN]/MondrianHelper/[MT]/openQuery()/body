{
  String connectString="Provider=mondrian;" + "Jdbc='" + databaseMeta.getURL() + "';"+ "Catalog='"+ catalog+ "';"+ "JdbcDrivers="+ databaseMeta.getDriverClass()+ ";";
  if (!Const.isEmpty(databaseMeta.getUsername())) {
    connectString+="JdbcUser=" + databaseMeta.getUsername() + ";";
  }
  if (!Const.isEmpty(databaseMeta.getPassword())) {
    connectString+="JdbcPasssword=" + databaseMeta.getPassword() + ";";
  }
  connection=DriverManager.getConnection(connectString,null,true);
  query=connection.parseQuery(queryString);
  result=connection.execute(query);
}

{
  if (!Const.isEmpty(resultfieldName)) {
    ValueMetaInterface v=null;
    if (checksumtype.equals(TYPE_CRC32) || checksumtype.equals(TYPE_ADLER32))     v=new ValueMeta(space.environmentSubstitute(resultfieldName),ValueMeta.TYPE_INTEGER);
 else     v=new ValueMeta(space.environmentSubstitute(resultfieldName),ValueMeta.TYPE_STRING);
    v.setOrigin(name);
    inputRowMeta.addValueMeta(v);
  }
}

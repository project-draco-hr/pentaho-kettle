{
  try {
    if (data.fis != null) {
      data.fis.close();
      data.fis=null;
    }
    if (data.dis != null) {
      data.dis.close();
      data.dis=null;
    }
  }
 catch (  IOException e) {
    throw new KettleFileException(Messages.getString("GroupBy.Exception.UnableToCloseInputStream"),e);
  }
}

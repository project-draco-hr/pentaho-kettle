{
  if (valueType == ValueMetaInterface.TYPE_INTEGER) {
    if (Utils.isEmpty(conversionMask)) {
      conversionMask="0";
    }
    if (Utils.isEmpty(decimalSymbol)) {
      decimalSymbol=".";
    }
    if (Utils.isEmpty(groupingSymbol)) {
      groupingSymbol=",";
    }
  }
  if (valueType == ValueMetaInterface.TYPE_NUMBER) {
    if (Utils.isEmpty(conversionMask)) {
      conversionMask="0.0";
    }
    if (Utils.isEmpty(decimalSymbol)) {
      decimalSymbol=".";
    }
    if (Utils.isEmpty(groupingSymbol)) {
      groupingSymbol=",";
    }
  }
}

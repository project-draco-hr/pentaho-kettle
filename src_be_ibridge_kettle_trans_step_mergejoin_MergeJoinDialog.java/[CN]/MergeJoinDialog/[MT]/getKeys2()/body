{
  try {
    StepMeta stepMeta=transMeta.findStep(input.getStepName2());
    if (stepMeta != null) {
      Row prev=transMeta.getStepFields(stepMeta);
      if (prev != null) {
        for (int i=0; i < prev.size(); i++) {
          TableItem item=new TableItem(wKeys2.table,SWT.NONE);
          item.setText(1,prev.getValue(i).getName());
        }
        wKeys2.removeEmptyRows();
        wKeys2.setRowNums();
        wKeys2.optWidth(true);
      }
    }
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("MergeJoinDialog.ErrorGettingFields.DialogTitle"),Messages.getString("MergeJoinDialog.ErrorGettingFields.DialogMessage"),e);
  }
}

{
  methodType=getMethodType(method);
switch (methodType) {
case PARTITIONING_METHOD_SPECIAL:
{
      partitioner=StepLoader.getPartitioner(method).getInstance();
      break;
    }
case PARTITIONING_METHOD_NONE:
default :
  partitioner=null;
}
if (partitioner != null) {
partitioner.setMeta(this);
}
}

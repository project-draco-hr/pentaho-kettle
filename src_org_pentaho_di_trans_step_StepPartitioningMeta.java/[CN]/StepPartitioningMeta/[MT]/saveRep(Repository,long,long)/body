{
  rep.saveStepAttribute(id_transformation,id_step,"PARTITIONING_SCHEMA",partitionSchema != null ? partitionSchema.getName() : "");
  rep.saveStepAttribute(id_transformation,id_step,"PARTITIONING_METHOD",getMethodCode());
  if (partitioner != null) {
    partitioner.saveRep(rep,id_transformation,id_step);
  }
}

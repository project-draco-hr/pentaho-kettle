  wAutoinc.setLayoutData(fdAutoinc);
  wAutoinc.setToolTipText(Messages.getString("CombinationLookupDialog.AutoincButton.Tooltip",Const.CR));
  wlTk=new Label(shell,SWT.RIGHT);

    public void widgetSelected(    SelectionEvent e){
      enableFields();
    }
  }
);
  wlSeq=new Label(shell,SWT.RIGHT);
  wlSeq.setText(Messages.getString("CombinationLookupDialog.Sequence.Label"));
  props.setLook(wlSeq);
  fdlSeq=new FormData();

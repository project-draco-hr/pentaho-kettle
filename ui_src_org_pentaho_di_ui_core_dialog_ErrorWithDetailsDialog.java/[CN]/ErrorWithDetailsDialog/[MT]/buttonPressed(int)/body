{
  super.buttonPressed(buttonId);
  if (buttonId == detailsIndex) {
    DetailsDialog detailsDialog=new DetailsDialog(getParentShell(),BaseMessages.getString(PKG,"ErrorDialog.ShowDetail.Title"),null,BaseMessages.getString(PKG,"ErrorDialog.ShowDetail.Message"),0,new String[]{BaseMessages.getString(PKG,"ErrorDialog.ShowDetail.Close")},0,details);
    detailsDialog.open();
  }
}

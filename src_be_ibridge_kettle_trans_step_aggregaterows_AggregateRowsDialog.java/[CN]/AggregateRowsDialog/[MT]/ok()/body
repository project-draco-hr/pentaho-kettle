{
  int i;
  int nrfields=wFields.nrNonEmpty();
  stepname=wStepname.getText();
  input.allocate(nrfields);
  for (i=0; i < nrfields; i++) {
    TableItem item=wFields.getNonEmpty(i);
    input.getFieldName()[i]=item.getText(1);
    input.getFieldNewName()[i]=item.getText(2);
    if (input.getFieldNewName()[i] == null || input.getFieldNewName()[i].length() == 0)     input.getFieldNewName()[i]=input.getFieldName()[i];
    input.getAggregateType()[i]=AggregateRowsMeta.getType(item.getText(3));
  }
  dispose();
}

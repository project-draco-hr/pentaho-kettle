{
  TimedTransRunner timedTransRunner=new TimedTransRunner("test/org/pentaho/di/run/calculator/Calculate_A_plus_B_D_over_E_addDays_Booleans.ktr",LogWriter.LOG_LEVEL_ERROR,AllRunTests.getOldTargetDatabase(),AllRunTests.getNewTargetDatabase(),rowCount * 10);
  assertTrue(timedTransRunner.runOldAndNew());
  be.ibridge.kettle.core.Result oldResult=timedTransRunner.getOldResult();
  assertTrue(oldResult.getNrErrors() == 0);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 0);
}

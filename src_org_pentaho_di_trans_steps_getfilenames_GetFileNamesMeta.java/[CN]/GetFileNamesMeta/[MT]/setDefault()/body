{
  int nrfiles=0;
  filterfiletype="all_files";
  isaddresult=true;
  filefield=false;
  includeRowNumber=false;
  rowNumberField="";
  dynamicFilenameField="";
  allocate(nrfiles);
  for (int i=0; i < nrfiles; i++) {
    fileName[i]="filename" + (i + 1);
    fileMask[i]="";
    fileRequired[i]=NO;
  }
}

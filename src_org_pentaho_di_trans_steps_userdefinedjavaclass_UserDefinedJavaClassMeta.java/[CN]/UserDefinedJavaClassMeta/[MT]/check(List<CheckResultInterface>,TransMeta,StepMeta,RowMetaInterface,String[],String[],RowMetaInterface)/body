{
  CheckResult cr;
  if (input.length > 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("UserDefinedJavaClassMeta.CheckResult.ConnectedStepOK2"),stepinfo);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,Messages.getString("UserDefinedJavaClassMeta.CheckResult.NoInputReceived"),stepinfo);
    remarks.add(cr);
  }
}

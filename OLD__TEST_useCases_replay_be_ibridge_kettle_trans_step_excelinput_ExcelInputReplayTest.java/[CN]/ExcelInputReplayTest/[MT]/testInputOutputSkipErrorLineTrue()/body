{
  directory="test/useCases/replay/excelInputOutputSkipErrorLineTrue/";
  expectFiles(directory,2);
  meta=new TransMeta(directory + "transform.ktr");
  trans=new Trans(log,meta);
  boolean ok=trans.execute(null);
  assertTrue(ok);
  trans.waitUntilFinished();
  trans.endProcessing("end");
  assertEquals(0,trans.getErrors());
  expectFiles(directory,4);
  expectContent(directory + "input.xls_Sheet1." + getDateFormatted()+ ".line","3" + Const.CR + "6"+ Const.CR);
  expectContent(directory + "result.out","name;age" + Const.CR + "john; 23"+ Const.CR+ "ward; 15"+ Const.CR+ "john; 24"+ Const.CR+ "ward; 16"+ Const.CR+ "john; 25"+ Const.CR);
}

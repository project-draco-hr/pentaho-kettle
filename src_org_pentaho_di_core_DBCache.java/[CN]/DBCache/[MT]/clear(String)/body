{
  if (dbname == null) {
    cache=new Hashtable<DBCacheEntry,RowMetaInterface>();
    setActive();
  }
 else {
    Enumeration<DBCacheEntry> keys=cache.keys();
    while (keys.hasMoreElements()) {
      DBCacheEntry entry=(DBCacheEntry)keys.nextElement();
      if (entry.sameDB(dbname)) {
        cache.remove(entry);
      }
    }
  }
}

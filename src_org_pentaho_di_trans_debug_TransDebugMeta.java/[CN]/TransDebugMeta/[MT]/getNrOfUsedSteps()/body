{
  int nr=0;
  for (  StepDebugMeta stepDebugMeta : stepDebugMetaMap.values()) {
    if (stepDebugMeta.isReadingFirstRows() && stepDebugMeta.getRowCount() > 0) {
      nr++;
    }
 else     if (stepDebugMeta.isReadingFirstRows() && stepDebugMeta.getRowCount() > 0 && stepDebugMeta.getCondition() != null && !stepDebugMeta.getCondition().isEmpty()) {
      nr++;
    }
  }
  return nr;
}

{
  TimedTransRunner timedTransRunner=new TimedTransRunner("experimental_test/org/pentaho/di/run/abort/AbortAboveLimit.ktr",LogWriter.LOG_LEVEL_ERROR,1);
  timedTransRunner.runOldAndNew();
  Result oldResult=timedTransRunner.getOldResult();
  assertTrue(oldResult.getNrErrors() == 1);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 1);
}

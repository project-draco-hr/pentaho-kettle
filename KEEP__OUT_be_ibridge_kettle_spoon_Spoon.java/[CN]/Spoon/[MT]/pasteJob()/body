{
  String xml=fromClipboard();
  try {
    Document doc=XMLHandler.loadXMLString(xml);
    JobMeta jobMeta=new JobMeta(log,XMLHandler.getSubNode(doc,JobMeta.XML_TAG),rep);
    addChefGraph(jobMeta);
    refreshGraph();
    refreshTree();
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("Spoon.Dialog.ErrorPastingJob.Title"),Messages.getString("Spoon.Dialog.ErrorPastingJob.Message"),e);
  }
}

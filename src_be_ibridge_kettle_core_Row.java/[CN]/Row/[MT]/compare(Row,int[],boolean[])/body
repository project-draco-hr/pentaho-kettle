{
  int retval=0;
  int i;
  int len=fieldnrs.length;
  Value v1, v2;
  for (i=0; i < len; i++) {
    v1=getValue(fieldnrs[i]);
    v2=r.getValue(fieldnrs[i]);
    retval=v1.compare(v2);
    if (!ascending[i]) {
      retval=retval * (-1);
    }
    if (retval != 0)     return retval;
  }
  return retval;
}

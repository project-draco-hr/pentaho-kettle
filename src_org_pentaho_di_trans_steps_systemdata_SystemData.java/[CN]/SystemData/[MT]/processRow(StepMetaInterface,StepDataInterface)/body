{
  Object[] row;
  if (data.readsRows) {
    row=getRow();
    if (row == null) {
      setOutputDone();
      return false;
    }
    if (first) {
      first=false;
      data.outputRowMeta=(RowMetaInterface)getInputRowMeta().clone();
      meta.getFields(data.outputRowMeta,getStepname(),null);
    }
  }
 else {
    row=new Object[]{};
    linesRead++;
    if (first) {
      first=false;
      data.outputRowMeta=new RowMeta();
      meta.getFields(data.outputRowMeta,getStepname(),null);
    }
  }
  RowMetaInterface imeta=getInputRowMeta();
  if (imeta == null)   this.setInputRowMeta(imeta=new RowMeta());
  row=getSystemData(imeta,row);
  if (log.isRowLevel())   logRowlevel("System info returned: " + row);
  putRow(data.outputRowMeta,row);
  if (!data.readsRows) {
    setOutputDone();
    return false;
  }
  return true;
}

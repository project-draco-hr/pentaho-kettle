}
row[index]=stop;
break;
case SystemDataMeta.TYPE_SYSTEM_INFO_PREVIOUS_RESULT_NR_ERRORS:
previousResult=getTransMeta().getPreviousResult();
value=0;
if (previousResult != null) {
value=previousResult.getNrErrors();
}
row[index]=value;
break;
default :
break;
}
}
return row;
}

{
  if (input.length > 0) {
    CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("FilesToResultMeta.CheckResult.StepReceivingInfoFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
 else {
    CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,Messages.getString("FilesToResultMeta.CheckResult.NoInputReceivedError"),stepinfo);
    remarks.add(cr);
  }
}

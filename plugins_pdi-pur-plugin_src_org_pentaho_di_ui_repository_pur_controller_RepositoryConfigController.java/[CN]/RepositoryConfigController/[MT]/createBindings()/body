{
  repositoryConfigDialog=(XulDialog)document.getElementById("repository-config-dialog");
  url=(XulTextbox)document.getElementById("repository-url");
  name=(XulTextbox)document.getElementById("repository-name");
  id=(XulTextbox)document.getElementById("repository-id");
  modificationComments=(XulCheckbox)document.getElementById("repository-modification-comments");
  okButton=(XulButton)document.getElementById("repository-config-dialog_accept");
  bf.setBindingType(Type.BI_DIRECTIONAL);
  bf.createBinding(model,"url",url,"value");
  bf.createBinding(model,"name",name,"value");
  bf.createBinding(model,"id",id,"value");
  bf.createBinding(model,"modificationComments",modificationComments,"checked");
  bf.setBindingType(Type.ONE_WAY);
  bf.createBinding(model,"valid",okButton,"!disabled");
}

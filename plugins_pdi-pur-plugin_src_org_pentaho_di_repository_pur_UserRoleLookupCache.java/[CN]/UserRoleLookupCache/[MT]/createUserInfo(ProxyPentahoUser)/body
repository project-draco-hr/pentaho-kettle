{
  IUser userInfo=null;
  try {
    userInfo=this.rsm.constructUser();
    userInfo.setDescription(user.getDescription());
    userInfo.setLogin(user.getName());
    userInfo.setName(user.getName());
    userInfo.setPassword(user.getPassword());
  }
 catch (  KettleException e) {
    e.printStackTrace();
  }
  return userInfo;
}

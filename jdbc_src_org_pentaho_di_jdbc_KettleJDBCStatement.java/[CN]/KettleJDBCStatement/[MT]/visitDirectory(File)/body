{
  File[] files=f.listFiles();
  if (files != null) {
    for (int i=0; i < files.length; i++) {
      try {
        if (files[i].getName().lastIndexOf("ktr") != -1) {
          addMetadata(files[i]);
        }
      }
 catch (      KettleException e) {
        e.printStackTrace();
        return false;
      }
    }
  }
  return true;
}

{
  ValidatorMeta retval=(ValidatorMeta)super.clone();
  if (validatorField != null) {
    retval.allocate(validatorField.length);
    for (int i=0; i < validatorField.length; i++)     retval.getValidatorField()[i]=validatorField[i].clone();
  }
 else {
    retval.allocate(0);
  }
  return retval;
}

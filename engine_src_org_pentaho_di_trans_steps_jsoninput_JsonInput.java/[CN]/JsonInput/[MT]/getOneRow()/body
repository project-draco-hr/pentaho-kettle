{
  if (!meta.isInFields()) {
    while ((data.recordnr >= data.nrrecords || data.file == null)) {
      if (!openNextFile()) {
        return null;
      }
    }
  }
 else {
    while ((data.recordnr >= data.nrrecords || data.readrow == null)) {
      if (!ReadNextString()) {
        return null;
      }
      if (data.readrow == null) {
        return null;
      }
    }
  }
  return buildRow();
}

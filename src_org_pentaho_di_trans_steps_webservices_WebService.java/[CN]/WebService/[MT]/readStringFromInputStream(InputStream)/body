{
  StringBuffer response=new StringBuffer();
  try {
    int c=anXml.read();
    while (c >= 0) {
      response.append((char)c);
      c=anXml.read();
    }
    anXml.close();
    return response.toString();
  }
 catch (  Exception e) {
    throw new KettleStepException("Unable to read web service response data from input stream",e);
  }
}

{
  meta=(MailValidatorMeta)smi;
  data=(MailValidatorData)sdi;
  if (super.init(smi,sdi)) {
    if (Const.isEmpty(meta.getResultFieldName())) {
      log.logError(toString(),Messages.getString("MailValidator.Error.ResultFieldMissing"));
      return false;
    }
    return true;
  }
  return false;
}

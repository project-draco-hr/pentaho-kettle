{
synchronized (waitingForData) {
    if (isWaitingForData())     return false;
    for (    RowSet rowSet : inputRowSets) {
synchronized (rowSet) {
        if (rowSet.size() > 0) {
          return false;
        }
        if (!isPassingData() || !rowSet.isBlocking()) {
          return false;
        }
      }
    }
    return true;
  }
}

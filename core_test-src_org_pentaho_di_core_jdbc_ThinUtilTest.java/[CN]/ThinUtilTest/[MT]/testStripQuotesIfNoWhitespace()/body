{
  Map<String,String> testStrings=new ImmutableMap.Builder<String,String>().put("\"quotedNospace\"","quotedNospace").put("unquoted","unquoted").put("\"quoted space\"","\"quoted space\"").put("\"field\" \"fieldAlias\"","\"field\" \"fieldAlias\"").put("\"field with space\" \"fieldAlias\"","\"field with space\" \"fieldAlias\"").put("\"field\" AS \"field Alias\"","\"field\" AS \"field Alias\"").build();
  for (  String test : testStrings.keySet()) {
    for (    char quote : new char[]{'\'','"','`'}) {
      assertEquals(testStrings.get(test).replace('"',quote),ThinUtil.stripQuotesIfNoWhitespace(test.replace('"',quote),quote));
    }
  }
}

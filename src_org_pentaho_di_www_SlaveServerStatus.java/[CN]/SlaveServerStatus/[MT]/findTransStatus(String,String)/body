{
  for (int i=0; i < transStatusList.size(); i++) {
    SlaveServerTransStatus transStatus=(SlaveServerTransStatus)transStatusList.get(i);
    if (transStatus.getTransName().equalsIgnoreCase(transName) && (Const.isEmpty(id) || transStatus.getId().equals(id)))     return transStatus;
  }
  return null;
}

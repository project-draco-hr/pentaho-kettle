{
  try {
    RowMetaInterface r=transMeta.getPrevStepFields(stepname);
    if (r != null) {
      BaseStepDialog.getFieldsFromPrevious(r,wParam,1,new int[]{1},new int[]{2},-1,-1,null);
    }
  }
 catch (  KettleException ke) {
    new ErrorDialog(shell,Messages.getString("DatabaseJoinDialog.GetFieldsFailed.DialogTitle"),Messages.getString("DatabaseJoinDialog.GetFieldsFailed.DialogMessage"),ke);
  }
}

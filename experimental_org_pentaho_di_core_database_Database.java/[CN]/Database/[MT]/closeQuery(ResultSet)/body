{
  try {
    if (res != null)     res.close();
    if (sel_stmt != null) {
      sel_stmt.close();
      sel_stmt=null;
    }
    if (pstmt != null) {
      pstmt.close();
      pstmt=null;
    }
  }
 catch (  SQLException ex) {
    throw new KettleDatabaseException("Couldn't close query: resultset or prepared statements",ex);
  }
}

{
  if (rowMeta == null) {
    ResultSetMetaData rsmd=null;
    try {
      rsmd=rs.getMetaData();
    }
 catch (    SQLException e) {
      throw new KettleDatabaseException("Unable to retrieve metadata from resultset",e);
    }
    rowMeta=getRowInfo(rsmd);
  }
  return getRow(rs,null,rowMeta);
}

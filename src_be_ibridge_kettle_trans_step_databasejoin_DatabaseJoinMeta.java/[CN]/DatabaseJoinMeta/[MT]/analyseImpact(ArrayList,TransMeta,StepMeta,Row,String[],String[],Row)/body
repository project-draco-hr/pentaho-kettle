{
  Row out=getFields(null,stepinfo.getName(),info);
  if (out != null) {
    for (int i=0; i < out.size(); i++) {
      Value outvalue=out.getValue(i);
      DatabaseImpact di=new DatabaseImpact(DatabaseImpact.TYPE_IMPACT_READ,transMeta.getName(),stepinfo.getName(),databaseMeta.getDatabaseName(),"",outvalue.getName(),outvalue.getName(),stepinfo.getName(),sql,"read from one or more database tables via SQL statement");
      impact.add(di);
    }
  }
}

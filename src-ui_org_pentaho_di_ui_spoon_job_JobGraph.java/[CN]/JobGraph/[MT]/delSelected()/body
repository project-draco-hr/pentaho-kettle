{
  List<JobEntryCopy> copies=jobMeta.getSelectedEntries();
  int nrsels=copies.size();
  if (nrsels == 0)   return;
  List<String> stepList=new ArrayList<String>();
  for (int i=0; i < copies.size(); ++i) {
    stepList.add(copies.get(i).toString());
  }
  MessageBox mb=new DeleteMessageBox(shell,BaseMessages.getString(PKG,"Spoon.Dialog.DeletionConfirm.Message"),stepList);
  int answer=mb.open();
  if (answer == SWT.YES) {
    for (int i=0; i < copies.size(); i++) {
      spoon.deleteJobEntryCopies(jobMeta,copies.get(i));
    }
    spoon.refreshTree();
    spoon.refreshGraph();
  }
}

{
  JobEntryCopy[] copies=jobMeta.getSelectedEntries();
  int nrsels=copies.length;
  if (nrsels == 0)   return;
  List<String> stepList=new ArrayList<String>();
  for (int i=0; i < copies.length; ++i) {
    stepList.add(copies[i].toString());
  }
  MessageBox mb=new DeleteMessageBox(shell,Messages.getString("Chef.Dialog.DeletionConfirm.Message"),stepList);
  int answer=mb.open();
  if (answer == SWT.YES) {
    for (int i=0; i < copies.length; i++) {
      spoon.deleteJobEntryCopies(jobMeta,copies[i]);
    }
    spoon.refreshTree();
    spoon.refreshGraph();
  }
}

{
  if (job != null && job.isInitialized() && job.isFinished()) {
    for (    RefreshListener listener : refreshListeners)     listener.refreshNeeded();
    log.logMinimal(BaseMessages.getString(PKG,"JobLog.Log.JobHasEnded"));
    System.out.println("Running = " + job.isActive());
  }
  setControlStates();
}

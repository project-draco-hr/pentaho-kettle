{
  if (update)   checkUpdate(remarks,stepinfo,prev);
 else   checkLookup(remarks,stepinfo,prev);
  if (input.length > 0) {
    CheckResult cr=new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("DimensionLookupMeta.CheckResult.StepReceiveInfoOK"),stepinfo);
    remarks.add(cr);
  }
 else {
    CheckResult cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("DimensionLookupMeta.CheckResult.NoInputReceiveFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
}

{
  if (upd) {
    for (int i=0; i < type_desc.length; i++) {
      if (type_desc[i].equalsIgnoreCase(ty))       return i;
    }
    if ("Y".equalsIgnoreCase(ty))     return TYPE_UPDATE_DIM_PUNCHTHROUGH;
    return TYPE_UPDATE_DIM_INSERT;
  }
 else {
    int retval=Value.getType(ty);
    if (retval == Value.VALUE_TYPE_NONE)     retval=Value.VALUE_TYPE_STRING;
    return retval;
  }
}

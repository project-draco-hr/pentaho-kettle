{
  List<XulEventHandler> handlers=new ArrayList<XulEventHandler>();
  for (  SpoonPlugin p : plugins) {
    if (p.getEventHandlers() == null) {
      continue;
    }
    if (p.getEventHandlers().containsKey(category)) {
      handlers.add(p.getEventHandlers().get(category));
    }
  }
  return handlers;
}

{
  List<XulOverlay> overlays=new ArrayList<XulOverlay>();
  for (  SpoonPluginInterface p : plugins) {
    if (p.getOverlays() == null) {
      continue;
    }
    if (p.getOverlays().containsKey(category)) {
      for (      XulOverlay o : p.getOverlays().get(category)) {
        overlays.add(o);
      }
    }
  }
  return overlays;
}

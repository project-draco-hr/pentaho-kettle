{
  int q=0;
  boolean quote_opened=false;
  boolean dquote_opened=false;
  for (int x=0; x < sql.length(); x++) {
    char c=sql.charAt(x);
switch (c) {
case '\'':
      quote_opened=!quote_opened;
    break;
case '"':
  dquote_opened=!dquote_opened;
break;
case '?':
if (!quote_opened && !dquote_opened) q++;
break;
default :
break;
}
}
return q;
}

{
  if (shortFilenameField != null) {
    ValueMetaInterface v=new ValueMetaString(space.environmentSubstitute(shortFilenameField));
    v.setLength(100,-1);
    v.setOrigin(name);
    r.addValueMeta(v);
  }
  if (extensionField != null) {
    ValueMetaInterface v=new ValueMetaString(space.environmentSubstitute(extensionField));
    v.setLength(100,-1);
    v.setOrigin(name);
    r.addValueMeta(v);
  }
  if (pathField != null) {
    ValueMetaInterface v=new ValueMetaString(space.environmentSubstitute(pathField));
    v.setLength(100,-1);
    v.setOrigin(name);
    r.addValueMeta(v);
  }
  if (sizeField != null) {
    ValueMetaInterface v=new ValueMetaInteger(space.environmentSubstitute(sizeField));
    v.setOrigin(name);
    v.setLength(9);
    r.addValueMeta(v);
  }
  if (hiddenField != null) {
    ValueMetaInterface v=new ValueMetaBoolean(space.environmentSubstitute(hiddenField));
    v.setOrigin(name);
    r.addValueMeta(v);
  }
  if (lastModificationField != null) {
    ValueMetaInterface v=new ValueMetaDate(space.environmentSubstitute(lastModificationField));
    v.setOrigin(name);
    r.addValueMeta(v);
  }
  if (uriField != null) {
    ValueMetaInterface v=new ValueMetaString(space.environmentSubstitute(uriField));
    v.setLength(100,-1);
    v.setOrigin(name);
    r.addValueMeta(v);
  }
  if (rootUriField != null) {
    ValueMetaInterface v=new ValueMetaString(space.environmentSubstitute(rootUriField));
    v.setLength(100,-1);
    v.setOrigin(name);
    r.addValueMeta(v);
  }
}

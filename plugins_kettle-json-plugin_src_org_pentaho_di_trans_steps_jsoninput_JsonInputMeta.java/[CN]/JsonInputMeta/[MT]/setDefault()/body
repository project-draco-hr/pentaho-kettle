{
  additionalFileOutputFields=new AdditionalFileOutputFields();
  isIgnoreEmptyFile=false;
  ignoreMissingPath=false;
  doNotFailIfNoFile=true;
  includeFilename=false;
  filenameField="";
  includeRowNumber=false;

{
  try {
    long id=rep.getClusterID(clusterName);
    if (id > 0) {
      rep.delClusterSchema(id);
    }
    refreshTree();
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,Messages.getString("RepositoryExplorerDialog.Cluster.Delete.UnexpectedError.Title"),Messages.getString("RepositoryExplorerDialog.Cluster.Delete.UnexpectedError.Message"),e);
  }
}

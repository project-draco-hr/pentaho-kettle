{
  TreeItem items[]=wTree.getSelection();
  boolean error=false;
  for (int i=0; i < items.length; i++) {
    final RepositoryObject repositoryObject=objectMap.get(ConstUI.getTreePath(items[i],0));
    if (repositoryObject != null) {
      try {
        rep.undeleteObject(new RepositoryElementLocation(repositoryObject.getName(),repositoryObject.getRepositoryDirectory(),repositoryObject.getObjectType()));
      }
 catch (      Exception e) {
        new ErrorDialog(shell,BaseMessages.getString(PKG,"RepositoryExplorerDialog.Trans.Delete.ErrorRestoring.Title"),BaseMessages.getString(PKG,"RepositoryExplorerDialog.Trans.Delete.ErrorRestoring.Message"),e);
        error=true;
      }
    }
  }
  refreshTree();
  return !error;
}

{
  try {
    rep.commit();
  }
 catch (  KettleException e) {
    new ErrorDialog(shell,BaseMessages.getString(PKG,"RepositoryExplorerDialog.PopupMenu.Dialog.ErrorCommitingChanges.Title"),BaseMessages.getString(PKG,"RepositoryExplorerDialog.PopupMenu.Dialog.ErrorCommitingChanges.Message"),e);
  }
  props.setScreen(new WindowProperty(shell));
  shell.dispose();
}

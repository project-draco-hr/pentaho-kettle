{
  try {
    dateFormat.parse(dateStr);
  }
 catch (  ParseException ex) {
    if (log4j.isDebugEnabled()) {
      if (log4j.isDebugEnabled()) {
        log4j.debug(BaseMessages.getString(PKG,"MVSFileParser.DEBUG.Date.Parse.Error"));
      }
    }
    if ((alternateFormatString != null)) {
      if (log4j.isDebugEnabled()) {
        if (log4j.isDebugEnabled()) {
          log4j.debug(BaseMessages.getString(PKG,"MVSFileParser.DEBUG.Date.Parse.Choose.Alt"));
        }
      }
      dateFormatString=alternateFormatString;
      dateFormat=new SimpleDateFormat(dateFormatString);
      alternateFormatString=null;
      try {
        dateFormat.parse(dateStr);
      }
 catch (      ParseException ex2) {
        return false;
      }
    }
 else {
      log4j.error(BaseMessages.getString(PKG,"MVSFileParser.ERROR.Date.Parse.Fail",dateStr));
      return false;
    }
  }
  return true;
}

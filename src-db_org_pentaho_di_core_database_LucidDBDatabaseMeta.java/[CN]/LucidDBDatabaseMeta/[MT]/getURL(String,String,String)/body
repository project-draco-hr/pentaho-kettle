{
  if (getAccessType() == DatabaseMeta.TYPE_ACCESS_ODBC) {
    return "jdbc:odbc:" + databaseName;
  }
 else {
    if (!Const.isEmpty(port) && Const.toInt(port,-1) > 0) {
      return "jdbc:luciddb:rmi://" + hostname + ":"+ port;
    }
 else {
      return "jdbc:luciddb:rmi://" + hostname;
    }
  }
}

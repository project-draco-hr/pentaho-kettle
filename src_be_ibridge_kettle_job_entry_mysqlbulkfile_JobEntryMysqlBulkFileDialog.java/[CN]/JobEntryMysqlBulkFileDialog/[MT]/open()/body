      if (cid.open() != null) {
        jobMeta.addDatabase(databaseMeta);
        wConnection.add(databaseMeta.getName());
        wConnection.select(wConnection.getItemCount() - 1);
      }
    }
  }
);
  fdbConnection=new FormData();

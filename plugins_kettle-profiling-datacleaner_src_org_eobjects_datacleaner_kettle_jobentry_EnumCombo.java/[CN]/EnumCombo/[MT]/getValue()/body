{
  final String text=_combo.getText();
  E[] constants=_enumClass.getEnumConstants();
  for (  E item : constants) {
    if (text.equals(getStringValue(item))) {
      return item;
    }
  }
  if (_nullable) {
    return null;
  }
 else {
    throw new IllegalStateException("No value found that matches string: " + text);
  }
}

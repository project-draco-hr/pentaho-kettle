{
  IRole roleInfo=null;
  try {
    roleInfo=rsm.constructRole();
  }
 catch (  KettleException e) {
    e.printStackTrace();
  }
  roleInfo.setDescription(description);
  roleInfo.setName(name);
  for (  UIRepositoryUser user : getAssignedUsers()) {
    roleInfo.addUser(user.getUserInfo());
  }
  return roleInfo;
}

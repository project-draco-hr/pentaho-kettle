{
  String args[]=new String[10];
  for (int i=0; i < row.size(); i++) {
    ValueMetaInterface valueMeta=row.getValueMeta(i);
    int argNr=getArgumentNumber(valueMeta);
    if (argNr >= 0 && argNr < 10) {
      try {
        args[argNr]=row.getString(i,"");
      }
 catch (      KettleValueException e) {
        args[argNr]="";
      }
    }
  }
  return args;
}

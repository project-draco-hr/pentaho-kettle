{
  try {
    if (!acceptingFilenames) {
      FileObject fileObject=KettleVFS.getFileObject(space.environmentSubstitute(dbfFileName),space);
      if (fileObject.exists()) {
        dbfFileName=resourceNamingInterface.nameResource(fileObject.getName().getBaseName(),fileObject.getParent().getName().getPath(),space.toString(),FileNamingType.DATA_FILE);
        return dbfFileName;
      }
    }
    return null;
  }
 catch (  Exception e) {
    throw new KettleException(e);
  }
}

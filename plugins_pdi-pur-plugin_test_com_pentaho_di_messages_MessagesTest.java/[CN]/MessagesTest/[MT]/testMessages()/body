{
  assertEquals("Wrong message returned","test message 1",Messages.getInstance().getString("test.MESSAGE1"));
  assertEquals("Wrong message returned","test message 2: A",Messages.getInstance().getString("test.MESSAGE2","A"));
  assertEquals("Wrong message returned","test message 3: A B",Messages.getInstance().getString("test.MESSAGE3","A","B"));
  assertEquals("Wrong message returned","test message 4: A B C",Messages.getInstance().getString("test.MESSAGE4","A","B","C"));
  assertEquals("Wrong message returned","test message 5: A B C D",Messages.getInstance().getString("test.MESSAGE5","A","B","C","D"));
}

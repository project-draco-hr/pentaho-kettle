  colinf[1].setToolTip(BaseMessages.getString(PKG,"RssInputDialog.Field.Column.Tooltip"));
  wFields=new TableView(transMeta,wFieldsComp,SWT.FULL_SELECTION | SWT.MULTI,colinf,FieldsRows,lsMod,props);

{
  TimedTransRunner timedTransRunner=new TimedTransRunner("test/org/pentaho/di/trans/steps/mapping/filereader/use filereader.ktr",LogWriter.LOG_LEVEL_ERROR,AllRunTests.getOldTargetDatabase(),AllRunTests.getNewTargetDatabase(),1000);
  timedTransRunner.init();
  timedTransRunner.runNewEngine(true);
  Result newResult=timedTransRunner.getNewResult();
  assertTrue(newResult.getNrErrors() == 0);
}

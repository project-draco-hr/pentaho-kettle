{
  if (upd) {
    for (int i=0; i < typeDesc.length; i++) {
      if (typeDesc[i].equalsIgnoreCase(ty))       return i;
    }
    if ("Y".equalsIgnoreCase(ty))     return TYPE_UPDATE_DIM_PUNCHTHROUGH;
    return TYPE_UPDATE_DIM_INSERT;
  }
 else {
    int retval=ValueMeta.getType(ty);
    if (retval == ValueMetaInterface.TYPE_NONE)     retval=ValueMetaInterface.TYPE_STRING;
    return retval;
  }
}

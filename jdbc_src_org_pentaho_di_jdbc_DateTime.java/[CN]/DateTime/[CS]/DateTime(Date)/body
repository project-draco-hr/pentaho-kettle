{
  dateValue=d;
  GregorianCalendar cal=(GregorianCalendar)calendar.get();
  cal.setTime(d);
  this.year=(short)cal.get(Calendar.YEAR);
  this.month=(short)(cal.get(Calendar.MONTH) + 1);
  this.day=(short)cal.get(Calendar.DAY_OF_MONTH);
  this.hour=0;
  this.minute=0;
  this.second=0;
  this.millis=0;
  packDate();
  this.time=TIME_NOT_USED;
  unpacked=true;
}

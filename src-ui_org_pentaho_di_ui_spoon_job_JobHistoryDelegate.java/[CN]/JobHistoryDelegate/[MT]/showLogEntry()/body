{
  if (rowList == null) {
    wText.setText("");
    return;
  }
  int nr=wFields.table.getSelectionIndex();
  if (nr >= 0 && rowList != null && nr < rowList.size()) {
    RowMetaAndData row=rowList.get(nr);
    String logging=null;
    try {
      int logFieldIndex=row.getRowMeta().indexOfValue("LOG_FIELD");
      logging=logFieldIndex < 0 ? "-" : row.getString(logFieldIndex,"");
    }
 catch (    KettleValueException e) {
      new ErrorDialog(jobGraph.getShell(),Messages.getString("JobHistory.Error.GettingLoggingInfo"),Messages.getString("JobHistory.Error.GettingLogFieldFromLoggingTable"),e);
    }
    if (logging != null) {
      wText.setText(logging);
    }
 else {
      wText.setText("");
    }
  }
}

{
  BatchFolderIterator bfi=new BatchFolderIterator(folder,2,1,5);
  Assert.assertTrue(bfi.hasNext());
  bfi.next();
  Assert.assertTrue(bfi.hasNext());
  bfi.next();
  Assert.assertTrue(bfi.hasNext());
  bfi.next();
  Assert.assertTrue(bfi.hasNext());
  bfi.next();
  Assert.assertTrue(bfi.hasNext());
  bfi.next();
  Assert.assertFalse(bfi.hasNext());
}

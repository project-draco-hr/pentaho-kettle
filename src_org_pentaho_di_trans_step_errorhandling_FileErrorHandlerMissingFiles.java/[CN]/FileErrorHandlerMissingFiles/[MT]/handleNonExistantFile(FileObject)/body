{
  handleFile(file);
  try {
    getWriter(NO_PARTS).write(THIS_FILE_DOES_NOT_EXIST);
    getWriter(NO_PARTS).write(Const.CR);
  }
 catch (  Exception e) {
    throw new KettleException(Messages.getString("FileErrorHandlerMissingFiles.Exception.CouldNotCreateNonExistantFile") + file.getName().getURI(),e);
  }
}

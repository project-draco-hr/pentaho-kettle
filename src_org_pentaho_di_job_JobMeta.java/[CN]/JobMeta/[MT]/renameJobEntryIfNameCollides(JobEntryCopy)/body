{
  String newname=je.getName();
  boolean found;
  int nr=1;
  do {
    found=false;
    for (    JobEntryCopy copy : jobcopies) {
      if (copy != je && copy.getName().equalsIgnoreCase(newname) && copy.getNr() == 0)       found=true;
    }
    if (found)     nr++;
    newname=je.getName() + " (" + nr+ ")";
  }
 while (found);
  je.setName(newname);
}

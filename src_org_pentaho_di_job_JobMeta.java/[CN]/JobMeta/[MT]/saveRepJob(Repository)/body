{
  try {
    rep.insertJob(getID(),directory.getID(),getName(),logConnection == null ? -1 : logConnection.getID(),logTable,modifiedUser,modifiedDate,useBatchId,batchIdPassed,logfieldUsed,sharedObjectsFile,description,extendedDescription,jobVersion,jobStatus,created_user,created_date);
  }
 catch (  KettleDatabaseException dbe) {
    throw new KettleException(Messages.getString("JobMeta.Exception.UnableToSaveJobToRepository"),dbe);
  }
}

{
  int count=0;
  for (int i=0; i < nrJobEntries(); i++) {
    JobEntryCopy je=getJobEntry(i);
    if (je.getName().equalsIgnoreCase(name))     count++;
  }
  JobEntryCopy retval[]=new JobEntryCopy[count];
  count=0;
  for (int i=0; i < nrJobEntries(); i++) {
    JobEntryCopy je=getJobEntry(i);
    if (je.getName().equalsIgnoreCase(name)) {
      retval[count]=je;
      count++;
    }
  }
  return retval;
}

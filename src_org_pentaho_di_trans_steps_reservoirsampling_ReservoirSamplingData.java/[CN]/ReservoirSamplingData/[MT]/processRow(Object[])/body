{
  if (m_currentRow < m_k) {
    setElement(m_sample,m_currentRow,row);
  }
 else   if (m_k > 0) {
    double r=m_random.nextDouble();
    if (r < ((double)m_k / (double)m_currentRow)) {
      r=m_random.nextDouble();
      int replace=(int)((double)m_k * r);
      setElement(m_sample,replace,row);
    }
  }
  m_currentRow++;
}

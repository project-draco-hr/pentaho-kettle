{
  ValueMetaInterface v=new ValueMeta(technicalKeyField,ValueMetaInterface.TYPE_INTEGER);
  v.setLength(10);
  v.setPrecision(0);
  v.setOrigin(origin);
  row.addValueMeta(v);
  if (replaceFields) {
    for (int i=0; i < keyField.length; i++) {
      int idx=row.indexOfValue(keyField[i]);
      if (idx >= 0) {
        row.removeValueMeta(idx);
      }
    }
  }
}

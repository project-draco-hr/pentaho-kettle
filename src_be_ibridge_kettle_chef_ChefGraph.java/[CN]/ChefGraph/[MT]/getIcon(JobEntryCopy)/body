{
  Image im=null;
  if (je == null)   return null;
switch (je.getType()) {
case JobEntryInterface.TYPE_JOBENTRY_SPECIAL:
    if (je.isStart())     im=GUIResource.getInstance().getImageStart();
  if (je.isDummy())   im=GUIResource.getInstance().getImageDummy();
break;
default :
im=images[je.getType()];
}
return im;
}

{
  String exactFilename=StringUtil.environmentSubstitute(entry.getFilename());
  String exactJobname=StringUtil.environmentSubstitute(entry.getJobName());
  if (Const.isEmpty(exactFilename) && !Const.isEmpty(exactJobname)) {
    try {
      JobMeta newJobMeta=new JobMeta(log,spoon.rep,exactJobname,entry.getDirectory());
      newJobMeta.clearChanged();
      spoon.addChefGraph(jobMeta);
    }
 catch (    Throwable e) {
      MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_ERROR);
      mb.setMessage(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJob.Message") + Const.CR + e.getMessage());
      mb.setText(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJob.Title"));
      mb.open();
    }
  }
 else {
    try {
      File file=new File(exactFilename);
      JobMeta newJobMeta;
      if (file.exists()) {
        newJobMeta=new JobMeta(log,exactFilename,spoon.rep);
      }
 else {
        newJobMeta=new JobMeta(log);
      }
      newJobMeta.setFilename(exactFilename);
      newJobMeta.clearChanged();
      spoon.addChefGraph(jobMeta);
    }
 catch (    Throwable e) {
      MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_ERROR);
      mb.setMessage(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJobFromXML.Message") + Const.CR + e.getMessage());
      mb.setText(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJobFromXML.Title"));
      mb.open();
    }
  }
}

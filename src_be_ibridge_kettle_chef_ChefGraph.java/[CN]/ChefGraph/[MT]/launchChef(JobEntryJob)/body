{
  if (Const.isEmpty(entry.getFileName()) && !Const.isEmpty(entry.getName())) {
    try {
      Chef ch=new Chef(log,chef.disp,chef.rep);
      ch.jobMeta=new JobMeta(log,ch.rep,StringUtil.environmentSubstitute(entry.getJobName()),entry.getDirectory());
      ch.jobMeta.clearChanged();
      ch.open();
    }
 catch (    Throwable e) {
      MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_ERROR);
      mb.setMessage(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJob.Message") + Const.CR + e.getMessage());
      mb.setText(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJob.Title"));
      mb.open();
    }
  }
 else {
    try {
      Chef ch=new Chef(log,chef.disp,null);
      ch.jobMeta=new JobMeta(log,StringUtil.environmentSubstitute(entry.getFileName()),chef.rep);
      ch.jobMeta.setFilename(StringUtil.environmentSubstitute(entry.getFileName()));
      ch.jobMeta.clearChanged();
      ch.refreshTree();
      ch.refreshGraph();
      ch.open();
    }
 catch (    Throwable e) {
      MessageBox mb=new MessageBox(shell,SWT.OK | SWT.ICON_ERROR);
      mb.setMessage(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJobFromXML.Message") + Const.CR + e.getMessage());
      mb.setText(Messages.getString("ChefGraph.Dialog.ErrorLaunchingChefCanNotLoadJobFromXML.Title"));
      mb.open();
    }
  }
}

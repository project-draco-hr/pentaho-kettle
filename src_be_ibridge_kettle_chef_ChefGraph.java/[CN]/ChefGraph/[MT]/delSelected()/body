{
  JobEntryCopy[] copies=jobMeta.getSelectedEntries();
  int nrsels=copies.length;
  if (nrsels == 0)   return;
  MessageBox mb=new MessageBox(shell,SWT.YES | SWT.NO | SWT.ICON_WARNING);
  mb.setText("Warning!");
  String message="Are you sure you want to delete " + nrsels + (nrsels == 1 ? " job entry" : " job entries")+ Const.CR;
  mb.setMessage(message);
  int answer=mb.open();
  if (answer == SWT.YES) {
    for (int i=0; i < copies.length; i++) {
      spoon.deleteChefGraphEntry(jobMeta,copies[i].getName());
    }
    spoon.refreshTree();
    spoon.refreshGraph();
  }
}

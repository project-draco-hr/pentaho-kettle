{
  Row row;
  if (r == null)   row=new Row();
 else   row=r;
  for (int i=0; i < outputFields.length; i++) {
    TextFileField field=outputFields[i];
    Value v=row.searchValue(field.getName());
    if (v != null) {
      v.setLength(field.getLength(),field.getPrecision());
    }
  }
  return row;
}

{
  StringBuffer url=new StringBuffer(meta.getUrl());
  for (int i=0; i < data.argnrs.length; i++) {
    if (i == 0) {
      url.append("?");
    }
 else {
      url.append("&");
    }
    url.append(meta.getArgumentParameter()[i]);
    url.append("=");
    Value v=new Value(row.getValue(data.argnrs[i])).replace(" ","%20");
    url.append(v.toString(false));
  }
  return url.toString();
}

{
  if (info != null) {
    for (int i=0; i < info.length; i++) {
      if (info[i] != null) {
        r.mergeRowMeta(info[i],name);
      }
    }
  }
  for (int i=0; i < r.size(); i++) {
    ValueMetaInterface vmi=r.getValueMeta(i);
    if (vmi != null && Const.isEmpty(vmi.getName())) {
      vmi.setOrigin(name);
    }
  }
  return;
}

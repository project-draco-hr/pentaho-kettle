{
  meta=(DimensionLookupMeta)smi;
  data=(DimensionLookupData)sdi;
  Row r=getRow();
  if (r == null) {
    setOutputDone();
    return false;
  }
  try {
    lookupValues(r);
    putRow(r);
    if ((linesRead > 0) && (linesRead % Const.ROWS_UPDATE) == 0)     logBasic(Messages.getString("DimensionLookup.Log.LineNumber") + linesRead);
  }
 catch (  KettleException e) {
    logError(Messages.getString("DimensionLookup.Log.StepCanNotContinueForErrors",e.getMessage()));
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
    setOutputDone();
    return false;
  }
  return true;
}

{
  String retval="";
  retval+="      <dependency>" + Const.CR;
  retval+="        " + XMLHandler.addTagValue("connection",db == null ? "" : db.getName());
  retval+="        " + XMLHandler.addTagValue("table",tablename);
  retval+="        " + XMLHandler.addTagValue("field",fieldname);
  retval+="        </dependency>" + Const.CR;
  return retval;
}

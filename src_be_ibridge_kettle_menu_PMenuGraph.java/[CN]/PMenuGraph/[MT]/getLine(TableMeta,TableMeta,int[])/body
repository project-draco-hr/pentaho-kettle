{
  Point from=fs.getLocation();
  Point to=ts.getLocation();
  offset=getOffset();
  Point A=new Point(from.x,from.y);
  Point B=new Point(to.x,to.y);
  Point X=new Point(0,0);
  Point Y=new Point(0,0);
  double angle=calcRelationshipLine(A,B,X,Y);
  line[0]=X.x;
  line[1]=X.y;
  line[2]=Y.x;
  line[3]=Y.y;
  return angle;
}

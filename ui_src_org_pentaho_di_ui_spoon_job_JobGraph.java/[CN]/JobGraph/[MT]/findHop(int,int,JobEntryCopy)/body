{
  int i;
  JobHopMeta online=null;
  for (i=0; i < jobMeta.nrJobHops(); i++) {
    JobHopMeta hi=jobMeta.getJobHop(i);
    JobEntryCopy fs=hi.getFromEntry();
    JobEntryCopy ts=hi.getToEntry();
    if (fs == null || ts == null)     return null;
    if (exclude != null && (exclude.equals(fs) || exclude.equals(ts)))     continue;
    int line[]=getLine(fs,ts);
    if (pointOnLine(x,y,line))     online=hi;
  }
  return online;
}

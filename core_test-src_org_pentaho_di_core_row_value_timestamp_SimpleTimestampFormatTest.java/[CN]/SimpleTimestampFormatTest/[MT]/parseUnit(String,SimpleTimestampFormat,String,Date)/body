{
  if (date instanceof Timestamp) {
    assertEquals(localeForErrorMSG + "=locale localized pattern= " + stf.toLocalizedPattern(),date,(stf.parse(tdb.getString(patternName))));
  }
 else {
    assertEquals(localeForErrorMSG + "=locale localized pattern= " + stf.toLocalizedPattern(),date,(stf.parse(tdb.getString(patternName))));
  }
}

{
  CharsetInfo charset;
  if (collation[4] != 0) {
    charset=getCharsetForSortOrder((int)collation[4] & 0xFF);
  }
 else {
    charset=getCharsetForLCID(((int)collation[2] & 0x0F) << 16 | ((int)collation[1] & 0xFF) << 8 | ((int)collation[0] & 0xFF));
  }
  if (charset == null) {
    throw new SQLException(BaseMessages.getString(PKG,"error.charset.nocollation",Support.toHex(collation)),"2C000");
  }
  return charset;
}

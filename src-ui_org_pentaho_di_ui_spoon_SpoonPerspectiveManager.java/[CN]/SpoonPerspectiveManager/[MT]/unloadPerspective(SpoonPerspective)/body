{
  per.setActive(false);
  List<XulOverlay> overlays=per.getOverlays();
  if (overlays != null) {
    for (    XulOverlay overlay : overlays) {
      try {
        domContainer.removeOverlay(overlay.getOverlayUri());
      }
 catch (      XulException e) {
        e.printStackTrace();
      }
    }
  }
}

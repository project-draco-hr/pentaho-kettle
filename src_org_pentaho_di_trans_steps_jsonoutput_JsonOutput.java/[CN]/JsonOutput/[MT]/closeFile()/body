{
  if (data.writer == null)   return true;
  boolean retval=false;
  try {
    data.writer.close();
    data.writer=null;
    retval=true;
  }
 catch (  Exception e) {
    logError(BaseMessages.getString(PKG,"JsonOutput.Error.ClosingFile",e.toString()));
    setErrors(1);
    retval=false;
  }
  return retval;
}

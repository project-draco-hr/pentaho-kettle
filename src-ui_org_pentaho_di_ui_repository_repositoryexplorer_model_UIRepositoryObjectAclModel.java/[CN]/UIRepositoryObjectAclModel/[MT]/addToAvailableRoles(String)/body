{
  for (  UIRepositoryRole role : masterAvailableRoleList) {
    if (role.getName().equals(name)) {
      availableRoleList.add(role);
      break;
    }
  }
  this.firePropertyChange("availableRoleList",null,availableRoleList);
  if (roleUnassignmentPossible && getSelectedRoleList().size() > 0) {
    this.firePropertyChange("roleUnassignmentPossible",null,true);
  }
 else {
    this.firePropertyChange("roleUnassignmentPossible",null,false);
  }
}

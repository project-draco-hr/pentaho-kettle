{
  for (  UIRepositoryRole role : masterAvailableRoleList) {
    if (role.getName().equals(name)) {
      availableRoleList.add(role);
      break;
    }
  }
  this.firePropertyChange("availableRoleList",null,availableRoleList);
  fireUnassignPropertyChangeEvent(roleUnassignmentPossible,getSelectedRoleList(),Type.ROLE);
}

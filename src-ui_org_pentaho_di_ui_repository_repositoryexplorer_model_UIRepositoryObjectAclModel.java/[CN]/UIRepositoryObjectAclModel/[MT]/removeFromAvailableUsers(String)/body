{
  for (  UIRepositoryUser user : availableUserList) {
    if (user.getName().equals(name)) {
      availableUserList.remove(user);
      break;
    }
  }
  this.firePropertyChange("availableUserList",null,availableUserList);
  fireAssignPropertyChangeEvent(userAssignmentPossible,availableUserList,Type.USER);
}

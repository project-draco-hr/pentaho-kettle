{
  for (  UIRepositoryUser user : availableUserList) {
    if (user.getName().equals(name)) {
      availableUserList.remove(user);
      break;
    }
  }
  this.firePropertyChange("availableUserList",null,availableUserList);
  if (userAssignmentPossible && availableUserList.size() > 0) {
    this.firePropertyChange("userAssignmentPossible",null,true);
  }
 else {
    this.firePropertyChange("userAssignmentPossible",null,false);
  }
}

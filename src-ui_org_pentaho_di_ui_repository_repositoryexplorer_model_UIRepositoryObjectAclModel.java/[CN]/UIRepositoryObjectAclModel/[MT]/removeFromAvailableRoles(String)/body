{
  for (  UIRepositoryRole role : availableRoleList) {
    if (role.getName().equals(name)) {
      availableRoleList.remove(role);
      break;
    }
  }
  this.firePropertyChange("availableRoleList",null,availableRoleList);
  fireAssignPropertyChangeEvent(roleAssignmentPossible,availableRoleList,Type.ROLE);
}

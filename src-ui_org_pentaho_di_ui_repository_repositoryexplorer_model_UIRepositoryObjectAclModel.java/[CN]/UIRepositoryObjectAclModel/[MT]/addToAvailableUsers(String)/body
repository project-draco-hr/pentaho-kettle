{
  for (  UIRepositoryUser user : masterAvailableUserList) {
    if (user.getName().equals(name)) {
      availableUserList.add(user);
      break;
    }
  }
  this.firePropertyChange("availableUserList",null,availableUserList);
  fireUnassignPropertyChangeEvent(userUnassignmentPossible,getSelectedUserList(),Type.USER);
}

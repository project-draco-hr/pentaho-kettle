{
  TreeItem[] ti=wTree.getSelection();
  if (ti.length == 1) {
    TreeItem parent=ti[0].getParentItem();
    if (parent != null) {
      String schemaName=parent.getText();
      String tableName=ti[0].getText();
      if (ti[0].getItemCount() == 0) {
        String tab=null;
        if (schemaName.equalsIgnoreCase(STRING_TABLES) || schemaName.equalsIgnoreCase(STRING_VIEWS) || schemaName.equalsIgnoreCase(STRING_SYNONYMS)|| (schemaName != null && schemaName.length() == 0)) {
          tab=tableName;
        }
 else {
          tab=dbMeta.getQuotedSchemaTableCombination(schemaName,tableName);
        }
        return tab;
      }
    }
  }
  return null;
}

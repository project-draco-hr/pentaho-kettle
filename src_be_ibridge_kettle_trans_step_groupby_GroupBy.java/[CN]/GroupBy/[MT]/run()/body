{
  try {
    logBasic("Starting to run...");
    while (processRow(meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError("Unexpected error in '" + debug + "' : "+ e.toString());
    setErrors(1);
    stopAll();
  }
 finally {
    if (data.tempFile != null)     data.tempFile.delete();
    dispose(meta,data);
    logSummary();
    markStop();
  }
}

{
  Set<IRole> roles=new HashSet<IRole>();
  for (  UserToRoleAssignment assignment : assignments) {
    if (name.equals(assignment.getUserId())) {
      IRole role=null;
      try {
        role=rsm.constructRole();
      }
 catch (      KettleException e) {
        e.printStackTrace();
      }
      role.setName(assignment.getRoleId());
      roles.add(role);
    }
  }
  return roles;
}

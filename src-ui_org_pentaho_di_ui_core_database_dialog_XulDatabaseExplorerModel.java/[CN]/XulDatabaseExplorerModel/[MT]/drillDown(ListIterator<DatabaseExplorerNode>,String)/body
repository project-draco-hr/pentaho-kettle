{
  DatabaseExplorerNode theNode=null;
  while (aNodes.hasNext()) {
    theNode=aNodes.next();
    if (theNode.getName().equals(aName)) {
      break;
    }
 else {
      theNode=drillDown(theNode.getChildren().listIterator(),aName);
      if (theNode != null) {
        break;
      }
    }
  }
  return theNode;
}

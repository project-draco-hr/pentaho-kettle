{
  try {
    while (processRow((StepMetaInterface)meta,data) && !isStopped())     ;
  }
 catch (  Exception e) {
    logError("Unexpected error in '" + debug + "' : "+ e.toString());
    logError(Const.getStackTracker(e));
    setErrors(1);
    stopAll();
  }
 finally {
    logSummary();
    markStop();
  }
}

{
  if (getID() < 0) {
    try {
      if (rep.getTransformationID(getName(),directory.getID()) > 0)       return true;
    }
 catch (    KettleDatabaseException dbe) {
      log.logError(toString(),Messages.getString("TransMeta.Log.DatabaseError") + dbe.getMessage());
      return true;
    }
  }
  return false;
}

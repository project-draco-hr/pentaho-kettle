{
  TreeItem sel=(TreeItem)e.item;
  TreeItem up1=sel.getParentItem();
  if (up1 != null) {
    TreeItem up2=up1.getParentItem();
    if (up2 != null) {
      TreeItem up3=up2.getParentItem();
      if (up3 != null) {
        tableName=sel.getText();
        if (!justLook)         handleOK();
 else         previewTable(tableName,false);
      }
    }
  }
}

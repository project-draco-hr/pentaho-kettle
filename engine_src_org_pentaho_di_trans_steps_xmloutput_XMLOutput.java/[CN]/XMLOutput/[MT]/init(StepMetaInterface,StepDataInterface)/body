{
  meta=(XMLOutputMeta)smi;
  data=(XMLOutputData)sdi;
  if (super.init(smi,sdi)) {
    data.splitnr=0;
    if (!meta.isDoNotOpenNewFileInit()) {
      if (openNewFile()) {
        data.OpenedNewFile=true;
        return true;
      }
 else {
        logError("Couldn't open file " + meta.getFileName());
        setErrors(1L);
        stopAll();
      }
    }
 else     return true;
  }
  return false;
}

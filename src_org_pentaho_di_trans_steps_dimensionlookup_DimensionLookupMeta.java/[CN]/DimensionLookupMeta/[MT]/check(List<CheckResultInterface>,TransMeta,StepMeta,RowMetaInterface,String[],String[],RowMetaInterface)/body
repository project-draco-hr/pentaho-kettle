{
  if (update)   checkUpdate(remarks,stepinfo,prev);
 else   checkLookup(remarks,stepinfo,prev);
  if (techKeyCreation != null) {
    if (!(CREATION_METHOD_AUTOINC.equals(techKeyCreation) || CREATION_METHOD_SEQUENCE.equals(techKeyCreation) || CREATION_METHOD_TABLEMAX.equals(techKeyCreation))) {
      String error_message=Messages.getString("DimensionLookupMeta.CheckResult.ErrorTechKeyCreation") + ": " + techKeyCreation+ "!";
      CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,error_message,stepinfo);
      remarks.add(cr);
    }
  }
  if (input.length > 0) {
    CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("DimensionLookupMeta.CheckResult.StepReceiveInfoOK"),stepinfo);
    remarks.add(cr);
  }
 else {
    CheckResult cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,Messages.getString("DimensionLookupMeta.CheckResult.NoInputReceiveFromOtherSteps"),stepinfo);
    remarks.add(cr);
  }
}

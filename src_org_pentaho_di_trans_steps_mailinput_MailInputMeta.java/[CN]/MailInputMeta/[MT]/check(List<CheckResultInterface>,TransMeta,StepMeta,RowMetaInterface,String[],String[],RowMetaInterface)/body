{
  CheckResult cr;
  if (input.length > 0) {
    cr=new CheckResult(CheckResult.TYPE_RESULT_ERROR,BaseMessages.getString(PKG,"MailInputMeta.CheckResult.NoInputExpected"),stepMeta);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResult.TYPE_RESULT_OK,BaseMessages.getString(PKG,"MailInputMeta.CheckResult.NoInput"),stepMeta);
    remarks.add(cr);
  }
}

{
  StringBuilder sbsql=new StringBuilder(sql.getServiceName());
  sbsql.append(" - ");
  if (isService) {
    sbsql.append("Service");
  }
 else {
    sbsql.append("SQL");
  }
  sbsql.append(" - ");
  sbsql.append(sql.getSqlString());
  for (int i=sbsql.length() - 1; i >= 0; i--) {
    if (sbsql.charAt(i) == '\n' || sbsql.charAt(i) == '\r') {
      sbsql.setCharAt(i,' ');
    }
  }
  return sbsql.toString();
}

{
  String localFilename=filename;
  targetFilename=returnTargetFilename(localFilename);
  if ((!onlyGettingNewFiles) || (onlyGettingNewFiles && needsDownload(targetFilename))) {
    if (isDetailed())     logDetailed(BaseMessages.getString(PKG,"JobEntryFTP.GettingFile",filename,environmentSubstitute(targetDirectory)));
    ftpclient.get(targetFilename,filename);
    updateRetrievedFiles();
    if (isDetailed())     logDetailed(BaseMessages.getString(PKG,"JobEntryFTP.GotFile",filename));
    addFilenameToResultFilenames(result,parentJob,targetFilename);
    if (remove) {
      ftpclient.delete(filename);
      if (isDetailed())       if (isDetailed())       logDetailed(BaseMessages.getString(PKG,"JobEntryFTP.DeletedFile",filename));
    }
 else {
      if (movefiles) {
        ftpclient.rename(filename,realMoveToFolder + FILE_SEPARATOR + filename);
        if (isDetailed())         logDetailed(BaseMessages.getString(PKG,"JobEntryFTP.MovedFile",filename,realMoveToFolder));
      }
    }
  }
}

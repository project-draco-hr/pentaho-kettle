{
  meta=(GetFileNamesMeta)smi;
  data=(GetFileNamesData)sdi;
  if (super.init(smi,sdi)) {
    try {
      data.files=meta.getTextFileList(getTransMeta());
      handleMissingFiles();
      return true;
    }
 catch (    Exception e) {
      logError("Error initializing step: " + e.toString());
      logError(Const.getStackTracker(e));
      return false;
    }
  }
  return false;
}

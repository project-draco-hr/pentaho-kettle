{
  boolean retval=false;
  int limitErrors=Const.toInt(environmentSubstitute(getNrErrorsLessThan()),10);
  if ((NrErrors > 0 && getSuccessCondition().equals("success_when_all_works_fine")) || (NrErrors >= limitErrors && !getSuccessCondition().equals("success_when_all_works_fine"))) {
    retval=true;
  }
  return retval;
}

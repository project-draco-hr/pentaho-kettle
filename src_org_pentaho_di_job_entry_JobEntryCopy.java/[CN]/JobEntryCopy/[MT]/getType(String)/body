{
  if (dsc != null) {
    JobEntryType[] types=JobEntryType.values();
    for (    JobEntryType type : types) {
      if (type.getTypeCode().equalsIgnoreCase(dsc))       return type;
    }
    for (    JobEntryType type : types) {
      if (type.getDescription().equalsIgnoreCase(dsc))       return type;
    }
  }
  return JobEntryType.NONE;
}

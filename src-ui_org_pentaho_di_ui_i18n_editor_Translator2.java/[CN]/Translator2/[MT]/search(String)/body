{
  EnterStringDialog dialog=new EnterStringDialog(shell,Const.NVL(searchString,""),Messages.getString("i18nDialog.SearchKey"),Messages.getString("i18nDialog.SearchLocale1") + " '" + Const.NVL(searchLocale,"")+ "' "+ Messages.getString("i18nDialog.SearchLocale2"));
  searchString=dialog.open();
  lastFoundKey=null;
  searchAgain(searchLocale);
}

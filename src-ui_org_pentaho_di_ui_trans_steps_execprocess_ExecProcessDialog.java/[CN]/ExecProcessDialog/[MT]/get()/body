{
  if (!gotPreviousFields) {
    try {
      String fieldvalue=wProcess.getText();
      wProcess.removeAll();
      RowMetaInterface r=transMeta.getPrevStepFields(stepname);
      if (r != null) {
        wProcess.setItems(r.getFieldNames());
      }
      if (fieldvalue != null)       wProcess.setText(fieldvalue);
      gotPreviousFields=true;
    }
 catch (    KettleException ke) {
      new ErrorDialog(shell,Messages.getString("ExecProcessDialog.FailedToGetFields.DialogTitle"),Messages.getString("ExecProcessDialog.FailedToGetFields.DialogMessage"),ke);
    }
  }
}

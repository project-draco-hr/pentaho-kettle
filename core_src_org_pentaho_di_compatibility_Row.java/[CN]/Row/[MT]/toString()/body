{
  StringBuffer retval=new StringBuffer(128);
  retval.append('[');
  for (int i=0; i < size(); i++) {
    Value value=getValue(i);
    if (i != 0) {
      retval.append(", ");
    }
    if (value != null) {
      retval.append(value.getName()).append('=').append(value.toString(false));
    }
 else {
      retval.append("NULL");
    }
  }
  retval.append(']');
  return retval.toString();
}

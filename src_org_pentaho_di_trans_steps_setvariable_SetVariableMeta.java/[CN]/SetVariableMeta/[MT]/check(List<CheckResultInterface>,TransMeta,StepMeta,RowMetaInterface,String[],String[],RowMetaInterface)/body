{
  CheckResult cr;
  if (prev == null || prev.size() == 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_WARNING,Messages.getString("SetVariableMeta.CheckResult.NotReceivingFieldsFromPreviousSteps"),stepMeta);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("SetVariableMeta.CheckResult.ReceivingFieldsFromPreviousSteps","" + prev.size()),stepMeta);
    remarks.add(cr);
  }
  if (input.length > 0) {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_OK,Messages.getString("SetVariableMeta.CheckResult.ReceivingInfoFromOtherSteps"),stepMeta);
    remarks.add(cr);
  }
 else {
    cr=new CheckResult(CheckResultInterface.TYPE_RESULT_ERROR,Messages.getString("SetVariableMeta.CheckResult.NotReceivingInfoFromOtherSteps"),stepMeta);
    remarks.add(cr);
  }
}

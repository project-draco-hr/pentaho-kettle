{
  String join="";
  if (complex_join != null) {
    join=complex_join;
  }
 else   if (table_from != null && table_to != null && fieldnr_from >= 0 && fieldnr_to >= 0) {
    TableField frf=getFieldFrom();
    TableField tof=getFieldTo();
    join=frf.getTableField() + " = " + tof.getTableField();
  }
  return join;
}

{
  boolean retval=false;
  try {
    SftpATTRS attrs=c.stat(foldername);
    if (attrs == null)     return false;
    if ((attrs.getFlags() & SftpATTRS.SSH_FILEXFER_ATTR_PERMISSIONS) == 0)     throw new KettleJobException("Unknown permissions error");
    retval=attrs.isDir();
  }
 catch (  Exception e) {
  }
  return retval;
}

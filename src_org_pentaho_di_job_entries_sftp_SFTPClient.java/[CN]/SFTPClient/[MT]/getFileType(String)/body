{
  try {
    SftpATTRS attrs=c.stat(filename);
    if (attrs == null)     return FileType.IMAGINARY;
    if ((attrs.getFlags() & SftpATTRS.SSH_FILEXFER_ATTR_PERMISSIONS) == 0)     throw new KettleJobException("Unknown permissions error");
    if (attrs.isDir())     return FileType.FOLDER;
 else     return FileType.FILE;
  }
 catch (  Exception e) {
    throw new KettleJobException(e);
  }
}

{
  if (tt == null) {
    return 0;
  }
  for (int i=0; i < ElementOldTypeCode.length; i++) {
    if (ElementOldTypeCode[i].equalsIgnoreCase(tt)) {
      return i;
    }
  }
  for (int i=0; i < ElementTypeCode.length; i++) {
    if (ElementTypeCode[i].equalsIgnoreCase(tt)) {
      return i;
    }
  }
  return 0;
}

{
  int i;
  NodeList children;
  Node childnode;
  if (n == null) {
    return null;
  }
  children=n.getChildNodes();
  for (i=0; i < children.getLength(); i++) {
    childnode=children.item(i);
    if (childnode.getNodeName().equalsIgnoreCase(tag)) {
      return childnode;
    }
  }
  return null;
}

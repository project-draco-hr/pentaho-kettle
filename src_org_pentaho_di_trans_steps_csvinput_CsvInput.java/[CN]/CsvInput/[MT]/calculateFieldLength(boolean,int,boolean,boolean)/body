{
  int length=data.endBuffer - data.startBuffer;
  if (newLineFound) {
    length-=newLines;
    if (length <= 0)     length=0;
    if (endOfBuffer)     data.startBuffer++;
  }
  if (enclosureFound) {
    data.startBuffer++;
    length-=2;
    if (length <= 0)     length=0;
  }
  if (length <= 0)   length=0;
  if (data.encodingType != EncodingType.SINGLE) {
    length--;
  }
  return length;
}

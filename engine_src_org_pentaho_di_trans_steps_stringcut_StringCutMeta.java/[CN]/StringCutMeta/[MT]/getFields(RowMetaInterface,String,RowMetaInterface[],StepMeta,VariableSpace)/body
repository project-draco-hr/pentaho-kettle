{
  for (int i=0; i < fieldOutStream.length; i++) {
    if (!Const.isEmpty(fieldOutStream[i])) {
      ValueMetaInterface v=new ValueMeta(space.environmentSubstitute(fieldOutStream[i]),ValueMeta.TYPE_STRING);
      v.setLength(100,-1);
      v.setOrigin(name);
      inputRowMeta.addValueMeta(v);
    }
  }
}

    }
  }
);
  wTabFolder.setSelection(0);
  getData();
  activeCopyFromPrevious();
  activeUseKey();
  AfterFTPPutActivate();
  BaseStepDialog.setSize(shell);
  shell.open();
  while (!shell.isDisposed()) {
    if (!display.readAndDispatch()) {
      display.sleep();
    }
  }
  return jobEntry;
}

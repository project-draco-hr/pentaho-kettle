{
  if (filename1 != null) {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("JobEntryFileCompare.CheckResult_Filename_1_Defined"),this));
  }
 else {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryFileCompare.CheckResult_Filename_1_Not_Defined"),this));
  }
  if (filename2 != null) {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("JobEntryFileCompare.CheckResult_Filename_2_Defined"),this));
  }
 else {
    remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryFileCompare.CheckResult_Filename_2_Not_Defined"),this));
  }
  if ((filename1 != null) && (filename2 != null)) {
    FileObject file1=null;
    FileObject file2=null;
    String realFilename1=getRealFilename1();
    String realFilename2=getRealFilename2();
    try {
      file1=KettleVFS.getFileObject(realFilename1);
      if (file1.exists()) {
        remarks.add(new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("JobEntryFileCompare.CheckResult_File_Exists",realFilename1),this));
      }
 else {
        remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryFileCompare.CheckResult_File_Does_Not_Exist",realFilename1),this));
      }
      try {
        file1.close();
      }
 catch (      IOException ignored) {
      }
    }
 catch (    IOException ex) {
      remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryFileCompare.ERROR_0008_File_IOError",realFilename1),this));
    }
    try {
      file2=KettleVFS.getFileObject(realFilename2);
      if (file2.exists()) {
        remarks.add(new CheckResult(CheckResult.TYPE_RESULT_OK,Messages.getString("JobEntryFileCompare.CheckResult_File_Exists",realFilename2),this));
      }
 else {
        remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryFileCompare.CheckResult_File_Does_Not_Exist",realFilename2),this));
      }
      try {
        file2.close();
      }
 catch (      IOException ignored) {
      }
    }
 catch (    IOException ex) {
      remarks.add(new CheckResult(CheckResult.TYPE_RESULT_ERROR,Messages.getString("JobEntryFileCompare.ERROR_0008_File_IOError",realFilename2),this));
    }
  }
}

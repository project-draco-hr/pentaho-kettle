{
  try {
    super.loadRep(rep,id_jobentry,databases);
    filename1=rep.getJobEntryAttributeString(id_jobentry,"filename1");
    filename2=rep.getJobEntryAttributeString(id_jobentry,"filename2");
  }
 catch (  KettleException dbe) {
    throw new KettleException(Messages.getString("JobEntryFileCompare.ERROR_0002_Unable_To_Load_Job_From_Repository",Long.toString(id_jobentry)),dbe);
  }
}

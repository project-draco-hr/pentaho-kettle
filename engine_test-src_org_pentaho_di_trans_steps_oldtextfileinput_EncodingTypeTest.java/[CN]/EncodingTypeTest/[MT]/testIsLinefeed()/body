{
  int lineFeed='\n';
  int carriageReturn='\r';
  assertFalse("SINGLE.isReturn is line feed",OldEncodingType.SINGLE.isReturn(lineFeed));
  assertFalse("DOUBLE_BIG_ENDIAN.isReturn is line feed",OldEncodingType.DOUBLE_BIG_ENDIAN.isReturn(lineFeed));
  assertFalse("DOUBLE_LITTLE_ENDIAN.isReturn is line feed",OldEncodingType.DOUBLE_LITTLE_ENDIAN.isReturn(lineFeed));
  assertTrue("SINGLE.isReturn is not carriage return",OldEncodingType.SINGLE.isReturn(carriageReturn));
  assertTrue("DOUBLE_BIG_ENDIAN.isReturn is not carriage return",OldEncodingType.DOUBLE_BIG_ENDIAN.isReturn(carriageReturn));
  assertTrue("DOUBLE_LITTLE_ENDIAN.isReturn is not carriage return",OldEncodingType.DOUBLE_LITTLE_ENDIAN.isReturn(carriageReturn));
}

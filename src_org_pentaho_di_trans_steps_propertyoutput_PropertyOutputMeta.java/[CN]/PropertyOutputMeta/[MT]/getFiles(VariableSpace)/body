{
  int copies=1;
  int parts=1;
  if (stepNrInFilename) {
    copies=3;
  }
  if (partNrInFilename) {
    parts=3;
  }
  int nr=copies * parts;
  if (nr > 1)   nr++;
  String retval[]=new String[nr];
  int i=0;
  for (int copy=0; copy < copies; copy++) {
    for (int part=0; part < parts; part++) {
      retval[i]=buildFilename(space,copy);
      i++;
    }
  }
  if (i < nr) {
    retval[i]="...";
  }
  return retval;
}

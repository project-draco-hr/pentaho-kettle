{
  index=new int[meta.getModelFields().length + 1];
  index[0]=-1;
  if (meta.getKeyLookups().size() == 1 && meta.getKeyLookups().get(0)[0].equals("id") && meta.getKeyLookups().get(0)[1].equals("="))   index[0]=getInputRowMeta().indexOfValue(meta.getKeyLookups().get(0)[2]);
  for (int i=0; i < meta.getModelFields().length; i++)   index[i + 1]=getInputRowMeta().indexOfValue(meta.getStreamFields()[i]);
  this.fieldList=data.helper.getFieldListForImport(meta.getModelName(),meta.getModelFields());
  this.fieldCache=data.helper.getModelFields(meta.getModelName());
}

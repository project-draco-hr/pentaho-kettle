{
  try {
    LDIFInputField retval=(LDIFInputField)super.clone();
    if (fieldPosition != null) {
      retval.setFieldPosition(new LDIFInputFieldPosition[fieldPosition.length]);
      for (int i=0; i < fieldPosition.length; i++) {
        retval.getFieldPosition()[i]=(LDIFInputFieldPosition)fieldPosition[i].clone();
      }
    }
    return retval;
  }
 catch (  CloneNotSupportedException e) {
    return null;
  }
}
